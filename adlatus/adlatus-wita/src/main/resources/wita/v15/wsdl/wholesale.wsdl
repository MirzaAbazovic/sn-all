    <wsdl:portType name="WholesalePort">
        <wsdl:operation name="annehmenAuftrag">
            <wsdl:documentation>Mit der Operation 'annehmenAufrag' übermittelt der Carrier einen Auftrag zur Bearbeitung
                an die Telekom
            </wsdl:documentation>
            <wsdl:input message="tns:annehmenAuftragRequest"/>
            <wsdl:output message="tns:annehmenAuftragResponse"/>
        </wsdl:operation>
        <wsdl:operation name="annehmenMeldung">
            <wsdl:documentation>Bei der Prozessierung eines Auftrages kann es notwendig werden, dass von einem Carrier
                Meldungen angefordert werden (z.B. Widerspruch bei Providerwechsel). Diese Meldungen werden vom Carrier
                an die Telekom über diese Operation übermitelt
            </wsdl:documentation>
            <wsdl:input message="tns:annehmenMeldungRequest"/>
            <wsdl:output message="tns:annehmenMeldungResponse"/>
        </wsdl:operation>
        <wsdl:operation name="annehmenAuftragNachVPO">
            <wsdl:documentation>Mit der Operation 'annehmenAuftragNachVPO' übermittelt der Carrier einen Auftrag nach
                einer erfolgreichen PreOrder zur Bearbeitung an die Telekom
            </wsdl:documentation>
            <wsdl:input message="tns:annehmenAuftragNachVPORequest"/>
            <wsdl:output message="tns:annehmenAuftragNachVPOResponse"/>
        </wsdl:operation>
        <wsdl:operation name="annehmenNotConnectedAuftrag">
            <wsdl:documentation>Mit der Operation 'annehmenNotConnectedAuftrag' übermittelt der Carrier einen
                NotConnected-Auftrag zur Bearbeitung an die Telekom
            </wsdl:documentation>
            <wsdl:input message="tns:annehmenNotConnectedAuftragRequest"/>
            <wsdl:output message="tns:annehmenNotConnectedAuftragResponse"/>
        </wsdl:operation>
        <wsdl:operation name="annehmenNgAuftrag">
            <wsdl:documentation>Mit der Operation 'annehmenNgAuftrag' übermittelt der Carrier einen Ng(FTTB)-Auftrag zur
                Bearbeitung an die Telekom
            </wsdl:documentation>
            <wsdl:input message="tns:annehmenNgAuftragRequest"/>
            <wsdl:output message="tns:annehmenNgAuftragResponse"/>
        </wsdl:operation>
    </wsdl:portType>
