    <wsdl:service name="WholesaleService">
        <wsdl:documentation>Der WholesaleSerice definiert den Serviceanteil der T-Com an der Orderschnittstelle.
        </wsdl:documentation>
        <wsdl:port name="wholesale" binding="tns:WholesaleBinding">
            <soap:address location="http://wholesale.telekom.de/oss/v15/wholesale"/>
        </wsdl:port>
    </wsdl:service>
