<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:jms="http://www.tibco.com/namespaces/ws/2004/soap/binding/JMS"
                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                  xmlns:tns="http://www.mnet.de/esb/cdm/SupplierPartner/LineOrderService/v2"
                  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
                  name="LineOrderService"
                  targetNamespace="http://www.mnet.de/esb/cdm/SupplierPartner/LineOrderService/v2">
   <wsdl:documentation>Version 2.0</wsdl:documentation>
   <wsdl:types>
      <xsd:schema xmlns="http://www.mnet.de/esb/cdm/SupplierPartner/LineOrderService/v2"
                  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                  targetNamespace="http://www.mnet.de/esb/cdm/SupplierPartner/LineOrderService/v2"
                  version="2.0"
                  elementFormDefault="qualified"
                  attributeFormDefault="unqualified">
         <xsd:simpleType name="AbfragestelleType">
            <xsd:annotation>
               <xsd:documentation>Nummer der Abfragestelle, maximal 6 Ziffern</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">Beispiel: 0</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="6"/>
               <xsd:pattern value="\d{1,6}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="AbgebenderProviderMitAbgabedatumType">
            <xsd:complexContent>
               <xsd:extension base="tns:AbgebenderProviderType">
                  <xsd:sequence>
                     <xsd:element name="abgabedatum" type="xsd:date" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Datum, an dem der Anschluss von Provider abgegeben wird</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AbgebenderProviderType">
            <xsd:sequence>
               <xsd:element name="providername" type="tns:ProvidernameType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Name des abgebenden Providers</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="zustimmungProviderwechsel" type="tns:BoolDecisionType">
                  <xsd:annotation>
                     <xsd:documentation>Kennzeichner, ob der abgebende Provider dem Wechsel zustimmt</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="antwortcode" type="tns:MeldungscodeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Kurzbezeichnung für die Ablehnung/Zustimmung zum Providerwechsel</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Nur zulässige Antwortcode laut Spezifikation Anbieterwechsel zu verwenden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="antworttext" type="tns:MeldungstextType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Begründung, warum der Wechsel nicht durchgeführt werden kann</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Antworttext ist für zugehörigen Code in der Spezifikation Anbieterwechsel geführt.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="AenderungskennzeichenType">
            <xsd:annotation>
               <xsd:documentation>spezifiziert einen Auftrag als Standard, Storno oder Terminverschiebung</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="30"/>
               <xsd:enumeration value="Standard">
                  <xsd:annotation>
                     <xsd:documentation>Kennzeichnung eines Standardauftrages</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Storno">
                  <xsd:annotation>
                     <xsd:documentation>Kennzeichnung der Stornierung des Auftrages mit den korrespondierenden Auftragsdaten</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Terminverschiebung">
                  <xsd:annotation>
                     <xsd:documentation>Kennzeichnung eines Auftrages zur Terminverschiebung des Auftrages mit den korrespondierenden Auftragsdaten</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="AktionscodeType">
            <xsd:annotation>
               <xsd:documentation>(Zugang, Wegfall)</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:length value="1"/>
               <xsd:enumeration value="A">
                  <xsd:annotation>
                     <xsd:documentation>Änderung</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="W">
                  <xsd:annotation>
                     <xsd:documentation>Wegfall</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Z">
                  <xsd:annotation>
                     <xsd:documentation>Zugang</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="AngabenZurLeitungABMType">
            <xsd:complexContent>
               <xsd:extension base="tns:AngabenZurLeitungType">
                  <xsd:sequence>
                     <xsd:element name="leitungsabschnitt"
                                  type="tns:LeitungsabschnittType"
                                  minOccurs="0"
                                  maxOccurs="99"/>
                     <xsd:element name="maxBruttoBitrate" type="tns:BitrateType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Pflichtangabe, wenn beauftragtes Übertragungsverfahren: H16</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AngabenZurLeitungType">
            <xsd:complexContent>
               <xsd:extension base="tns:BasisAngabenZurLeitungType">
                  <xsd:sequence>
                     <xsd:element name="schleifenwiderstand"
                                  type="tns:LeitungswiderstandType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Gesamt-Widerstand (Einheit: Ohm) der Schleife</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AnlageMitTypType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:AnlageType">
                  <xsd:sequence>
                     <xsd:element name="anlagentyp" type="tns:AnlagentypType">
                        <xsd:annotation>
                           <xsd:documentation>Gibt die Art der Anlage an</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AnlageType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="dateiname">
                  <xsd:annotation>
                     <xsd:documentation>der Dateiname zum File einschließlich der Erweiterung (Datei-Typ)</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="tns:ErlaubterZeichenBereichType">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="255"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="dateityp" type="tns:DokumenttypType">
                  <xsd:annotation>
                     <xsd:documentation>der Datentyp des Dokuments (Text, PDF, ...) im Mime-Type-Format</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="beschreibung" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Freitext zur Beschreibung des Inhaltes der Anlage</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="tns:ErlaubterZeichenBereichMehrzeiligType">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="160"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="inhalt" type="xsd:base64Binary">
                  <xsd:annotation>
                     <xsd:documentation>der eigentliche Inhalt der Datei (binärer Dateninhalt)</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">zulässige Dateigröße: max. 1 MB pro Anlage, max. 4 MB gesamt</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="AnlagentypType">
            <xsd:annotation>
               <xsd:documentation>Lageplan, Kündigungsschreiben, Kundenauftrag, ..., Sonstige</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="30"/>
               <xsd:enumeration value="Kuendigungsschreiben">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Lageplan">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Kundenauftrag">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Portierungsanzeige">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Kuendigung abgebender Provider">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="letzte Telekom-Rechnung">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Sonstige">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="AnredeFirmaType">
            <xsd:annotation>
               <xsd:documentation>die Anrede zur Firma (deutsch: Firma)</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="1"/>
               <xsd:enumeration value="4">
                  <xsd:annotation>
                     <xsd:documentation>Firma</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="9">
                  <xsd:annotation>
                     <xsd:documentation>keine Anrede</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="AnredePersonType">
            <xsd:annotation>
               <xsd:documentation>die Anrede zur Person (deutsch: Frau/Herr)</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="1"/>
               <xsd:enumeration value="1">
                  <xsd:annotation>
                     <xsd:documentation>Herr</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="2">
                  <xsd:annotation>
                     <xsd:documentation>Frau</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="9">
                  <xsd:annotation>
                     <xsd:documentation>keine Anrede</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="AnschlussType">
            <xsd:annotation>
               <xsd:documentation>Der Typ Anschluss ermöglicht die Identifikation eines Anschlusses</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="ONKZ" type="tns:OrtsnetzkennzahlType">
                  <xsd:annotation>
                     <xsd:documentation>Ortsnetzkennzahl des Anschlusses</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="rufnummer" type="tns:RufnummerType">
                  <xsd:annotation>
                     <xsd:documentation>Rufnummer des Anschlusses</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="AnschlussartType">
            <xsd:annotation>
               <xsd:documentation>Übertragungseinrichtung am ISIS/OPAL-Netz</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="10"/>
               <xsd:enumeration value="Analog"/>
               <xsd:enumeration value="ISDN-Basis"/>
               <xsd:enumeration value="PMX"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="AnschlussbereichType">
            <xsd:annotation>
               <xsd:documentation>Der Anschlussbereich wird mit maximal 4 Ziffern spezifiziert.</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="4"/>
               <xsd:pattern value="\d{1,4}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="AnsprechpartnerBaseType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:PersonType">
                  <xsd:sequence>
                     <xsd:element name="telefonnummer" type="tns:TelefonnummerType">
                        <xsd:annotation>
                           <xsd:documentation>Telefonnummer des Ansprechpartners</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="emailadresse" type="tns:EmailadresseType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>E-mail-Adresse des Ansprechpartners</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AnsprechpartnerMitEmailType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:PersonType">
                  <xsd:sequence>
                     <xsd:element name="telefonnummer" type="tns:TelefonnummerType">
                        <xsd:annotation>
                           <xsd:documentation>Telefonnummer des Ansprechpartners</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="emailadresse" type="tns:EmailadresseType">
                        <xsd:annotation>
                           <xsd:documentation>E-mail-Adresse des Ansprechpartners</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AnsprechpartnerMitRolleType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:AnsprechpartnerBaseType">
                  <xsd:sequence>
                     <xsd:element name="rolle" type="tns:AnsprechpartnerRolleType">
                        <xsd:annotation>
                           <xsd:documentation>Ansprechpartner den Kunden für vertriebliche, technische Nachfragen
							</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:simpleType name="AnsprechpartnerRolleType">
            <xsd:annotation>
               <xsd:documentation>Auftragsmanagement, Technik, ...</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="20"/>
               <xsd:enumeration value="Auftragsmanagement">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Technik">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="AnsprechpartnerType">
            <xsd:sequence>
               <xsd:element name="auftragsmanagement" type="tns:AnsprechpartnerMitEmailType">
                  <xsd:annotation>
                     <xsd:documentation>Angaben zu Ansprechpartnern des Kunden für das Auftragsmanagement</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ansprechpartner"
                            type="tns:AnsprechpartnerMitRolleType"
                            minOccurs="0"
                            maxOccurs="99">
                  <xsd:annotation>
                     <xsd:documentation>Angaben zu weiteren Ansprechpartnern des Kunden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="AufnehmenderProviderABMType">
            <xsd:complexContent>
               <xsd:extension base="tns:AufnehmenderProviderType">
                  <xsd:sequence>
                     <xsd:element name="uebernahmeDatumVerbindlich" type="xsd:date">
                        <xsd:annotation>
                           <xsd:documentation>Datum, zu dem Providerwechsel verbindlich durchgeführt wird</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Das Datum ist identisch dem VLT der ABM. Es kann vom Abgabedatum aus vertraglichen Gründen (z.B. regionaler Feiertag) oder betrieblichen Gründen abweichen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AufnehmenderProviderAKMType">
            <xsd:complexContent>
               <xsd:extension base="tns:AufnehmenderProviderType">
                  <xsd:sequence>
                     <xsd:element name="uebernahmeDatumGeplant" type="xsd:date">
                        <xsd:annotation>
                           <xsd:documentation>Datum, zu dem der Providerwechsel voraussichtlich durchgeführt wird</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Das Datum kann vom KWT des Auftrags des TNBauf abweichen wenn KWT vertraglich nicht zulässig ist</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="antwortfrist" type="xsd:date">
                        <xsd:annotation>
                           <xsd:documentation>Datum, zu dem die Antwort zur AKM-PV spätestens erwartet wird</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Die Antwortfrist endet zum angegebenen Datum um 24:00 Uhr. Anschließend wird automatisch der Folgeprozesschritt ausgeführt</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AufnehmenderProviderType" abstract="true">
            <xsd:sequence>
               <xsd:element name="providernameAufnehmend" type="tns:ProvidernameType">
                  <xsd:annotation>
                     <xsd:documentation>Name des aufnehmenden Providers</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="AuftragType">
            <xsd:sequence>
               <xsd:element name="externeAuftragsnummer" type="tns:ExterneAuftragsnummerType">
                  <xsd:annotation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Die Auftragsnummer kann wieder verwendet werden wenn Vorgängeraufträge mit gleicher Auftragsnummer abgeschlossen sind</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="kunde" type="tns:KundeType">
                  <xsd:annotation>
                     <xsd:documentation>Kundendaten (bei Beauftragung im Namen des Kunden ist der Kunde gemeint, für den bestellt wird)</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="besteller" type="tns:KundeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Angaben zum Besteller, für den Fall dass der Besteller im Namen eines anderen Kunden handelt (nur bei Beauftragung im Namen Dritter)</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element ref="tns:geschaeftsfall">
                  <xsd:annotation>
                     <xsd:documentation>Erweiterungspunkt für die konkreten Geschäftsfälle</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="kenner" type="tns:KennerType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>ID zur Gruppierung von Aufträgen</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="AuftragskennerType">
            <xsd:annotation>
               <xsd:documentation>setzt sich zusammen aus Auftragsklammer und Anzahl der Aufträge</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="auftragsklammer" type="tns:AuftragsklammerType">
                  <xsd:annotation>
                     <xsd:documentation>kennzeichnet zusammengehörige Aufträge</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Der Wert der Auftragsklammer darf nur einmalig verwendet werden. Auch bei einem zweiten Versand muss ein neuer Wert vergeben werden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="anzahlAuftraege">
                  <xsd:annotation>
                     <xsd:documentation>gibt die Anzahl der Aufträge an, die zu einer Auftragsklammer gehören</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:int">
                        <xsd:pattern value="\d{1,3}"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="AuftragsklammerType">
            <xsd:annotation>
               <xsd:documentation>kennzeichnet zusammengehörige Aufträge</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="10"/>
               <xsd:pattern value="\d{1,10}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="AuftragspositionBereitstellungType">
            <xsd:sequence>
               <xsd:element name="produkt" type="tns:ProduktType">
                  <xsd:annotation>
                     <xsd:documentation>die Beschreibung des Produktes der Auftragsposition (Erweiterungspunkt)</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="geschaeftsfallProdukt">
                  <xsd:annotation>
                     <xsd:documentation>die Geschäftsfall-spezifischen Angaben zum Produkt (Erweiterungspunkt)</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                     <xsd:choice>
                        <xsd:element name="TAL" type="tns:TALBereitstellungType"/>
                        <xsd:element name="XDSL" type="tns:XDSLBereitstellungType"/>
                     </xsd:choice>
                  </xsd:complexType>
               </xsd:element>
               <xsd:element name="position"
                            type="tns:AuftragspositionType"
                            minOccurs="0"
                            maxOccurs="99">
                  <xsd:annotation>
                     <xsd:documentation>die Unterpositionen zur Auftragsposition</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="AuftragspositionKuendigungType">
            <xsd:sequence>
               <xsd:element name="produkt" type="tns:ProduktType">
                  <xsd:annotation>
                     <xsd:documentation>die Beschreibung des Produktes der Auftragsposition (Erweiterungspunkt)</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="geschaeftsfallProdukt">
                  <xsd:annotation>
                     <xsd:documentation>die Geschäftsfall-spezifischen Angaben zum Produkt (Erweiterungspunkt)</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                     <xsd:choice>
                        <xsd:element name="TAL" type="tns:TALKuendigungType"/>
                        <xsd:element name="XDSL" type="tns:XDSLKuendigungType"/>
                     </xsd:choice>
                  </xsd:complexType>
               </xsd:element>
               <xsd:element name="position"
                            type="tns:AuftragspositionType"
                            minOccurs="0"
                            maxOccurs="99">
                  <xsd:annotation>
                     <xsd:documentation>die Unterpositionen zur Auftragsposition</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
