         <xsd:complexType name="AuftragstypType">
            <xsd:sequence>
               <xsd:element name="geschaeftsfall" type="tns:GeschaeftsfallEnumType"/>
               <xsd:element name="aenderungsKennzeichen" type="tns:AenderungskennzeichenType"/>
               <xsd:element name="geschaeftsfallart" type="tns:GeschaeftsfallArtType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="BasisAngabenZurLeitungType">
            <xsd:sequence>
               <xsd:element name="leitungsbezeichnung" type="tns:LeitungsbezeichnungType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="BereitstellungAnlagenType">
            <xsd:annotation>
               <xsd:documentation>die möglichen Anlagen für eine Bereitstellung (je Dokument max. 1 Mb, insgesamt max. 4 Mb)</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="lageplan" type="tns:AnlageType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>der Lageplan für eventuelle Montageleistungen</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Mögliche Beilage zur Unterstützung des Service. Kann nur genutzt werden wenn zusätlich im Montagehinweis auf die Anlage hingewiesen wird oder im Rahmen von Projekten mit Projektkenner.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="sonstige"
                            type="tns:AnlageMitTypType"
                            minOccurs="0"
                            maxOccurs="99">
                  <xsd:annotation>
                     <xsd:documentation>weitere Anlagen</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Neben Portierungsanzeige (notwendig im Anbieterwechsel wenn keine Haftungsfreistellungserklärung vorliegt) kann hier auch Lageplan und Kündigungsschreiben beigefügt werden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="BereitstellungProduktType" abstract="true">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basis für alle geschäftsfall-spezifischen Produkt-Daten (Erweiterungspunkt)</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallProduktType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="BereitstellungTermineType">
            <xsd:annotation>
               <xsd:documentation>die möglichen Termine für eine Bereitstellung</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="kundenwunschtermin" type="tns:KundenwunschterminType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="BereitstellungTerminverschiebungType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallTerminverschiebungType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="BereitstellungType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallType">
                  <xsd:sequence>
                     <xsd:element name="BKTOFaktura" type="tns:BuchungskontonummerType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Nummer, die zur Fakturierung heranzuziehen ist</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Wünscht Kunde dass ein Auftrag auf eine andere als die Standard BKTO abgerechnet wird, kann Kunde hier eine ihm zugeteilte aktive BKTO angeben</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Keine Angabe für die PG Preselection</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="anlagen" type="tns:BereitstellungAnlagenType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Anlagen zum Auftrag</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="ansprechpartner" type="tns:AnsprechpartnerType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Ansprechpartnern des Kunden</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="termine" type="tns:BereitstellungTermineType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Terminen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="teilausfuehrung" type="tns:BoolDecisionType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Auftrag kann auch durchgeführt werden, wenn nur ein Teil der bestellten Produkte lieferbar ist</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Angabe nur bei PG Preselection möglich, bei allen anderen PG wird die Angabe ignoriert. Bei fehlender Angabe wird der Wert 'N'(Nein) genommen.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="preOrderID" type="tns:UUIDType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>ID einer gültigen PreOrder, der Auftrag wird mit den Daten dieser PreOrder vervollständigt. Nur für Produktgruppen, für die eine vorgezogene PreOrder angeboten wird.</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Status der PreOrder: abgeschlossen oder in_Auftragspruefung; Eine PreOrder kann im Rahmen des Gültigkeitszeitraums wiederverwendet werden, wenn ein Vorgängerauftrag in der Auftragsprüfung abgelehnt worden ist. Nur für Produktgruppen, für die eine vorgezogene PreOrder angeboten wird.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="auftragsposition"
                                  type="tns:AuftragspositionBereitstellungType"
                                  maxOccurs="99">
                        <xsd:annotation>
                           <xsd:documentation>Liste der Auftragspositionen, in der die Produkte / Leistungsmerkmale aufgeführt sind, die im Auftrag bearbeitet werden sollen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="BestandsuebersichtType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallType">
                  <xsd:sequence>
                     <xsd:element name="BKTOFaktura" type="tns:BuchungskontonummerType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Nummer, die zur Fakturierung heranzuziehen ist</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Wünscht Kunde dass ein Auftrag auf eine andere als die Standard BKTO abgerechnet wird, kann Kunde hier eine ihm zugeteilte aktive BKTO angeben</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="BKTOBestand" type="tns:BuchungskontonummerType">
                        <xsd:annotation>
                           <xsd:documentation>Nummer, die zur Ermittlung des  Gesamtbestandes heranzuziehen ist</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Keine Angabe bei Online-Abfrage eines Einzelbestandes über Dialog</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="ansprechpartner" type="tns:AnsprechpartnerType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Ansprechpartnern des Kunden</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:simpleType name="BitrateType">
            <xsd:annotation>
               <xsd:documentation>Max. Bruttobitrate in Kbit/s</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">Beispiel: 16000</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="6"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="BoolDecisionType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:length value="1"/>
               <xsd:enumeration value="J"/>
               <xsd:enumeration value="N"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="BuchungskontonummerType">
            <xsd:annotation>
               <xsd:documentation>die Buchungskontonummer identifiziert den Debitor wird von der Telekom zur Fakturierung verwendet.</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="10"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="DokumenttypType">
            <xsd:annotation>
               <xsd:documentation>zulässige Dateiformate</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:enumeration value="image/tiff">
                  <xsd:annotation>
                     <xsd:documentation>Der MIME-Typ für *.tif - Dateien</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="image/jpeg">
                  <xsd:annotation>
                     <xsd:documentation>Der MIME-Typ für *.jpg - Dateien</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="application/msword">
                  <xsd:annotation>
                     <xsd:documentation>Der MIME-Typ für *.doc - Dateien</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="application/excel">
                  <xsd:annotation>
                     <xsd:documentation>Der MIME-Typ für *.xls - Dateien</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="application/pdf">
                  <xsd:annotation>
                     <xsd:documentation>Der MIME-Typ für *.pdf - Dateien</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="DoppeladerBelegtType">
            <xsd:sequence>
               <xsd:element name="leitungsbezeichnung"
                            type="tns:LeitungsbezeichnungType"
                            maxOccurs="2"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="DoppeladerType">
            <xsd:annotation>
               <xsd:documentation>Nummer der Doppelader</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Einstellige Angaben werden immer mit führender 0 ergänzt</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="2"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="DurchwahlnummerType">
            <xsd:annotation>
               <xsd:documentation>Durchwahlnummer des Anlageanschlüsses, maximal 8 Ziffern</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">Beispiel: 1234</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="8"/>
               <xsd:pattern value="\d{1,8}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="EmailadresseType">
            <xsd:annotation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
	Format: 'local-part'@'domain'. Nur zulässige E-Mail-Adressformate möglich.
	Beispiel: Heribert.Mustermann@telekom.de
			</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:maxLength value="320"/>
               <xsd:whiteSpace value="collapse"/>
               <xsd:pattern value=".+@(.+\.)+\w+"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="EndverschlussType">
            <xsd:annotation>
               <xsd:documentation>Nummer des Endverschlusses</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Einstellige Angaben werden immer mit führender 0 ergänzt</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="2"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="ErlaubterZeichenBereichMehrzeiligType">
            <xsd:annotation>
               <xsd:documentation>
				Einschränkung von zugelassenen UTF-8 Zeichen für die mehrzeiligen alphanumerischen Datenwerte. Folgende Zeichen sind erlaubt: Tabulator, LineFeed, CarriageReturn, Leerzeichen, 0-9, a-z, A-Z, !"#$%&amp;'()*+,-./:;&lt;=&gt;?@[\]^_{}§°±²³ÀÁÂÃÄÅÆÇÈÉÊËÌÍÎÏÐÑÒÓÔÕÖØÙÚÛÜÝÞßàáâãäåæçèéêëìíîïðñòóôõöøùúûüýþÿ			 				
			</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:pattern value="[&#x9;&#xA;&#xD; -_a-{}&#x7f;§°-³À-ÖØ-öø-ÿ]*"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="ErlaubterZeichenBereichType">
            <xsd:annotation>
               <xsd:documentation>
				Einschränkung von zugelassenen UTF-8 Zeichen für die alphanumerischen Datenwerte. Folgende Zeichen sind erlaubt: Tabulator, Leerzeichen, 0-9, a-z, A-Z, !"#$%&amp;'()*+,-./:;&lt;=&gt;?@[\]^_{}§°±²³ÀÁÂÃÄÅÆÇÈÉÊËÌÍÎÏÐÑÒÓÔÕÖØÙÚÛÜÝÞßàáâãäåæçèéêëìíîïðñòóôõöøùúûüýþÿ			 				
			</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:pattern value="[&#x9; -_a-{}&#x7f;§°-³À-ÖØ-öø-ÿ]*"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="ErweiterteBestandssucheType">
            <xsd:annotation>
               <xsd:documentation>Bestandssuche-Identifikation für Einzel- und Anlagenanschlüsse</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Erweiterte Bestandssuche wird ausschließlich bei GF/PG: NEU/Preselection und REX-MK/TAL verwendet</xsd:documentation>
            </xsd:annotation>
            <xsd:choice>
               <xsd:element name="anlagenanschluss" type="tns:OnkzDurchwahlAbfragestelleType">
                  <xsd:annotation>
                     <xsd:documentation>Bestandssuche ID für Durchwahlanlagen</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="einzelanschluss" type="tns:OnkzRufNrType">
                  <xsd:annotation>
                     <xsd:documentation>Bestandssuche ID für Einzelanschlüsse</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:choice>
         </xsd:complexType>
         <xsd:simpleType name="ExterneAuftragsnummerType">
            <xsd:annotation>
               <xsd:documentation>Eindeutiger Identifier des Auftrages, durch den Kunden zu vergeben</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="20"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="FirmaType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="anrede" type="tns:AnredeFirmaType">
                  <xsd:annotation>
                     <xsd:documentation>Anrede zur Firma</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="firmenname" type="tns:VornameType">
                  <xsd:annotation>
                     <xsd:documentation>Name der Firma</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="firmennameZweiterTeil" type="tns:NachnameType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Firmenname 2. Teil</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="GebaeudeteilNameType">
            <xsd:annotation>
               <xsd:documentation>Gebäudeteil</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="25"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="GebaeudeteilType">
            <xsd:sequence>
               <xsd:element name="gebaeudeteilName" type="tns:GebaeudeteilNameType"/>
               <xsd:element name="gebaeudeteilZusatz"
                            type="tns:GebaeudeteilZusatzType"
                            minOccurs="0"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="GebaeudeteilZusatzType">
            <xsd:annotation>
               <xsd:documentation>Gebäudeteilzusatz</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="25"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="GeschaeftsfallAenderungType" abstract="true"/>
         <xsd:simpleType name="GeschaeftsfallArtType">
            <xsd:annotation>
               <xsd:documentation>der Schlüssel für die Art des Geschäftsfalles nach der Orderschnittstelle als Kategorisierung für die Art des Kundenwunsches</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="50"/>
               <xsd:enumeration value="Bereitstellung">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Kuendigung">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Aenderung">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Produktgruppenwechsel">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Anbieterwechsel">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="BereitstellungService">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Auskunft">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="GeschaeftsfallEnumType">
            <xsd:annotation>
               <xsd:documentation>der Schlüssel zur Auswahl des Geschäftsfalles an der Orderschnittstelle als Unterkategorie der Geschäftsfall-Art; beschreibt, wie der Kundenauftrag prozessiert wird</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="10"/>
               <xsd:enumeration value="NEU">
                  <xsd:annotation>
                     <xsd:documentation>Bereitstellung (Bestellung neues Produkt)</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="KUE-KD">
                  <xsd:annotation>
                     <xsd:documentation>Kündigung durch den Kunden</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="KUE-DT">
                  <xsd:annotation>
                     <xsd:documentation>Kündigung durch die Telekom</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="AEN-LMAE">
                  <xsd:annotation>
                     <xsd:documentation>Änderung von Leistungsmerkmalen</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="LAE">
                  <xsd:annotation>
                     <xsd:documentation>Änderung von Leistungen</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="PGW">
                  <xsd:annotation>
                     <xsd:documentation>Produktgruppenwechsel</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="PV">
                  <xsd:annotation>
                     <xsd:documentation>Providerwechsel</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="VBL">
                  <xsd:annotation>
                     <xsd:documentation>Verbundleistung</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="SER-POW">
                  <xsd:annotation>
                     <xsd:documentation>Portwechsel</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="AUS-BUE">
                  <xsd:annotation>
                     <xsd:documentation>Bestandsübersicht</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="GeschaeftsfallProduktType" abstract="true">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basis für alle geschäftsfall-spezifische Produkt-Daten</xsd:documentation>
            </xsd:annotation>
         </xsd:complexType>
         <xsd:complexType name="GeschaeftsfallTerminverschiebungType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallAenderungType">
                  <xsd:sequence>
                     <xsd:element name="kundenwunschtermin" type="tns:TerminType">
                        <xsd:annotation>
                           <xsd:documentation>Wunschtermin bei der Terminverschiebung</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="zeitfenster" type="tns:ZeitfensterType">
                        <xsd:annotation>
                           <xsd:documentation>Eingrenzung des Kundenwunschtermins auf definierte Stundenintervalle</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Nur zulässige Zeitfenster möglich. Zeitfenster und Kundenwunschtermin müssen konsistent sein.</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Die Zuordnungen von einzelnen Werten zu den Zeitfenstern müssen aus der Zeitfenstertabelle entnommen werden</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="GeschaeftsfallType" abstract="true"/>
         <xsd:simpleType name="HausnummerType">
            <xsd:annotation>
               <xsd:documentation>Die Hausnummer wird als Zahl (0-9999) angegeben.</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:pattern value="\d{1,4}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="HausnummernZusatzType">
            <xsd:annotation>
               <xsd:documentation>eine mögliche Ergänzung zur Hausnummer</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="6"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="IPTVBasisUserType">
            <xsd:annotation>
               <xsd:documentation>Kennzeichnet den Endkunden des Providers. Einer der fixen Anteile der IPTV-OnlineKennung.</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="40"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="IPTVOnlineKennungType">
            <xsd:annotation>
               <xsd:documentation>Einzelne Angaben zur Onlinekennung des Endkunden für Accessvarianten mit IPTV</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Pflichtfeld bei erstmaliger Bereitstellung von Accessvarianten mit IPTV.</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="providerKennung" type="tns:IPTVProviderKennungType"/>
               <xsd:element name="basisUser" type="tns:IPTVBasisUserType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="IPTVProviderKennungType">
            <xsd:annotation>
               <xsd:documentation>Kennzeichnet den Provider, der den Access betreibt. Einer der fixen Anteile der IPTV-OnlineKennung.</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="15"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="KVZNrType">
            <xsd:annotation>
               <xsd:documentation>Eindeutiger Identifier des Kabelverteilers</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="8"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="KVZSchaltnummerType">
            <xsd:annotation>
               <xsd:documentation>Angaben des Carriers zu seinen Schaltdaten</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">Beispiel: 3 K1 0060</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="8"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="KennerType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="ProjektID" type="tns:ProjektIDType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>eindeutige Identifikation eines Projektes</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">nicht vorgesehen für GF/PG: NEU/Preselection, AUS-BUE</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Auftragskenner" type="tns:AuftragskennerType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>setzt sich zusammen aus Auftragsklammer und Anzahl der Aufträge</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">nicht vorgesehen für GF: KUE-KD, REX-MK, AUS-BUE</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="KopplungskennerType">
            <xsd:annotation>
               <xsd:documentation>eindeutige Identifikation zur Kopplung mehrerer Aufträge innerhalb eines Projektes</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="30"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="KuendigungAnlagenType">
            <xsd:annotation>
               <xsd:documentation>die möglichen Anlagen für eine Kündigung (je Dokument max. 1 Mb, insgesamt max. 4 Mb)</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="kuendigungsschreiben" type="tns:AnlageType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>das Kündigungsschreiben des Endkunden</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Beilage z.B. in Interimsprozessen. Siehe AHB4WITA</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="sonstige"
                            type="tns:AnlageMitTypType"
                            minOccurs="0"
                            maxOccurs="99">
                  <xsd:annotation>
                     <xsd:documentation>weitere Anlagen</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Neben Portierungsanzeige (notwendig im Anbieterwechsel wenn keine Haftungsfreistellungserklärung vorliegt) kann hier auch Lageplan und Kündigungsschreiben beigefügt werden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="KuendigungProduktType" abstract="true">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basis für alle geschäftsfall-spezifischen Produkt-Daten (Erweiterungspunkt)</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallProduktType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="KuendigungTermineType">
            <xsd:annotation>
               <xsd:documentation>die möglichen Termine für eine Kündigung</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="kundenwunschtermin" type="tns:KundenwunschterminType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="KuendigungTerminverschiebungType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallTerminverschiebungType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="KuendigungType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallType">
                  <xsd:sequence>
                     <xsd:element name="BKTOFaktura" type="tns:BuchungskontonummerType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Nummer, die zur Fakturierung heranzuziehen ist</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Wünscht Kunde dass ein Auftrag auf eine andere als die Standard BKTO abgerechnet wird, kann Kunde hier eine ihm zugeteilte aktive BKTO angeben</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="vertragsnummer" type="tns:VertragsnummerType">
                        <xsd:annotation>
                           <xsd:documentation>bezeichnet den Einzelvertrag, auf den sich der Auftrag bezieht</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="anlagen" type="tns:KuendigungAnlagenType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Anlagen zum Auftrag</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="ansprechpartner" type="tns:AnsprechpartnerType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Ansprechpartnern des Kunden</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="termine" type="tns:KuendigungTermineType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Terminen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="auftragsposition"
                                  type="tns:AuftragspositionKuendigungType"
                                  maxOccurs="99">
                        <xsd:annotation>
                           <xsd:documentation>Liste der Auftragspositionen, in der die Produkte / Leistungsmerkmale aufgeführt sind, die im Auftrag bearbeitet werden sollen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="KundeType">
            <xsd:sequence>
               <xsd:element name="kundennummer" type="tns:KundennummerType"/>
               <xsd:element name="leistungsnummer" type="tns:LeistungsnummerType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="KundennummerType">
            <xsd:annotation>
               <xsd:documentation>Eindeutige ID des Kunden, wird bei Neuanlage des Kunden vergeben</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:length value="10"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="KundenwunschterminType">
            <xsd:annotation>
               <xsd:documentation>Terminwunsch des Providers</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="datum" type="tns:TerminType">
                  <xsd:annotation>
                     <xsd:documentation>Terminwunsch des Providers, zu dem das Produkt betriebsfähig bereitgestellt werden soll</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="zeitfenster" type="tns:ZeitfensterType">
                  <xsd:annotation>
                     <xsd:documentation>Eingrenzung des Kundenwunschtermins auf definierte Stundenintervalle</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Nur zulässige Zeitfenster möglich. Zeitfenster und Kundenwunschtermin müssen konsistent sein.</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Die Zuordnungen von einzelnen Werten zu den Zeitfenstern müssen aus der Zeitfenstertabelle entnommen werden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="KupplungType">
            <xsd:annotation>
               <xsd:documentation>Nummer der Kupplung</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Einstellige Angaben werden immer mit führender 0 ergänzt</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="2"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="LandType">
            <xsd:annotation>
               <xsd:documentation>der zweistellige Ländercode (ISO)</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">der zweistellige ISO-Ländercode, Beispiel: de</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:length value="2"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="LeistungsaenderungAnlagenType">
            <xsd:annotation>
               <xsd:documentation>die möglichen Anlagen für eine Leistungsänderung(je Dokument max. 1 Mb, insgesamt max. 4 Mb)</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="sonstige"
                            type="tns:AnlageMitTypType"
                            minOccurs="0"
                            maxOccurs="99">
                  <xsd:annotation>
                     <xsd:documentation>weitere Anlagen</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Neben Portierungsanzeige (notwendig im Anbieterwechsel wenn keine Haftungsfreistellungserklärung vorliegt) kann hier auch Lageplan und Kündigungsschreiben beigefügt werden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="LeistungsaenderungProduktType" abstract="true">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basis für alle geschäftsfall-spezifischen Produkt-Daten (Erweiterungspunkt)</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallProduktType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="LeistungsaenderungTermineType">
            <xsd:annotation>
               <xsd:documentation>die möglichen Termine für eine Leistungsänderung</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="kundenwunschtermin" type="tns:KundenwunschterminType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="LeistungsaenderungTerminverschiebungType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallTerminverschiebungType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="LeistungsaenderungType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallType">
                  <xsd:sequence>
                     <xsd:element name="BKTOFaktura" type="tns:BuchungskontonummerType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Nummer, die zur Fakturierung heranzuziehen ist</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Wünscht Kunde dass ein Auftrag auf eine andere als die Standard BKTO abgerechnet wird, kann Kunde hier eine ihm zugeteilte aktive BKTO angeben</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="vertragsnummer" type="tns:VertragsnummerType">
                        <xsd:annotation>
                           <xsd:documentation>bezeichnet den Einzelvertrag, auf den sich der Auftrag bezieht</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="anlagen"
                                  type="tns:LeistungsaenderungAnlagenType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Anlagen zum Auftrag</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="ansprechpartner" type="tns:AnsprechpartnerType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Ansprechpartnern des Kunden</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="termine" type="tns:LeistungsaenderungTermineType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Terminen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="preOrderID" type="tns:UUIDType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>ID einer gültigen PreOrder, der Auftrag wird mit den Daten dieser PreOrder vervollständigt. Nur für Produktgruppen, für die eine vorgezogene PreOrder angeboten wird.</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Status der PreOrder: abgeschlossen oder in_Auftragspruefung; Eine PreOrder kann im Rahmen des Gültigkeitszeitraums wiederverwendet werden, wenn ein Vorgängerauftrag in der Auftragsprüfung abgelehnt worden ist. Nur für Produktgruppen, für die eine vorgezogene PreOrder angeboten wird.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="auftragsposition"
                                  type="tns:AuftragspositionLeistungsaenderungType"
                                  maxOccurs="99">
                        <xsd:annotation>
                           <xsd:documentation>Liste der Auftragspositionen, in der die Produkte / Leistungsmerkmale aufgeführt sind, die im Auftrag bearbeitet werden sollen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="LeistungsmerkmalAenderungAnlagenType">
            <xsd:annotation>
               <xsd:documentation>die möglichen Anlagen für eineLeistungsmerkmaländerung (je Dokument max. 1 Mb, insgesamt max. 4 Mb)</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="sonstige"
                            type="tns:AnlageMitTypType"
                            minOccurs="0"
                            maxOccurs="99">
                  <xsd:annotation>
                     <xsd:documentation>weitere Anlagen</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Neben Portierungsanzeige (notwendig im Anbieterwechsel wenn keine Haftungsfreistellungserklärung vorliegt) kann hier auch Lageplan und Kündigungsschreiben beigefügt werden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="LeistungsmerkmalAenderungProduktType">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basis für alle geschäftsfall-spezifischen Produkt-Daten (Erweiterungspunkt)</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallProduktType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="LeistungsmerkmalAenderungTermineType">
            <xsd:annotation>
               <xsd:documentation>die möglichen Termine für eine Leistungsmerkmaländerung</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="kundenwunschtermin" type="tns:KundenwunschterminType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="LeistungsmerkmalAenderungTerminverschiebungType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallTerminverschiebungType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="LeistungsmerkmalAenderungType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallType">
                  <xsd:sequence>
                     <xsd:element name="BKTOFaktura" type="tns:BuchungskontonummerType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Nummer, die zur Fakturierung heranzuziehen ist</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Wünscht Kunde dass ein Auftrag auf eine andere als die Standard BKTO abgerechnet wird, kann Kunde hier eine ihm zugeteilte aktive BKTO angeben</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="vertragsnummer" type="tns:VertragsnummerType">
                        <xsd:annotation>
                           <xsd:documentation>bezeichnet den Einzelvertrag, auf den sich der Auftrag bezieht</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="anlagen"
                                  type="tns:LeistungsmerkmalAenderungAnlagenType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Anlagen zum Auftrag</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="ansprechpartner" type="tns:AnsprechpartnerType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Ansprechpartnern des Kunden</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="termine" type="tns:LeistungsmerkmalAenderungTermineType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Terminen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="preOrderID" type="tns:UUIDType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>ID einer gültigen PreOrder, der Auftrag wird mit den Daten dieser PreOrder vervollständigt. Nur für Produktgruppen, für die eine vorgezogene PreOrder angeboten wird.</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Status der PreOrder: abgeschlossen oder in_Auftragspruefung; Eine PreOrder kann im Rahmen des Gültigkeitszeitraums wiederverwendet werden, wenn ein Vorgängerauftrag in der Auftragsprüfung abgelehnt worden ist. Nur für Produktgruppen, für die eine vorgezogene PreOrder angeboten wird.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="auftragsposition"
                                  type="tns:AuftragspositionLeistungsmerkmalAenderungType"
                                  maxOccurs="99">
                        <xsd:annotation>
                           <xsd:documentation>Liste der Auftragspositionen, in der die Produkte / Leistungsmerkmale aufgeführt sind, die im Auftrag bearbeitet werden sollen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="LeistungsmerkmalPositionType">
            <xsd:complexContent>
               <xsd:extension base="tns:AuftragspositionType">
                  <xsd:sequence>
                     <xsd:element name="geschaeftsfallProdukt" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>die Geschäftsfall-spezifischen Ergänzungen des Produktes der Auftragsposition</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                           <xsd:choice>
                              <xsd:element name="TAL" type="tns:TALLeistungsmerkmalAenderungType"/>
                              <xsd:element name="XDSL" type="tns:XDSLLeistungsmerkmalAenderungType"/>
                           </xsd:choice>
                        </xsd:complexType>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:simpleType name="LeistungsnummerType">
            <xsd:annotation>
               <xsd:documentation>Eindeutiger Identifier der Leistung aus einem Rahmenvertrag des Kunden der für den Auftrag heranzuziehen ist</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="10"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="LeitungsabschnittType">
            <xsd:annotation>
               <xsd:documentation>Angaben zu Leitungsabschnitt der Leitung</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="LfdNrLeitungsabschnitt">
                  <xsd:annotation>
                     <xsd:documentation>fortlaufende Nummer zur Identifikation des Leitungsabschnittes</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:int">
                        <xsd:minExclusive value="0"/>
                        <xsd:maxExclusive value="99"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="Leitungslaenge" type="tns:LeitungslaengeType">
                  <xsd:annotation>
                     <xsd:documentation>Länge (Einheit: Meter) des Leitungsabschnitts</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Leitungsdurchmesser" type="tns:LeitungsdurchmesserType">
                  <xsd:annotation>
                     <xsd:documentation>Durchmesser des Leitungsabschnitts</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="LeitungsbezeichnungType">
            <xsd:annotation>
               <xsd:documentation>eindeutige Identifikation einer Leitung</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="leitungsschluesselzahl" type="tns:LeitungsschluesselzahlType">
                  <xsd:annotation>
                     <xsd:documentation>kennzeichnet die Art der Leitung</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="onkzA" type="tns:OrtsnetzkennzahlType">
                  <xsd:annotation>
                     <xsd:documentation>die Ortsnetzkennzahl des Standortes A</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="onkzB" type="tns:OrtsnetzkennzahlType">
                  <xsd:annotation>
                     <xsd:documentation>die Ortsnetzkennzahl des Standortes B</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ordnungsnummer" type="tns:OrdnungsnummerType">
                  <xsd:annotation>
                     <xsd:documentation>fortlaufende Nummer zur Kennzeichnung der Leitung</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="LeitungsdurchmesserType">
            <xsd:annotation>
               <xsd:documentation>Durchmesser des Leitungsabschnitts</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="5"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="LeitungslaengeType">
            <xsd:annotation>
               <xsd:documentation>Länge (Einheit: Meter) des Leitungsabschnitts</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="5"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="LeitungsschluesselzahlType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="4"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="LeitungswiderstandType">
            <xsd:annotation>
               <xsd:documentation>Gesamtwiderstand in Ohm</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">Beispiele: '&lt; 1200', '&gt; 1200'</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="10"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="LineIDType">
            <xsd:annotation>
               <xsd:documentation>LineID für eindeutige Anschlussidentifizierung</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">Format: "[ITU M.1400 Country Code].[ITU M.1400 CarrierCode].[Unique Number of Line]". Beispiel:
				DEU.DTAG.1754639821 </xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="7"/>
               <xsd:maxLength value="21"/>
               <xsd:pattern value="[0-9A-Z]{3}\.[0-9A-Z]{1,6}\.[0-9A-Z]{1,10}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="LinkAN512Type">
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="512"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="MeldungProduktType" abstract="true">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basis für alle meldungsspezifischen Produkt-Daten (Erweiterungspunkt)</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:ProduktType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="MeldungType">
            <xsd:sequence>
               <xsd:element name="auftragstyp" type="tns:AuftragstypType">
                  <xsd:annotation>
                     <xsd:documentation>die Klassifizierung des zugehörigen Auftrags</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="meldungstyp" type="tns:MeldungstypAbstractType">
                  <xsd:annotation>
                     <xsd:documentation>Erweiterungspunkt für die konkreten Meldungstypen</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="MeldungsattributeERLM-KType">
            <xsd:complexContent>
               <xsd:extension base="tns:MeldungsattributeType">
                  <xsd:sequence>
                     <xsd:element name="vertragsnummer" type="tns:VertragsnummerType"/>
                     <xsd:element name="externeAuftragsnummer" type="tns:ExterneAuftragsnummerType">
                        <xsd:annotation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Angaben aus dem Kundenauftrag</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="kundennummer" type="tns:KundennummerType"/>
                     <xsd:element name="kundennummerBesteller"
                                  type="tns:KundennummerType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Eindeutiger Identifier des Bestellers für den Fall, dass der Besteller einen Auftrag im Namen eines anderen Kunden erteilt (Beauftragung im Namen Dritter)</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="MeldungsattributeRUEM-PVType">
            <xsd:complexContent>
               <xsd:extension base="tns:MeldungsattributeType">
                  <xsd:sequence>
                     <xsd:element name="vertragsnummer" type="tns:VertragsnummerType">
                        <xsd:annotation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">wird die Vertragsnummer des abgebenden Providers angegeben</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="kundennummer" type="tns:KundennummerType">
                        <xsd:annotation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">die Kundennummer des abgebenden Providers wird angegeben</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="kundennummerBesteller"
                                  type="tns:KundennummerType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Eindeutiger Identifier des Bestellers für den Fall, dass der Besteller einen Auftrag im Namen eines anderen Kunden erteilt (Beauftragung im Namen Dritter)</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="abgebenderProvider" type="tns:AbgebenderProviderType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zum Provider, der den Anschluss abgibt</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="MeldungsattributeTAL-RUEM-PVType">
            <xsd:complexContent>
               <xsd:extension base="tns:MeldungsattributeRUEM-PVType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="MeldungsattributeType" abstract="true">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basisklasse der Meldungsattribute zu den verschiedenen Meldunsgtypen</xsd:documentation>
            </xsd:annotation>
         </xsd:complexType>
         <xsd:complexType name="MeldungsattributeVZM-PVType">
            <xsd:complexContent>
               <xsd:extension base="tns:MeldungsattributeType">
                  <xsd:sequence>
                     <xsd:element name="vertragsnummer" type="tns:VertragsnummerType">
                        <xsd:annotation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">wird die Vertragsnummer des abgebenden Providers angegeben</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="kundennummer" type="tns:KundennummerType"/>
                     <xsd:element name="verzoegerungstermin" type="xsd:date" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Voraussichtliches Datum, bis zu dem sich die Auftragsdurchführung verzögert</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Wird derzeit nicht genutzt</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:simpleType name="MeldungscodeType">
            <xsd:annotation>
               <xsd:documentation>Kurzbezeichnung zum Meldungstext</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="10"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="MeldungspositionOhneAttributeType">
            <xsd:annotation>
               <xsd:documentation>eine Einzelposition zu einer Meldung</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="meldungscode" type="tns:MeldungscodeType">
                  <xsd:annotation>
                     <xsd:documentation>Kurzbezeichnung zum Meldungstext</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Meldungscode muss aus der Meldungscodetabelle entnommen werden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="meldungstext" type="tns:MeldungstextType">
                  <xsd:annotation>
                     <xsd:documentation>Ausführliche Beschreibung des Meldungstextes, der für den Meldungscode definiert ist</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Meldungstext muss dem Meldungcode in der Meldungscodetabelle zugeordnet sein</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="MeldungspositionType">
            <xsd:annotation>
               <xsd:documentation>Menge der Meldungspositionen, die zu einer Meldung aufgeführt sein können</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:MeldungspositionOhneAttributeType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="MeldungspositionsattributeType" abstract="true">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basisklasse der Meldungspositiponsattribute zu den verschiedenen Meldunsgtypen</xsd:documentation>
            </xsd:annotation>
         </xsd:complexType>
         <xsd:simpleType name="MeldungstextType">
            <xsd:annotation>
               <xsd:documentation>ausführliche Beschreibung für den Meldungscode</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichMehrzeiligType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="255"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="MeldungstypAbstractType" abstract="true">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basisklasse der verschiedenen Meldunsgtypen</xsd:documentation>
            </xsd:annotation>
         </xsd:complexType>
         <xsd:complexType name="MeldungstypERLM-KType">
            <xsd:complexContent>
               <xsd:extension base="tns:MeldungstypAbstractType">
                  <xsd:sequence>
                     <xsd:element name="meldungsattribute" type="tns:MeldungsattributeERLM-KType">
                        <xsd:annotation>
                           <xsd:documentation>Meldungsattribute</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="meldungspositionen">
                        <xsd:annotation>
                           <xsd:documentation>Meldungspositionen</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                           <xsd:sequence>
                              <xsd:element name="position" type="tns:MeldungspositionType" maxOccurs="99"/>
                           </xsd:sequence>
                        </xsd:complexType>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="MeldungstypRUEM-PVType">
            <xsd:complexContent>
               <xsd:extension base="tns:MeldungstypAbstractType">
                  <xsd:sequence>
                     <xsd:element name="meldungsattribute">
                        <xsd:annotation>
                           <xsd:documentation>Meldungsattribute</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                           <xsd:complexContent>
                              <xsd:extension base="tns:MeldungsattributeRUEM-PVType">
                                 <xsd:choice minOccurs="0">
                                    <xsd:element name="TAL">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="leitung" type="tns:BasisAngabenZurLeitungType"
                                                          minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation>Hinweise zur beauftragten
                                                      Teilnehmeranschlussleitung
                                                   </xsd:documentation>
                                                   <xsd:documentation
                                                           source="http://wholesale.telekom.de/oss/doc/notes">Es ist die
                                                      Leitungsbezeichnung des abgebenden Providers aufgeführt
                                                   </xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="anschluss" type="tns:AnschlussType" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation>Hinweise zum beauftragen Anschluss
                                                   </xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="vorabstimmungsID"
                                                          type="tns:VorabstimmungsIdType"
                                                          minOccurs="0"/>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="XDSL">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="vorabstimmungsID"
                                                          type="tns:VorabstimmungsIdType"
                                                          minOccurs="0"/>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:choice>
                              </xsd:extension>
                           </xsd:complexContent>
                        </xsd:complexType>
                     </xsd:element>
                     <xsd:element name="meldungspositionen">
                        <xsd:annotation>
                           <xsd:documentation>Meldungspositionen</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                           <xsd:sequence>
                              <xsd:element name="position" type="tns:MeldungspositionType" maxOccurs="99"/>
                           </xsd:sequence>
                        </xsd:complexType>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:simpleType name="MeldungstypType">
            <xsd:annotation>
               <xsd:documentation>kategorisiert die Art der Meldung</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="10"/>
               <xsd:enumeration value="TEQ">
                  <xsd:annotation>
                     <xsd:documentation>Technische Quittung</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="QEB">
                  <xsd:annotation>
                     <xsd:documentation>Qualifizierte Eingangsbestätigung</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="ABM">
                  <xsd:annotation>
                     <xsd:documentation>Auftragsbestätigung</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="ERLM">
                  <xsd:annotation>
                     <xsd:documentation>Erledigungsmeldung</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="ERLM-K">
                  <xsd:annotation>
                     <xsd:documentation>Erledigungsmeldung Kunde</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="ERGM">
                  <xsd:annotation>
                     <xsd:documentation>Ergebnismeldung</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="ENTM">
                  <xsd:annotation>
                     <xsd:documentation>Entgeltmeldung</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="AKM-PV">
                  <xsd:annotation>
                     <xsd:documentation>Ankündigung an abgebenden Provider</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="RUEM-PV">
                  <xsd:annotation>
                     <xsd:documentation>Rückmeldung des abgebenden Provider</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="ABM-PV">
                  <xsd:annotation>
                     <xsd:documentation>Auftragsbestätigung an abgebenden Provider</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="ERLM-PV">
                  <xsd:annotation>
                     <xsd:documentation>Erledigungssmeldung an abgebenden Provider</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="ENTM-PV">
                  <xsd:annotation>
                     <xsd:documentation>Entgeltmeldung an abgebenden Provider</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="ABBM-PV">
                  <xsd:annotation>
                     <xsd:documentation>Abbruchmeldung an abgebenden Provider</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="ABBM">
                  <xsd:annotation>
                     <xsd:documentation>Abbruchmeldung</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="VZM">
                  <xsd:annotation>
                     <xsd:documentation>Verzögerungsmeldung</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="TAM">
                  <xsd:annotation>
                     <xsd:documentation>Terminanforderungsmeldung</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="MTAM">
                  <xsd:annotation>
                     <xsd:documentation>Mahn-Terminanforderungsmeldung</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="MeldungstypVZM-PVType">
            <xsd:complexContent>
               <xsd:extension base="tns:MeldungstypAbstractType">
                  <xsd:sequence>
                     <xsd:element name="meldungsattribute" type="tns:MeldungsattributeVZM-PVType">
                        <xsd:annotation>
                           <xsd:documentation>Meldungsattribute</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="meldungspositionen">
                        <xsd:annotation>
                           <xsd:documentation>Meldungspositionen</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                           <xsd:sequence>
                              <xsd:element name="position" type="tns:MeldungspositionType" maxOccurs="99"/>
                           </xsd:sequence>
                        </xsd:complexType>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:simpleType name="MinorVersionType">
            <xsd:annotation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
    Format: zweitstellige nummerische Angabe als Nummer der Minorversion 
	Beispiel: '00', '02'
			</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:length value="2"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="MontagehinweisType">
            <xsd:annotation>
               <xsd:documentation>Nähere Angaben zur Durchführung der Montageleistung</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Für die Felder Lage TAE und Montagehinweis stehen in Summe 160 Zeichen zur Verfügung. Gelesen wird zunächst der Montagehinweis. Bei Angabe von mehr als 160 Zeichen können diese nicht mehr an den Service übermittelt werden</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichMehrzeiligType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="160"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="NachnameType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="30"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="OnkzAnschlussType">
            <xsd:annotation>
               <xsd:documentation>Der Typ Anschluss ermöglicht die Identifikation eines Anschlusses nach einer aus einem Menge
				von Identifikationsverfahren.</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="ONKZ" type="tns:OrtsnetzkennzahlType">
                  <xsd:annotation>
                     <xsd:documentation>die Ortsnetzkennzahl des Standortes des Kollokationsraumes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="anschlussbereich" type="tns:AnschlussbereichType">
                  <xsd:annotation>
                     <xsd:documentation>Anschlussbereich (ASB), in dem sich der Kollokationsraum befindet
					</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="OnkzDurchwahlAbfragestelleType">
            <xsd:annotation>
               <xsd:documentation>Kombination von der ONKZ, Durchwahlnummer und Abfragestelle (für die Anlagenanschlüsse)</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="ONKZ" type="tns:OrtsnetzkennzahlType">
                  <xsd:annotation>
                     <xsd:documentation>Ortsnetzkennzahl des Anschlusses für Rufnummer</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="durchwahlnummer" type="tns:DurchwahlnummerType"/>
               <xsd:element name="abfragestelle" type="tns:AbfragestelleType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="OnkzErsteMSNRngNrType">
            <xsd:annotation>
               <xsd:documentation>Angaben zu ONKZ und Rufnummer (1. MSN)/Rechnungsnummer</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="ONKZ" type="tns:OrtsnetzkennzahlType">
                  <xsd:annotation>
                     <xsd:documentation>Ortsnetzkennzahl des Anschlusses für Rufnummer</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="rufnummer" type="tns:RufnummerType">
                  <xsd:annotation>
                     <xsd:documentation>Rufnummer des Anschlusses (1. MSN), bei Anlagenanschlüssen bzw. digitalen Sammelanschlüssen die Rechnungsnummer des den Auftrag betreffendes Anschlussbestandes</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Nur die 1. MSN zulässig</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="OnkzRnrRngNrType">
            <xsd:annotation>
               <xsd:documentation>Angaben zu ONKZ und Rufnummer (beliebige MSN)/Rechnungsnummer</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="ONKZ" type="tns:OrtsnetzkennzahlType">
                  <xsd:annotation>
                     <xsd:documentation>Ortsnetzkennzahl des Anschlusses für Rufnummer</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="rufnummer" type="tns:RufnummerType">
                  <xsd:annotation>
                     <xsd:documentation>Rufnummer des Anschlusses, bei Anlagenanschlüssen bzw. digitalen Sammelanschlüssen die Rechnungsnummer des den Auftrag betreffendes Anschlussbestandes</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Eine beliebige MSN zulässig</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="OnkzRufNrType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="ONKZ" type="tns:OrtsnetzkennzahlType">
                  <xsd:annotation>
                     <xsd:documentation>die Ortsnetzkennzahl des Anschlusses</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="rufnummer" type="tns:RufnummerType">
                  <xsd:annotation>
                     <xsd:documentation>Rufnummer des Anschlusses</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="OrdnungsnummerType">
            <xsd:annotation>
               <xsd:documentation>fortlaufende Nummer zur Leitungskennzeichnung</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="10"/>
               <xsd:whiteSpace value="collapse"/>
               <xsd:pattern value="\d{1,10}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="OrtType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="ortsname" type="tns:OrtsnameType">
                  <xsd:annotation>
                     <xsd:documentation>Ortsname</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ortsteil" type="tns:OrtsteilType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Ortsteil</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="OrtsnameType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="40"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="OrtsnetzkennzahlType">
            <xsd:annotation>
               <xsd:documentation>Ortsnetzkennzahl des Anschlusses</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">keine führende '0'</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="2"/>
               <xsd:maxLength value="5"/>
               <xsd:pattern value="\d{2,5}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="OrtsteilType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="40"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="PersonType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="anrede" type="tns:AnredePersonType">
                  <xsd:annotation>
                     <xsd:documentation>Anrede</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="vorname" type="tns:VornameType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Vorname</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="nachname" type="tns:NachnameType">
                  <xsd:annotation>
                     <xsd:documentation>Nachname</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="PortierungRufnummernMeldungType">
            <xsd:annotation>
               <xsd:documentation>Informationen über zu portierenden Rufnummern</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="zuPortierendeOnkzRnr" type="tns:OnkzRufNrType" maxOccurs="10">
                  <xsd:annotation>
                     <xsd:documentation>Auflistung der zu portierenden ONKZ/Runfnummern</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="PortierungRufnummernbloeckeMeldungType">
            <xsd:annotation>
               <xsd:documentation>Informationen über zu portierenden Rufnummernblöcken</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="onkzDurchwahlAbfragestelle"
                            type="tns:OnkzDurchwahlAbfragestelleType"/>
               <xsd:element name="zuPortierenderRufnummernblock"
                            type="tns:RufnummernblockType"
                            maxOccurs="5">
                  <xsd:annotation>
                     <xsd:documentation>Auflistung zu portierender Rufnummernblöcken am Anlagenanschluss</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="PortwechselAnlagenType">
            <xsd:annotation>
               <xsd:documentation>die möglichen Anlagen für eine Service Bereitstellung (je Dokument max. 1 Mb, insgesamt max. 4 Mb)</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="sonstige"
                            type="tns:AnlageMitTypType"
                            minOccurs="0"
                            maxOccurs="99">
                  <xsd:annotation>
                     <xsd:documentation>weitere Anlagen</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Neben Portierungsanzeige (notwendig im Anbieterwechsel wenn keine Haftungsfreistellungserklärung vorliegt) kann hier auch Lageplan und Kündigungsschreiben beigefügt werden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="PortwechselProduktType" abstract="true">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basis für alle geschäftsfall-spezifischen Produkt-Daten (Erweiterungspunkt)</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallProduktType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="PortwechselTermineType">
            <xsd:annotation>
               <xsd:documentation>die möglichen Termine für eine Service Bereitstellung</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="kundenwunschtermin" type="tns:KundenwunschterminType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="PortwechselTerminverschiebungType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallTerminverschiebungType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="PortwechselType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallType">
                  <xsd:sequence>
                     <xsd:element name="BKTOFaktura" type="tns:BuchungskontonummerType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Nummer, die zur Fakturierung heranzuziehen ist</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Wünscht Kunde dass ein Auftrag auf eine andere als die Standard BKTO abgerechnet wird, kann Kunde hier eine ihm zugeteilte aktive BKTO angeben</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="vertragsnummer" type="tns:VertragsnummerType">
                        <xsd:annotation>
                           <xsd:documentation>bezeichnet den Einzelvertrag, auf den sich der Auftrag bezieht</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="anlagen" type="tns:PortwechselAnlagenType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Anlagen zum Auftrag</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="ansprechpartner" type="tns:AnsprechpartnerType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Ansprechpartnern des Kunden</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="termine" type="tns:PortwechselTermineType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Terminen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="auftragsposition"
                                  type="tns:AuftragspositionPortwechselType"
                                  maxOccurs="99">
                        <xsd:annotation>
                           <xsd:documentation>Liste der Auftragspositionen, in der die Produkte / Leistungsmerkmale aufgeführt sind, die im Auftrag bearbeitet werden sollen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:simpleType name="PostleitzahlType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="10"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="ProduktTALMeldungType">
            <xsd:complexContent>
               <xsd:extension base="tns:MeldungProduktType">
                  <xsd:sequence>
                     <xsd:element name="uebertragungsVerfahren"
                                  type="tns:Uebertragungsverfahren-TAL-Type"
                                  minOccurs="0"/>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="ProduktType">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basis für alle Produkt-Strukturen</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="kategorie">
                  <xsd:annotation>
                     <xsd:documentation>Produktkategorie</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="TAL"/>
                        <xsd:enumeration value="XDSL"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="bezeichner" type="tns:ProduktbezeichnerType">
                  <xsd:annotation>
                     <xsd:documentation>Produktname des Produktes, das in der Auftragsposition zugebucht, in Wegfall gebracht oder geändert werden soll</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Nur zulässige Produktnamen aus Produktbezeichner-Anlage möglich</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">s. Anlage Produktbezeichner je Produktgruppe</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="ProduktbezeichnerType">
            <xsd:annotation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
s. Anlage Produktbezeichner je Produktgruppe
Beispiele: "ADSL SA 16000", "TAL; CuDA 2 Draht hbr (HVt)" 
			</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="100"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="ProduktgruppenwechselAnlagenType">
            <xsd:annotation>
               <xsd:documentation>die möglichen Anlagen für einen Produktgruppenwechsel (je Dokument max. 1 Mb, insgesamt max. 4 Mb)</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="sonstige"
                            type="tns:AnlageMitTypType"
                            minOccurs="0"
                            maxOccurs="99">
                  <xsd:annotation>
                     <xsd:documentation>weitere Anlagen</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Neben Portierungsanzeige (notwendig im Anbieterwechsel wenn keine Haftungsfreistellungserklärung vorliegt) kann hier auch Lageplan und Kündigungsschreiben beigefügt werden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="ProduktgruppenwechselProduktType" abstract="true">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basis für alle geschäftsfall-spezifischen Produkt-Daten (Erweiterungspunkt)</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallProduktType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="ProduktgruppenwechselTermineType">
            <xsd:annotation>
               <xsd:documentation>die möglichen Termine für einen Produktgruppenwechsel</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="kundenwunschtermin" type="tns:KundenwunschterminType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="ProduktgruppenwechselTerminverschiebungType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallTerminverschiebungType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="ProduktgruppenwechselType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallType">
                  <xsd:sequence>
                     <xsd:element name="BKTOFaktura" type="tns:BuchungskontonummerType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Nummer, die zur Fakturierung heranzuziehen ist</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Wünscht Kunde dass ein Auftrag auf eine andere als die Standard BKTO abgerechnet wird, kann Kunde hier eine ihm zugeteilte aktive BKTO angeben</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="vertragsnummer" type="tns:VertragsnummerType">
                        <xsd:annotation>
                           <xsd:documentation>bezeichnet den Einzelvertrag, auf den sich der Auftrag bezieht</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="anlagen"
                                  type="tns:ProduktgruppenwechselAnlagenType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Anlagen zum Auftrag</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="ansprechpartner" type="tns:AnsprechpartnerType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Ansprechpartnern des Kunden</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="termine" type="tns:ProduktgruppenwechselTermineType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Terminen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="preOrderID" type="tns:UUIDType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>ID einer gültigen PreOrder, der Auftrag wird mit den Daten dieser PreOrder vervollständigt. Nur für Produktgruppen, für die eine vorgezogene PreOrder angeboten wird.</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Status der PreOrder: abgeschlossen oder in_Auftragspruefung; Eine PreOrder kann im Rahmen des Gültigkeitszeitraums wiederverwendet werden, wenn ein Vorgängerauftrag in der Auftragsprüfung abgelehnt worden ist. Nur für Produktgruppen, für die eine vorgezogene PreOrder angeboten wird.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="auftragsposition"
                                  type="tns:AuftragspositionProduktgruppenwechselType"
                                  maxOccurs="99">
                        <xsd:annotation>
                           <xsd:documentation>Liste der Auftragspositionen, in der die Produkte / Leistungsmerkmale aufgeführt sind, die im Auftrag bearbeitet werden sollen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="ProduktpositionType">
            <xsd:sequence>
               <xsd:element name="aktionscode" type="tns:AktionscodeType">
                  <xsd:annotation>
                     <xsd:documentation>Kenner für Zugang Wegfall oder Änderung des Produktes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="produkt" type="tns:MeldungProduktType">
                  <xsd:annotation>
                     <xsd:documentation>Produkt, das im Auftrag zugebucht, geändert bzw. in Wegfall gebracht wurde</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="ProjektIDType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="projektkenner" type="tns:ProjektkennerType">
                  <xsd:annotation>
                     <xsd:documentation>eindeutige ID des Projektes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="kopplungskenner" type="tns:KopplungskennerType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>eindeutige ID zur Kopplung mehrerer Aufträge innerhalb des Projektes
					</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="ProjektkennerType">
            <xsd:annotation>
               <xsd:documentation>eindeutige Identifikation eines Projektes</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="30"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="ProvidernameType">
            <xsd:annotation>
               <xsd:documentation>Name eines Providers</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="60"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="ProviderwechselAnlagenType">
            <xsd:annotation>
               <xsd:documentation>die möglichen Anlagen für einen Providerwechsel (je Dokument max. 1 Mb, insgesamt max. 4 Mb)</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="kuendigungsschreiben" type="tns:AnlageType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>das Kündigungsschreiben des Endkunden</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Beilage z.B. in Interimsprozessen. Siehe AHB4WITA</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="sonstige"
                            type="tns:AnlageMitTypType"
                            minOccurs="0"
                            maxOccurs="99">
                  <xsd:annotation>
                     <xsd:documentation>weitere Anlagen</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Neben Portierungsanzeige (notwendig im Anbieterwechsel wenn keine Haftungsfreistellungserklärung vorliegt) kann hier auch Lageplan und Kündigungsschreiben beigefügt werden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="ProviderwechselProduktType" abstract="true">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basis für alle geschäftsfall-spezifischen Produkt-Daten (Erweiterungspunkt)</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallProduktType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="ProviderwechselTermineType">
            <xsd:annotation>
               <xsd:documentation>die möglichen Termine für einen Providerwechsel</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="kundenwunschtermin" type="tns:KundenwunschterminType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="ProviderwechselTerminverschiebungType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallTerminverschiebungType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="ProviderwechselType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallType">
                  <xsd:sequence>
                     <xsd:element name="BKTOFaktura" type="tns:BuchungskontonummerType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Nummer, die zur Fakturierung heranzuziehen ist</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Wünscht Kunde dass ein Auftrag auf eine andere als die Standard BKTO abgerechnet wird, kann Kunde hier eine ihm zugeteilte aktive BKTO angeben</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="vertragsnummer" type="tns:VertragsnummerType">
                        <xsd:annotation>
                           <xsd:documentation>bezeichnet den Einzelvertrag, auf den sich der Auftrag bezieht</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">falls abgebender Provider nicht Telekom ist, muss die Vertragsnummer des abgebenden Providers angegeben werden. Ist abgebender Provider die Telekom und Vorabstimmungsprozess über WBCI erfolgt, muss die Pseudo-Vertragsnummer angegeben werden.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="anlagen" type="tns:ProviderwechselAnlagenType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Anlagen zum Auftrag</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Pflichtangabe, falls keine Hafttungsfreistellung in den Dienstverträgen vereinbart</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="ansprechpartner" type="tns:AnsprechpartnerType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Ansprechpartnern des Kunden</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="termine" type="tns:ProviderwechselTermineType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Terminen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="preOrderID" type="tns:UUIDType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>ID einer gültigen PreOrder, der Auftrag wird mit den Daten dieser PreOrder vervollständigt. Nur für Produktgruppen, für die eine vorgezogene PreOrder angeboten wird.</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Status der PreOrder: abgeschlossen oder in_Auftragspruefung; Eine PreOrder kann im Rahmen des Gültigkeitszeitraums wiederverwendet werden, wenn ein Vorgängerauftrag in der Auftragsprüfung abgelehnt worden ist. Nur für Produktgruppen, für die eine vorgezogene PreOrder angeboten wird.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="auftragsposition"
                                  type="tns:AuftragspositionProviderwechselType"
                                  maxOccurs="99">
                        <xsd:annotation>
                           <xsd:documentation>Liste der Auftragspositionen, in der die Produkte / Leistungsmerkmale aufgeführt sind, die im Auftrag bearbeitet werden sollen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:simpleType name="ReservierungsnummerType">
            <xsd:annotation>
               <xsd:documentation>Eindeutige Reservierungsnummer für den gebuchten Montagetermin</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="20"/>
               <xsd:pattern value="\d{1,20}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="RufnummerType">
            <xsd:annotation>
               <xsd:documentation>Die Rufnummer besteht aus maximal 14 Ziffern.</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="14"/>
               <xsd:pattern value="\d{1,14}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="RufnummernblockType">
            <xsd:annotation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
	- der Wert von 'Rufnummernblock von' muss kleiner sein als der Wert von 'Rufnummernblock bis'
	- die die Nummernblocklänge von 'Rufnummernblock von' und von 'Rufnummernblock bis' müssen jeweils die gleiche Stellenzahl aufweisen
	- alle Ziffern außer der ersten Ziffer müssen bei 'Rufnummernblock von' gleich '0' sein, beispielsweise '20', '300'
	- alle Ziffern außer der ersten Ziffer müssen bei 'Rufnummernblock bis' gleich '9' sein, beispielsweise '29', '499'
	- die Schnittmenge der angegebenen Rufnummernblöcke muss die leere Menge sein, die Rufnummernblöcke dürfen sich nicht überschneiden		
			</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="rnrBlockVon">
                  <xsd:annotation>
                     <xsd:documentation>Untergrenze innerhalb des zu portierenden Rufnummernblocks</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:pattern value="\d{1,6}"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="rnrBlockBis">
                  <xsd:annotation>
                     <xsd:documentation>Obergrenze innerhalb des zu portierenden Rufnummernblocks</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:pattern value="\d{1,6}"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="RufnummernportierungEinzelanschlussMeldungType">
            <xsd:annotation>
               <xsd:documentation>Hinweise zu der Portierung  von Rufnummern oder Rufnummernblöcken eines Einzel- oder Anlagenanschlusses</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:restriction base="tns:RufnummernportierungMeldungType">
                  <xsd:sequence>
                     <xsd:element name="portierungRufnummern" type="tns:PortierungRufnummernMeldungType">
                        <xsd:annotation>
                           <xsd:documentation>Hinweise zu der Portierung von Rufnummern eines Anschlusses</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:restriction>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="RufnummernportierungMeldungType">
            <xsd:annotation>
               <xsd:documentation>Hinweise zu der Portierung  von Rufnummern oder Rufnummernblöcken eines Einzel- oder Anlagenanschlusses</xsd:documentation>
            </xsd:annotation>
            <xsd:choice>
               <xsd:element name="portierungRufnummern" type="tns:PortierungRufnummernMeldungType">
                  <xsd:annotation>
                     <xsd:documentation>Hinweise zu der Portierung von Rufnummern eines Anschlusses</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="portierungRufnummernbloecke"
                            type="tns:PortierungRufnummernbloeckeMeldungType">
                  <xsd:annotation>
                     <xsd:documentation>Hinweise zu der Portierung von Rufnummernblöcken eines Anlagenanschlusses</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:choice>
         </xsd:complexType>
         <xsd:complexType name="SchaltangabenType">
            <xsd:annotation>
               <xsd:documentation>beschreibt die technischen Vorgaben zur Schaltung</xsd:documentation>
