         <xsd:simpleType name="TelefonnummerType">
            <xsd:annotation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
	Es werden Formate mit und ohne Ländercode unterstützt. Beispiele: +49 351 111222 oder 0351 111222
			</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="30"/>
               <xsd:whiteSpace value="collapse"/>
               <xsd:pattern value="((\+\d{2} |0)\d{2,5} )?\d{1,14}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="TerminType">
            <xsd:annotation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
    Format: YYYY-MM-DD 
	Beispiel: 2009-02-18
			</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:date">
               <xsd:pattern value="\d{4}-.*"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="TerminverschiebungType">
            <xsd:complexContent>
               <xsd:extension base="tns:AuftragType">
                  <xsd:sequence>
                     <xsd:element name="geschaeftsfallAenderung"
                                  type="tns:GeschaeftsfallTerminverschiebungType">
                        <xsd:annotation>
                           <xsd:documentation>Änderungen zum Geschäftsfall bei dem Änderungskennzeichen: Terminverschiebung</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Bei AUS-BUE: Keine Terminverschiebung möglich</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="UFAType">
            <xsd:annotation>
               <xsd:documentation>Nummer des unbelegten fiktiven Anschlusses</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="UFAnummer">
                  <xsd:annotation>
                     <xsd:documentation>Nummer des unbelegten fiktiven Anschlusses</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:annotation>
                        <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
	Format 1: '[N1]C[N2]', wo [N1] - 2 bis 5-stellige Zahl, C - Trennzeichen, [N2] - 6 bis 7-stellige Zahl
	Beispiel: 12345C1234567
	Format 2: alphanumerisch 1 bis 15 Zeichen lang						
						</xsd:documentation>
                     </xsd:annotation>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="15"/>
                        <xsd:whiteSpace value="collapse"/>
                        <xsd:pattern value="\d{2,5}C\d{6,7}"/>
                        <xsd:pattern value="[0-9a-zA-Z]{1,15}"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="UUIDType">
            <xsd:annotation>
               <xsd:documentation source="http://wholesale.telekom.de/doc/format">Eindeutige ID, generiert nach nach dem UUID (Universally unique identifier) Standard
			Beispiel: de305d54-75b4-431b-adb2-eb6b9e546014</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="36"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="UebergabeverteilerType">
            <xsd:annotation>
               <xsd:documentation>Nummer des Übergabeverteilers</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="4"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="Uebertragungsverfahren-TAL-Type">
            <xsd:annotation>
               <xsd:documentation>Ausprägung des Übertragungsverfahrens</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:UebertragungsverfahrenType"/>
         </xsd:simpleType>
         <xsd:simpleType name="UebertragungsverfahrenType">
            <xsd:annotation>
               <xsd:documentation>Angaben zur produktspezifischen Qualität des Anschlusses</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:enumeration value="H01"/>
               <xsd:enumeration value="H02"/>
               <xsd:enumeration value="H03"/>
               <xsd:enumeration value="H04"/>
               <xsd:enumeration value="H05"/>
               <xsd:enumeration value="H06"/>
               <xsd:enumeration value="H07"/>
               <xsd:enumeration value="H08"/>
               <xsd:enumeration value="H10"/>
               <xsd:enumeration value="H11"/>
               <xsd:enumeration value="H13"/>
               <xsd:enumeration value="H14"/>
               <xsd:enumeration value="H15"/>
               <xsd:enumeration value="H16"/>
               <xsd:enumeration value="H17"/>
               <xsd:enumeration value="H18"/>
               <xsd:enumeration value="H19"/>
               <xsd:enumeration value="H20"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="V51IdType">
            <xsd:annotation>
               <xsd:documentation>ID der V5.1-Schnittstelle</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="8"/>
               <xsd:pattern value="\d{1,8}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="VerbundleistungAnlagenType">
            <xsd:annotation>
               <xsd:documentation>die möglichen Anlagen für eine Verbundleistung (je Dokument max. 1 Mb, insgesamt max. 4 Mb)</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="kuendigungsschreiben" type="tns:AnlageType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>das Kündigungsschreiben des Endkunden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="sonstige"
                            type="tns:AnlageMitTypType"
                            minOccurs="0"
                            maxOccurs="99">
                  <xsd:annotation>
                     <xsd:documentation>weitere Anlagen</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Neben Portierungsanzeige (notwendig im Anbieterwechsel wenn keine Haftungsfreistellungserklärung vorliegt) kann hier auch Lageplan und Kündigungsschreiben beigefügt werden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="VerbundleistungProduktType" abstract="true">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basis für alle geschäftsfall-spezifischen Produkt-Daten (Erweiterungspunkt)</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallProduktType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="VerbundleistungTermineType">
            <xsd:annotation>
               <xsd:documentation>die möglichen Termine für eine Verbundleistung</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="kundenwunschtermin" type="tns:KundenwunschterminType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="VerbundleistungTerminverschiebungType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallTerminverschiebungType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="VerbundleistungType">
            <xsd:complexContent>
               <xsd:extension base="tns:GeschaeftsfallType">
                  <xsd:sequence>
                     <xsd:element name="BKTOFaktura" type="tns:BuchungskontonummerType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Nummer, die zur Fakturierung heranzuziehen ist</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Wünscht Kunde dass ein Auftrag auf eine andere als die Standard BKTO abgerechnet wird, kann Kunde hier eine ihm zugeteilte aktive BKTO angeben</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="vertragsnummer" type="tns:VertragsnummerType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>bezeichnet den Einzelvertrag, auf den sich der Auftrag bezieht</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">falls abgebender Provider nicht Telekom ist, muss die Vertragsnummer des abgebenden Providers angegeben werden. Ist abgebender Provider die Telekom und Vorabstimmungsprozess über WBCI erfolgt, muss die Pseudo-Vertragsnummer angegeben werden.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="anlagen" type="tns:VerbundleistungAnlagenType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Anlagen zum Auftrag</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Pflichtangabe, falls keine Hafttungsfreistellung in den Dienstverträgen vereinbart</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="ansprechpartner" type="tns:AnsprechpartnerType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Ansprechpartnern des Kunden</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="termine" type="tns:VerbundleistungTermineType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben zu Terminen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="preOrderID" type="tns:UUIDType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>ID einer gültigen PreOrder, der Auftrag wird mit den Daten dieser PreOrder vervollständigt. Nur für Produktgruppen, für die eine vorgezogene PreOrder angeboten wird.</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Status der PreOrder: abgeschlossen oder in_Auftragspruefung; Eine PreOrder kann im Rahmen des Gültigkeitszeitraums wiederverwendet werden, wenn ein Vorgängerauftrag in der Auftragsprüfung abgelehnt worden ist. Nur für Produktgruppen, für die eine vorgezogene PreOrder angeboten wird.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="auftragsposition"
                                  type="tns:AuftragspositionVerbundleistungType"
                                  maxOccurs="99">
                        <xsd:annotation>
                           <xsd:documentation>Liste der Auftragspositionen, in der die Produkte / Leistungsmerkmale aufgeführt sind, die im Auftrag bearbeitet werden sollen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:simpleType name="VerschluesselterTextType">
            <xsd:annotation>
               <xsd:documentation>Verschlüsselte Textangaben</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="1024"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="VertragsnummerType">
            <xsd:annotation>
               <xsd:documentation>Bezeichnet den Einzelvertrag, auf den sich der Auftrag bezieht.</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="10"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="VorabstimmungsIdType">
            <xsd:annotation>
               <xsd:documentation>ID der Vorabstimmungsanfrage aus dem Vorabstimmungsprozess</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">Beispiel: DEU.EKP1.V000000013</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="16"/>
               <xsd:maxLength value="21"/>
               <xsd:pattern value="[0-9A-Z]{3}\.[0-9A-Z]{1,6}\.V[0-9A-Z]{9}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="VormieterAnschlussType">
            <xsd:annotation>
               <xsd:documentation>Daten zur Identifikation eines vorhandenen Netzanschlusses (Telefon / Anlagenanschluss)</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Wenn Vormieter angegeben, muss mindestens: Person oder Anschluss oder UFA  aufgeführt werden</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="Person" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Identifikation des Netzanschlusses anhand der Person</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="vorname" type="tns:VornameType" minOccurs="0">
                           <xsd:annotation>
                              <xsd:documentation>Vorname des Vormieters</xsd:documentation>
                           </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="nachname" type="tns:NachnameType">
                           <xsd:annotation>
                              <xsd:documentation>Nachname des Vormieters</xsd:documentation>
                           </xsd:annotation>
                        </xsd:element>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
               <xsd:element name="anschluss" type="tns:AnschlussType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Identifikation des Netzanschlusses anhand der Anschlussrufnummer
					</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="UFA" type="tns:UFAType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Identifikation des Netzanschlusses anhand der UFA-Nummer</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="VornameType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="30"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="XDSLBereitstellungType">
            <xsd:complexContent>
               <xsd:extension base="tns:BereitstellungProduktType">
                  <xsd:sequence>
                     <xsd:element name="vorabstimmungsID"
                                  type="tns:VorabstimmungsIdType"
                                  minOccurs="0"/>
                     <xsd:element name="standortA" type="tns:StandortAType">
                        <xsd:annotation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Es ist die Wohnadresse anzugeben, nicht der Standort des Leitungsabschlusses</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="vormieter" type="tns:VormieterAnschlussType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Daten zur Identifikation eines vorhandenen Netzanschlusses (Telefon, Anlagenanschluss)</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="ansprechpartnerMontage"
                                  type="tns:AnsprechpartnerBaseType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>der Ansprechpartner, mit dem die Montageleistung abzustimmen ist</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="montagehinweis" type="tns:MontagehinweisType" minOccurs="0"/>
                     <xsd:element name="terminReservierungsID"
                                  type="tns:ReservierungsnummerType"
                                  minOccurs="0"/>
                     <xsd:element name="IPTVOnlineKennungAngaben"
                                  type="tns:IPTVOnlineKennungType"
                                  minOccurs="0"/>
                     <xsd:element name="mitnahmeIPTVEinstellungen"
                                  type="tns:ExterneAuftragsnummerType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Externe Auftragsnummer eines Kündigungs- bzw. Bereitstellungsauftrags, wenn die IPTV Einstellungen des Endkunden übernommen werden sollen</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Die externe Auftragsnummer des Kündigungsauftrags ist anzugegeben</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Optionales Feld, nur bei Accessvarianten mit IPTV zugelassen</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes/important">Freigabe der Funktion erfolgt zu einem späteren Zeitpunkt</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="XDSLKuendigungType">
            <xsd:complexContent>
               <xsd:extension base="tns:KuendigungProduktType">
                  <xsd:sequence>
                     <xsd:element name="vorabstimmungsID"
                                  type="tns:VorabstimmungsIdType"
                                  minOccurs="0"/>
                     <xsd:element name="ansprechpartnerMontage"
                                  type="tns:AnsprechpartnerBaseType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>der Ansprechpartner, mit dem die Montageleistung abzustimmen ist</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="montagehinweis" type="tns:MontagehinweisType" minOccurs="0"/>
                     <xsd:element name="mitnahmeIPTVEinstellungen"
                                  type="tns:ExterneAuftragsnummerType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Externe Auftragsnummer eines Kündigungs- bzw. Bereitstellungsauftrags, wenn die IPTV Einstellungen des Endkunden übernommen werden sollen</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Die externe Auftragsnummer des Bereitstellungsauftrags ist anzugegeben</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Optionales Feld, nur bei Accessvarianten mit IPTV zugelassen</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes/important">Freigabe der Funktion erfolgt zu einem späteren Zeitpunkt</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="XDSLLeistungsaenderungType">
            <xsd:complexContent>
               <xsd:extension base="tns:LeistungsaenderungProduktType">
                  <xsd:sequence>
                     <xsd:element name="standortA" type="tns:StandortAType" minOccurs="0"/>
                     <xsd:element name="ansprechpartnerMontage"
                                  type="tns:AnsprechpartnerBaseType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>der Ansprechpartner, mit dem die Montageleistung abzustimmen ist</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="montagehinweis" type="tns:MontagehinweisType" minOccurs="0"/>
                     <xsd:element name="terminReservierungsID"
                                  type="tns:ReservierungsnummerType"
                                  minOccurs="0"/>
                     <xsd:element name="IPTVOnlineKennungAngaben"
                                  type="tns:IPTVOnlineKennungType"
                                  minOccurs="0"/>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="XDSLLeistungsmerkmalAenderungType">
            <xsd:complexContent>
               <xsd:extension base="tns:LeistungsmerkmalAenderungProduktType">
                  <xsd:sequence>
                     <xsd:element name="standortA" type="tns:StandortAType" minOccurs="0"/>
                     <xsd:element name="IPTVBenutzerpinAenderung"
                                  type="tns:BoolDecisionType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Beauftragung der Änderung der IPTV Benutzer-Pin</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Optionales Feld, nur bei Accessvarianten mit IPTV zugelassen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="XDSLProduktgruppenWechselType">
            <xsd:complexContent>
               <xsd:extension base="tns:ProduktgruppenwechselProduktType">
                  <xsd:sequence>
                     <xsd:element name="standortA" type="tns:StandortAType" minOccurs="0"/>
                     <xsd:element name="ansprechpartnerMontage"
                                  type="tns:AnsprechpartnerBaseType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>der Ansprechpartner, mit dem die Montageleistung abzustimmen ist</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="montagehinweis" type="tns:MontagehinweisType" minOccurs="0"/>
                     <xsd:element name="terminReservierungsID"
                                  type="tns:ReservierungsnummerType"
                                  minOccurs="0"/>
                     <xsd:element name="IPTVOnlineKennungAngaben"
                                  type="tns:IPTVOnlineKennungType"
                                  minOccurs="0"/>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="XDSLProviderwechselType">
            <xsd:complexContent>
               <xsd:extension base="tns:ProviderwechselProduktType">
                  <xsd:sequence>
                     <xsd:element name="vorabstimmungsID"
                                  type="tns:VorabstimmungsIdType"
                                  minOccurs="0"/>
                     <xsd:element name="standortA" type="tns:StandortAnbieterWechselErweitertType">
                        <xsd:annotation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Im Dreierprozess werden die Adressdaten aus den Vertragsdaten übernommen, Angaben zur Person, zum Gebäudeteil und Lage TAE_ONT werden berücksichtigt.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="ansprechpartnerMontage"
                                  type="tns:AnsprechpartnerBaseType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>der Ansprechpartner, mit dem die Montageleistung abzustimmen ist</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="montagehinweis" type="tns:MontagehinweisType" minOccurs="0"/>
                     <xsd:element name="terminReservierungsID"
                                  type="tns:ReservierungsnummerType"
                                  minOccurs="0"/>
                     <xsd:element name="IPTVOnlineKennungAngaben"
                                  type="tns:IPTVOnlineKennungType"
                                  minOccurs="0"/>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="XDSLVerbundleistungType">
            <xsd:complexContent>
               <xsd:extension base="tns:VerbundleistungProduktType">
                  <xsd:sequence>
                     <xsd:element name="vorabstimmungsID"
                                  type="tns:VorabstimmungsIdType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Pflichtangabe bei Wechsel von Telekom zu Provider</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="standortA" type="tns:StandortAnbieterWechselErweitertType">
                        <xsd:annotation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Beim Wechsel von Telekom zu Provider werden die Angaben aus der Vorabstimmung übernommen. Angaben zur Person, zum Gebäudeteil und Lage TAE_ONT werden berücksichtigt.</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Im Dreierprozess werden die Adressdaten aus den Vertragsdaten übernommen, Angaben zur Person, zum Gebäudeteil und Lage TAE_ONT werden berücksichtigt.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="ansprechpartnerMontage"
                                  type="tns:AnsprechpartnerBaseType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>der Ansprechpartner, mit dem die Montageleistung abzustimmen ist</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="montagehinweis" type="tns:MontagehinweisType" minOccurs="0"/>
                     <xsd:element name="terminReservierungsID"
                                  type="tns:ReservierungsnummerType"
                                  minOccurs="0"/>
                     <xsd:element name="IPTVOnlineKennungAngaben"
                                  type="tns:IPTVOnlineKennungType"
                                  minOccurs="0"/>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:simpleType name="ZeitfensterType">
            <xsd:annotation>
               <xsd:documentation>Das Zeitfenster konkretisiert den Termin (das Datum) um einen Bereich angegeben als Uhrzeit (von - bis).</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">Ein Zeichen aus folgendem Bereich: 0-9, a-z. Beispiel: b </xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:length value="1"/>
               <xsd:pattern value="[0-9a-z]"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="ZeitschlitzType">
            <xsd:annotation>
               <xsd:documentation>Kanal mit 64 KBit/s auf der 2MBit/s-Leitung</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="2"/>
               <xsd:pattern value="\d{1,2}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="ZeitstempelType">
            <xsd:annotation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
    Format: YYYY-MM-DDTHH:mm:ss.SSSZ, wo T - Trennungszeichen, SSS - Millisekunden, Z - Zeitzone 
	Beispiel: 2009-02-18T18:37:24.483+02:00
			</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:dateTime">
               <xsd:pattern value="\d{4}-.*"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:element name="AEN-LMAE" type="tns:LeistungsmerkmalAenderungType"/>
         <xsd:element name="AUS-BUE" type="tns:BestandsuebersichtType"/>
         <xsd:element name="Anlage" type="tns:AnlageType"/>
         <xsd:element name="ERLM-K" type="tns:MeldungstypERLM-KType"/>
         <xsd:element name="KUE-KD" type="tns:KuendigungType"/>
         <xsd:element name="LAE" type="tns:LeistungsaenderungType"/>
         <xsd:element name="NEU" type="tns:BereitstellungType"/>
         <xsd:element name="PGW" type="tns:ProduktgruppenwechselType"/>
         <xsd:element name="PV" type="tns:ProviderwechselType"/>
         <xsd:element name="RUEM-PV" type="tns:MeldungstypRUEM-PVType"/>
         <xsd:element name="SER-POW" type="tns:PortwechselType"/>
         <xsd:element name="VBL" type="tns:VerbundleistungType"/>
         <xsd:element name="VZM-PV" type="tns:MeldungstypVZM-PVType"/>
         <xsd:element name="auftrag" type="tns:AuftragType"/>
         <xsd:element name="auftragsposition" type="tns:AuftragspositionType"/>
         <xsd:element name="cancelOrder">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="order" type="tns:StornierungType"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="createOrder">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="order" type="tns:AuftragType"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="geschaeftsfall">
            <xsd:annotation>
               <xsd:documentation>Erweiterungspunkt für die konkreten Geschäftsfälle</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:choice>
                  <xsd:element ref="tns:AEN-LMAE"/>
                  <xsd:element ref="tns:AUS-BUE"/>
                  <xsd:element ref="tns:KUE-KD"/>
                  <xsd:element ref="tns:LAE"/>
                  <xsd:element ref="tns:NEU"/>
                  <xsd:element ref="tns:PGW"/>
                  <xsd:element ref="tns:PV"/>
                  <xsd:element ref="tns:SER-POW"/>
                  <xsd:element ref="tns:VBL"/>
               </xsd:choice>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="meldung" type="tns:MeldungType"/>
         <xsd:element name="meldungsposition" type="tns:MeldungspositionType"/>
         <xsd:element name="produktMeldung" type="tns:ProduktTALMeldungType"/>
         <xsd:element name="rescheduleOrder">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="order" type="tns:TerminverschiebungType"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="stornierung" type="tns:StornierungType"/>
         <xsd:element name="terminverschiebung" type="tns:TerminverschiebungType"/>
         <xsd:element name="updateOrder">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="order" type="tns:AuftragstypType"/>
                  <xsd:element name="message">
                     <xsd:complexType>
                        <xsd:choice>
                           <xsd:element ref="tns:ERLM-K"/>
                           <xsd:element ref="tns:RUEM-PV"/>
                        </xsd:choice>
                     </xsd:complexType>
                  </xsd:element>
