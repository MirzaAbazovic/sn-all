<?xml version="1.0" encoding="UTF-8"?>
<!--
GUI-Definitionen fuer einen Anschlussuebernahme-Dialog.
-->
<sf>
    
	<Text name="title">Anschlussübernahme</Text>
	
    <Label name="auftrags.id.old"><text>alter Auftrag:</text></Label>
    <FormattedTextField name="auftrags.id.old">
        <tooltip>ID des alten Auftrags</tooltip>
        <columns>15</columns>
        <format type="number">0</format>
    </FormattedTextField>
	
	<Label name="auftrags.id.new"><text>neuer Auftrag:</text></Label>
    <FormattedTextField name="auftrags.id.new">
        <tooltip>ID des neuen Auftrags</tooltip>
        <format type="number">0</format>
    </FormattedTextField>
    
	<Text name="uebernahme.title">Übernehmen?</Text>
	<Text name="uebernahme.question">Soll die Daten von Auftrag {0} wirklich 
auf den Auftrag {1} übernommen werden?</Text>
	
</sf>