<?xml version="1.0" encoding="UTF-8"?>
<sf>

    <Text name="basic.informations">allgemeine Informationen</Text>

    <Label name="timestamp"><text>Zeitstempel:</text></Label>
    <DateComponent name="timestamp">
        <tooltip>Zeitpunkt, zu dem die Nachricht verschickt bzw. erhalten wurde</tooltip>
        <format type="date">dd.MM.yyyy HH:mm</format>
    </DateComponent>

    <Label name="aufnehmender.ekp"><text>aufnehmender EKP:</text></Label>
    <TextField name="aufnehmender.ekp">
        <tooltip>aufnehmender Carrier</tooltip>
        <columns>15</columns>
    </TextField>

    <Label name="abgebender.ekp"><text>abgebender EKP:</text></Label>
    <TextField name="abgebender.ekp">
        <tooltip>abgebender Carrier</tooltip>
        <columns>15</columns>
    </TextField>

    <Label name="geschaeftsfall"><text>Geschäftsfall:</text></Label>
    <TextField name="geschaeftsfall">
        <tooltip>Geschäftsfall</tooltip>
        <columns>15</columns>
    </TextField>

</sf>