					<documentation>Is 'true ' if the SIM card was successfully activated, else 'false'.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="cancelServiceType">
		<complexContent>
			<extension base="tns:WebServiceRequestType">
				<sequence>
                    <element name="serviceNo" minOccurs="1" maxOccurs="1" type="int">
						<annotation>
							<documentation>The service history key in Taifun (AUFTRAG__NO).</documentation>
						</annotation>
                    </element>
                    <element name="validFrom" minOccurs="1" maxOccurs="1" type="date">
						<annotation>
							<documentation>The service will be cancelled per 'validFrom' date.</documentation>
						</annotation>
					</element>
					<element name="cancelledBy" minOccurs="1" maxOccurs="1" type="types:CancelledByType">
					    <annotation>
							<documentation>The initiator of the cancellation (Customer or Company)</documentation>
						</annotation>
					</element>
					<element name="cancellationReasonNo" minOccurs="1" maxOccurs="1" type="int">
					    <annotation>
							<documentation>Reason of the cancellation. This is the primary key to on the table "KUENDIGUNGSGRUND"</documentation>
						</annotation>
					</element>
					<element name="distributionChannelId" minOccurs="1" maxOccurs="1" type="string">
					    <annotation>
							<documentation>Distribution channel through which the cancellation was initiated.</documentation>
						</annotation>
					</element>
					<element name="businessCaseNo" minOccurs="1" maxOccurs="1" type="int">
					    <annotation>
							<documentation>Business case for the cancellation. This is the primary key to on the table "BUSINESS_CASE4SERVICE"</documentation>
						</annotation>
					</element>
					<element name="portingInformation" minOccurs="0" maxOccurs="1" type="types:DNPortingInformationType">
					    <annotation>
							<documentation>Information for number porting (optional)</documentation>
						</annotation>
					</element>
					<element name="entryDate" type="date" minOccurs="1" maxOccurs="1">
						<annotation>
							<documentation>Eingangsdatum</documentation>
						</annotation></element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="cancelServiceResponseType">
		<sequence>
			<element name="successful" minOccurs="1" maxOccurs="1" type="boolean">
				<annotation>
					<documentation>Is 'true ' if the service was successfully cancelled, else 'false'.</documentation>
				</annotation>
			</element>
			<element name="reclaimPositions" minOccurs="1" maxOccurs="1" type="boolean">
				<annotation>
					<documentation>Is 'true ' if the service has items to be reclaimed, else 'false'.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="undoPlannedServiceCancellationType">
		<complexContent>
			<extension base="tns:WebServiceRequestType">
				<sequence>
                    <element name="serviceNo" minOccurs="1" maxOccurs="1" type="int">
                    	<annotation>
							<documentation>The service history key in Taifun (AUFTRAG__NO).</documentation>
						</annotation>
                    </element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="undoPlannedServiceCancellationResponseType">
		<sequence>
			<element name="successful" minOccurs="1" maxOccurs="1" type="boolean">
				<annotation>
					<documentation>Is 'true ' if the cancellation was successfully reverted, else 'false'.</documentation>
				</annotation>
			</element>
			<element name="reclaimPositions" minOccurs="1" maxOccurs="1" type="boolean">
				<annotation>
					<documentation>Is 'true ' if the service has items to be reclaimed, else 'false'.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	
