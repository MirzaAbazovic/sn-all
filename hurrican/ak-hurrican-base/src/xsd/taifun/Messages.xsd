		</complexContent>
	</complexType>
	<complexType name="GetVersionResponseType">
		<sequence>
			<element name="version" type="string" />
		</sequence>
	</complexType>
	<complexType name="CreateDomainOrderType">
		<complexContent>
			<extension base="tns:WebServiceRequestType">
				<sequence>
					<element name="customerNo" minOccurs="1" maxOccurs="1"
						type="int">
						<annotation>
							<documentation>The taifun customer number of the customer this
								domain belongs to.</documentation>
						</annotation>
					</element>
					<element name="bundleOrderNo" minOccurs="1" maxOccurs="1"
						type="int">
						<annotation>
							<documentation>The taifun order number of the order this domain
								order must be bundled to.</documentation>
						</annotation>
					</element>
					<element name="domainName" minOccurs="1" maxOccurs="1"
						type="string">
						<annotation>
							<documentation>The name of the domain to be created, for example
								'test.de'.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="CreateDomainOrderResponseType">
		<sequence>
			<element name="serviceOrderNo" minOccurs="1" maxOccurs="1"
				type="int">
				<annotation>
					<documentation>The taifun order number of the order this domain
						belongs to.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="ChargeDomainOrderType">
		<complexContent>
			<extension base="tns:WebServiceRequestType">
				<sequence>
					<element name="customerNo" minOccurs="1" maxOccurs="1"
						type="int">
						<annotation>
							<documentation>The taifun customer number of the customer the
								domain order belongs to.</documentation>
						</annotation>
					</element>
					<element name="serviceOrderNumber" minOccurs="1" maxOccurs="1"
						type="int">
						<annotation>
							<documentation>The taifun order number of the domain order that
								must be charged.</documentation>
						</annotation>
					</element>
					<element name="productElementNo" minOccurs="1" maxOccurs="1"
						type="int">
						<annotation>
							<documentation>The Taifun product element number of the product
								element that must be referenced by the new serice order
								position.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ChargeDomainOrderResponseType">
		<sequence>
			<element name="successful" minOccurs="1" maxOccurs="1" type="int">
				<annotation>
					<documentation>Is 'true ' if the domain order was successfully
						charged, else 'false'.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="ActivateDomainType">
		<complexContent>
			<extension base="tns:WebServiceRequestType">
				<sequence>
					<element name="customerNo" minOccurs="1" maxOccurs="1"
						type="int">
						<annotation>
							<documentation>The taifun customer number of the customer this
								domain belongs to.</documentation>
						</annotation>
					</element>
					<element name="domainName" minOccurs="1" maxOccurs="1"
						type="string">
						<annotation>
							<documentation>The name of the domain to be activated, for
								example 'test.de'.</documentation>
						</annotation>
					</element>
					<element name="validFrom" minOccurs="0" maxOccurs="1" type="date">
						<annotation>
							<documentation>This domain will be billable beginning with date
								'validFrom'.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="ActivateDomainResponseType">
		<sequence>
			<element name="successful" minOccurs="1" maxOccurs="1" type="boolean">
				<annotation>
					<documentation>Is 'true ' if the domain was successfully activated,
						else 'false'.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="TransferDomainType">
		<complexContent>
			<extension base="tns:WebServiceRequestType">
				<sequence>
					<element name="oldCustomerNo" minOccurs="1" maxOccurs="1"
						type="int">
						<annotation>
							<documentation>The taifun customer number of the customer this
								domain belongs to and must be transferred from.</documentation>
						</annotation>
					</element>
					<element name="newCustomerNo" minOccurs="1" maxOccurs="1"
						type="int">
						<annotation>
							<documentation>The taifun customer number of the customer this
								domain must be transferred to. This can be the same as
								'oldCustomerNo. In this last case, the domain will be
								transferred within the same customer.</documentation>
						</annotation>
					</element>
					<element name="domainName" minOccurs="1" maxOccurs="1"
						type="string">
						<annotation>
							<documentation>The name of the domain to be transferred, for
								example 'test.de'.</documentation>
						</annotation>
					</element>
					<element name="newBundleOrderNo" minOccurs="1" maxOccurs="1"
						type="int">
						<annotation>
							<documentation>The domain order this domain is going to be
								transferred to, must be bundled with the given
								'newBundleOrderNo' Taifun order number.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="TransferDomainResponseType">
		<sequence>
			<element name="serviceOrderNo" minOccurs="1" maxOccurs="1"
				type="int">
				<annotation>
					<documentation>The taifun order number of the domain order the
						transferred domain belongs to.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="TerminateDomainType">
		<complexContent>
			<extension base="tns:WebServiceRequestType">
				<sequence>
					<element name="customerNo" minOccurs="1" maxOccurs="1"
						type="int">
						<annotation>
							<documentation>The taifun customer number of the customer this
								domain belongs to.</documentation>
						</annotation>
					</element>
					<element name="domainName" minOccurs="1" maxOccurs="1"
						type="string">
						<annotation>
							<documentation>The name of the domain to be terminated, for
								example 'test.de'.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="TerminateDomainResponseType">
		<sequence>
			<element name="successful" minOccurs="1" maxOccurs="1" type="boolean">
				<annotation>
					<documentation>Is 'true ' if the domain was successfully
						terminated, else 'false'.
					</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="activateSimCardType">
		<complexContent>
			<extension base="tns:WebServiceRequestType">
				<sequence>
                    <element name="customerNo" minOccurs="1" maxOccurs="1" type="int"></element>
                    <element name="ICC-ID" minOccurs="1" maxOccurs="1"
						type="string">
						<annotation>
							<documentation>The ICC-ID of the SIM card to be activated.</documentation>
						</annotation>
