	<complexType name="WebServiceRequestType" abstract='true'
		block='restriction'>
		<sequence>
			<element name='authentication' type="types:AuthenticationType" />
			<element name="testMode" type="int">
				<annotation>
					<documentation>This field allows to set the service in a test mode.
						What kind of test modes exist will is defined in the Webservice
						Interface Detail Specification. </documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

	<complexType name="WebServiceRequestOptionsType" abstract='true'
		block='restriction'>
		<complexContent>
			<extension base="tns:WebServiceRequestType">
				<sequence>
					<element name="objectsPerPage" type="int">
						<annotation>
							<documentation>Number of rows or records given back in the
								answer.</documentation>
						</annotation>
					</element>
					<element name="pageNo" type="int">
						<annotation>
							<documentation>Pagenumber starting from 1.</documentation>
						</annotation>
					</element>
					<element name="sortOrder" type="types:SortOrderType">
						<annotation>
							<documentation>Kennzeichnet die Sortierreihenfolge
							</documentation>
						</annotation>
					</element>
					<element name="sortField" type="string">
						<annotation>
							<documentation>The field to apply the sort order to. See the
								webservice specific documentation.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="GetVersionType">
		<complexContent>
			<extension base="tns:WebServiceRequestType">
				<sequence>
				</sequence>
