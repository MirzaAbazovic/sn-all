			<extension base="tns:ProductType">
				<sequence>
					<element name="fromDate" type="date"></element>
					<element name="toDate" type="date"></element>
					<element name="connectionType" type="string"></element>
					<element name="technologyType" type="string"></element>
					<element name="priority" type="int">
						<annotation>
							<documentation>The connection type priority.</documentation>
						</annotation>
					</element>
					<element name="bandwidthRange" type="tns:BandwidthRangeType">
					</element>
					<element name="availability" type="tns:AvailabilityType">
