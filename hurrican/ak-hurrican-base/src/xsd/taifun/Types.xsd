			<element name="productElementNo" type="int" />
			<element name="productElementName" type="string" />
			<element name="fromDate" type="date" />
			<element name="toDate" type="date" />
			<element name="price" type="decimal" />
			<element name="tags" type="tns:TagType" maxOccurs="unbounded"
