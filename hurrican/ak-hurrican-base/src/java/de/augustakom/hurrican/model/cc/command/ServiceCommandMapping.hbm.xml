<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!--
Hibernate-Mapping fuer die Klasse
de.augustakom.hurrican.model.cc.command.ServiceCommandMapping
-->
<hibernate-mapping package="de.augustakom.hurrican.model.cc.command">
    <class name="ServiceCommandMapping" table="T_SERVICE_COMMAND_MAPPING">

        <!-- PrimaryKey -->
        <id name="id" column="ID" unsaved-value="null" type="long">
            <generator class="sequence"><param name="sequence">S_T_SERVICE_COMMAND_MAPPI_0</param></generator>
        </id>

        <version name="version" type="long" unsaved-value="null"/>

        <!-- Sonstige Fields -->
        <property name="commandId" column="COMMAND_ID" type="long" not-null="true"/>
        <property name="refId" column="REF_ID" type="long" not-null="true"/>
        <property name="refClass" column="REF_CLASS" type="string" not-null="true"/>
        <property name="orderNo" column="ORDER_NO" type="integer"/>

    </class>

</hibernate-mapping>