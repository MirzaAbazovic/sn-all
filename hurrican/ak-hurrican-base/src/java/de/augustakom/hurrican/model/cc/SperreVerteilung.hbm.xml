<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!--
Hibernate-Mapping fuer die Klasse 
de.augustakom.hurrican.model.cc.SperreVerteilung
-->

<hibernate-mapping package="de.augustakom.hurrican.model.cc">
	<class name="SperreVerteilung" table="T_SPERRE_VERTEILUNG">
	    
	    <!-- PrimaryKey -->
		<composite-id>
        	<key-property name="produktId" column="PROD_ID" type="long"/>
    		<key-property name="abteilungId" column="ABTEILUNG_ID" type="long"/>
		</composite-id>

	</class>
	
</hibernate-mapping>