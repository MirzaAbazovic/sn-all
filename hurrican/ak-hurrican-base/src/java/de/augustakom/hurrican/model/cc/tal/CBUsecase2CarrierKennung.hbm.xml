<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!--
Hibernate-Mapping fuer die Klasse
de.augustakom.hurrican.model.cc.tal.CBUsecase2CarrierKennung
-->

<hibernate-mapping package="de.augustakom.hurrican.model.cc.tal">
    <class name="CBUsecase2CarrierKennung" table="T_CB_USECASE_2_CARRIER_KNG">

        <cache usage="read-only"/>

        <!-- PrimaryKey -->
        <id name="id" column="ID" unsaved-value="null" type="long">
            <generator class="assigned"/>
        </id>

        <version name="version" type="long" unsaved-value="null"/>

        <!-- Sonstige Fields -->
        <property name="usecaseId" column="USECASE_ID" type="long" not-null="true"/>
        <property name="carrierKennungId" column="CARRIERKENNUNG_ID" type="long" not-null="true"/>

    </class>

</hibernate-mapping>