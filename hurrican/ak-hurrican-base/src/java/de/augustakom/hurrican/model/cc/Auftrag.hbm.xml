<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!-- Hibernate-Mapping fuer die Klasse de.augustakom.hurrican.model.cc.Auftrag -->

<hibernate-mapping package="de.augustakom.hurrican.model.cc"
    auto-import="false">
    <class name="Auftrag" table="T_AUFTRAG">

        <!-- PrimaryKey -->
        <id name="id" column="ID" unsaved-value="null" type="long">
            <generator class="sequence">
                <param name="sequence">S_T_AUFTRAG_0</param>
            </generator>
        </id>

        <version name="version" type="long" unsaved-value="null" />

        <!-- Sonstige Fields -->
        <property name="kundeNo" column="KUNDE__NO" type="long" />

    </class>

</hibernate-mapping>