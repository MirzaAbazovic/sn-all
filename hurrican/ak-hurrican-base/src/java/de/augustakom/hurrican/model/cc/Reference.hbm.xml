<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!--
Hibernate-Mapping fuer die Klasse
de.augustakom.hurrican.model.cc.Reference
-->

<hibernate-mapping package="de.augustakom.hurrican.model.cc">
    <class name="Reference" table="T_REFERENCE">

        <cache usage="nonstrict-read-write"/>

        <!-- PrimaryKey -->
        <id name="id" column="ID" unsaved-value="null" type="long">
            <generator class="assigned"/>
        </id>
        <version name="version" type="long" unsaved-value="null"/>

        <!-- Sonstige Fields -->
        <property name="type" column="TYPE" type="string" not-null="true"/>
        <property name="strValue" column="STR_VALUE" type="string"/>
        <property name="intValue" column="INT_VALUE" type="integer"/>
        <property name="floatValue" column="FLOAT_VALUE" type="float"/>
        <property name="unitId" column="UNIT_ID" type="long"/>
        <property name="guiVisible" column="GUI_VISIBLE" type="boolean"/>
        <property name="orderNo" column="ORDER_NO" type="integer"/>
        <property name="wbciTechnologieCode" column="WBCI_CODE" type="string"/>

    </class>

</hibernate-mapping>