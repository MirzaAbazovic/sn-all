<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!--
Hibernate-Mapping fuer die Klasse
de.augustakom.hurrican.model.cc.HVTBestellung
-->

<hibernate-mapping package="de.augustakom.hurrican.model.cc">
    <class name="HVTBestellHistory" table="T_HVT_BES_HIST">

        <!-- PrimaryKey -->
        <id name="id" column="ID" unsaved-value="null" type="long">
            <generator class="sequence"><param name="sequence">S_T_HVT_BES_HIST_0</param></generator>
        </id>
        <version name="version" type="long" unsaved-value="null"/>

        <!-- Sonstige Fields -->
        <property name="bestellId" column="BEST_ID" type="long" not-null="true"/>
        <property name="leiste" column="LEISTE" type="string"/>
        <property name="anzahl" column="ANZAHL" type="integer"/>
        <property name="bearbeiter" column="BEARBEITER" type="string"/>
        <property name="datum" column="DATUM" type="date"/>

    </class>

</hibernate-mapping>