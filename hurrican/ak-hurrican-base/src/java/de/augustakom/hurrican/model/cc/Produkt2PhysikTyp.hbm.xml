        </id>
        <version name="version" type="long" unsaved-value="null"/>

        <!-- Sonstige Fields -->
        <property name="produktId" column="PROD_ID" type="long"/>
        <property name="physikTypId" column="PHYSIKTYP" type="long"/>
        <property name="physikTypAdditionalId" column="PHYSIKTYP_ADDITIONAL" type="long"/>
        <property name="virtuell" column="VIRTUELL" type="boolean"/>
        <property name="parentPhysikTypId" column="PARENTPHYSIKTYP_ID" type="long"/>
        <property name="priority" column="PRIORITY" type="long"/>
        <property name="useInRangMatrix" column="USE_IN_RANG_MATRIX" type="boolean"/>

    </class>

</hibernate-mapping>
