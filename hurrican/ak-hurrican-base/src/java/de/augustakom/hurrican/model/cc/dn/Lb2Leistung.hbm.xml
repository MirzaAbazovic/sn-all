        <version name="version" type="long" unsaved-value="null"/>

        <!-- Sonstige Fields -->
        <property name="lbId" column="LB_ID" type="long"/>
        <property name="leistungId" column="LEISTUNG_ID" type="long"/>
        <property name="oeNo" column="OE__NO" type="long"/>
        <property name="standard" column="STANDARD" type="boolean"/>
        <property name="gueltigVon" column="GUELTIG_VON" type="date"/>
        <property name="gueltigBis" column="GUELTIG_BIS" type="date"/>
        <property name="verwendenVon" column="VERWENDEN_VON" type="date"/>
        <property name="verwendenBis" column="VERWENDEN_BIS" type="date"/>
        <property name="defParamValue" column="PARAM_VALUE" type="string">

        </property>

    </class>

</hibernate-mapping>