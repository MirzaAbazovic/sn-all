<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!--
Hibernate-Mapping fuer die Klasse 
de.augustakom.hurrican.model.cc.Produkt2Schnittstelle
-->

<hibernate-mapping package="de.augustakom.hurrican.model.cc">
	<class name="Produkt2Schnittstelle" table="T_PRODUKT_2_SCHNITTSTELLE">
	    
	    <!-- PrimaryKey -->
		<composite-id>
			<key-property name="produktId" column="PROD_ID" type="long"/>
			<key-property name="schnittstelleId" column="SCHNITTSTELLE_ID" type="long"/>
		</composite-id>

	</class>
	
</hibernate-mapping>