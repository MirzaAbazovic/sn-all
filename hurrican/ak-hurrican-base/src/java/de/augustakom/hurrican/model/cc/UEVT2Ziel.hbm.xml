<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!--
Hibernate-Mapping fuer die Klasse
de.augustakom.hurrican.model.cc.UEVT2Ziel
-->

<hibernate-mapping package="de.augustakom.hurrican.model.cc">
    <class name="UEVT2Ziel" table="T_UEVT_2_ZIEL">

        <!-- PrimaryKey -->
        <id name="id" column="ID" unsaved-value="null" type="long">
            <generator class="sequence"><param name="sequence">S_T_UEVT_2_ZIEL_0</param></generator>
        </id>
        <version name="version" type="long" unsaved-value="null"/>

        <!-- Sonstige Fields -->
        <property name="produktId" column="PROD_ID" type="long"/>
        <property name="uevtId" column="UEVT_ID" type="long"/>
        <property name="hvtStandortIdZiel" column="HVT_STANDORT_ID_ZIEL" type="long"/>

    </class>

</hibernate-mapping>