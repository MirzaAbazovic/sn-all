<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!--
Hibernate-Mapping fuer die Klasse 
de.augustakom.hurrican.model.exmodules.tal.TALVorfall
-->

<hibernate-mapping package="de.augustakom.hurrican.model.exmodules.tal">
	<class name="TALVorfall" table="TTALBESTELLUNG_VORFAELLE">
	    
		<cache usage="read-only"/>
		
	    <!-- PrimaryKey -->
		<id name="id" column="TBV_ID" unsaved-value="null" type="long">
			<generator class="assigned"/>
		</id>
		
		<!-- Sonstige Fields -->
		<property name="name" column="TBV_NAME" type="string"/>
		<property name="typ" column="TBV_TYP" type="string"/>
		<property name="header" column="TBV_HEADER" type="string"/>
		<property name="meldungstyp" column="TBV_MELDUNGSTYP" type="string"/>
		<property name="vorfall" column="TBV_VORFALL" type="string"/>
		
	</class>
	
</hibernate-mapping>
