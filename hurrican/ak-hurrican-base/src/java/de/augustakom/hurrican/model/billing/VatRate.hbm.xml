<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!--
Hibernate-Mapping fuer die Klasse
de.augustakom.hurrican.model.billing.VatRate
-->

<hibernate-mapping package="de.augustakom.hurrican.model.billing">
	<class name="VatRate" table="VAT_RATE">

	    <!-- PrimaryKey -->
		<id name="vatRateNo" column="VAT_RATE_NO" unsaved-value="null" type="long">
			<generator class="assigned"/>
		</id>

		<!-- Sonstige Fields -->
        <property name="validFrom" column="VALID_FROM" type="date"/>
        <property name="validTo" column="VALID_TO" type="date"/>
        <property name="vatCodeId" column="VAT_CODE_ID" type="string"/>
		<property name="vatRate" column="VAT_RATE" type="float"/>

	</class>

</hibernate-mapping>