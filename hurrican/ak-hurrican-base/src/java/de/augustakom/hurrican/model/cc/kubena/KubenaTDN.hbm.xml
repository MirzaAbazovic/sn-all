<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!--
Hibernate-Mapping fuer die Klasse
de.augustakom.hurrican.model.cc.kubena.KubenaVbz
-->

<hibernate-mapping package="de.augustakom.hurrican.model.cc.kubena">
    <class name="KubenaVbz" table="T_KUBENA_TDN">

        <!-- PrimaryKey -->
        <id name="id" column="ID" unsaved-value="null" type="long">
            <generator class="sequence"><param name="sequence">S_T_KUBENA_TDN_0</param></generator>
        </id>

        <version name="version" type="long" unsaved-value="null"/>

        <!-- Sonstige Fields -->
        <property name="kubenaId" column="KUBENA_ID" type="long"/>
        <property name="vbz" column="TDN" type="string"/>
        <property name="input" column="INPUT" type="string"/>
        <property name="vorhanden" column="VORH" type="boolean"/>

    </class>

</hibernate-mapping>