<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!--
Hibernate-Mapping fuer die Klasse
de.augustakom.hurrican.model.cc.kubena.KubenaHVT
-->

<hibernate-mapping package="de.augustakom.hurrican.model.cc.kubena">
    <class name="KubenaHVT" table="T_KUBENA_HVT">

        <!-- PrimaryKey -->
        <id name="id" column="ID" unsaved-value="null" type="long">
            <generator class="sequence"><param name="sequence">S_T_KUBENA_HVT_0</param></generator>
        </id>

        <version name="version" type="long" unsaved-value="null"/>

        <!-- Sonstige Fields -->
        <property name="kubenaId" column="KUBENA_ID" type="long"/>
        <property name="hvtIdStandort" column="HVT_ID_STANDORT" type="long"/>

    </class>

</hibernate-mapping>