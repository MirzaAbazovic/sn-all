                        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"
       default-lazy-init="true">

    <context:annotation-config />

    <!-- ************************ WS Templates *************************** -->
    <!-- Definition des zu verwendenden Marshallers -->
    <bean id="monline.marshaller" class="org.springframework.oxm.xmlbeans.XmlBeansMarshaller"/>

    <!-- Definition des zu verwendenden Message Senders
         Hier koennen Parameter der HttpUrlConnection uebersteuert werden
         Beispiel: connectionTimeout
     -->
    <bean id="hurrican.messageSender" class="de.augustakom.common.tools.ws.MnetHttpUrlConnectionMessageSender"
          scope="prototype">
        <property name="connectionTimeout">
            <!-- Der Wert 10000 fuert zu einem Timeout von 20 Sekunden, weil vermutlich der Verbindungsaufbau
                 insgesamt 2 mal durchgefuehrt wird.
            -->
            <value>10000</value>
        </property>
        <property name="httpClient" ref="httpClient"/>
    </bean>

    <!-- MessageFactory fuer den WebService -->
    <bean id="monline.wsMessageFactory" class="org.springframework.ws.soap.saaj.SaajSoapMessageFactory">
        <property name="soapVersion" ref="org.springframework.ws.soap.SoapVersion.SOAP_11"/>
    </bean>

    <!-- WebService-Template Definition inkl. Un-/Marshaller -->
    <bean id="monlineWSTemplate" class="de.augustakom.common.tools.ws.MnetWebServiceTemplate">
        <property name="marshaller" ref="monline.marshaller" />
        <property name="unmarshaller" ref="monline.marshaller" />
        <property name="messageFactory" ref="monline.wsMessageFactory" />
        <property name="messageSender" ref="hurrican.messageSender" />
    </bean>

