<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-lazy-init="true">
    <bean name="/cc/PhysikService" class="org.springframework.remoting.httpinvoker.HttpInvokerServiceExporter">
        <property name="service" ref="de.augustakom.hurrican.service.cc.PhysikService"/>
        <property name="serviceInterface" value="de.augustakom.hurrican.service.cc.PhysikService"/>
    </bean>
    <bean name="/cc/RangierungsService" class="org.springframework.remoting.httpinvoker.HttpInvokerServiceExporter">
        <property name="service" ref="de.augustakom.hurrican.service.cc.RangierungsService"/>
        <property name="serviceInterface" value="de.augustakom.hurrican.service.cc.RangierungsService"/>
    </bean>
    <bean name="/cc/EndstellenService" class="org.springframework.remoting.httpinvoker.HttpInvokerServiceExporter">
        <property name="service" ref="de.augustakom.hurrican.service.cc.EndstellenService"/>
        <property name="serviceInterface" value="de.augustakom.hurrican.service.cc.EndstellenService"/>
    </bean>

</beans>
