    <resourceSpec>
        <id>dpu-1</id>
        <inventory>command</inventory>
        <name>dpu</name>
        <type>PHYSICAL</type>
        <description>Distribution Point Unit für G.Fast an GPON (Netzelement)</description>

        <characteristic>
            <name>bezeichnung</name>
            <type>string</type>
            <!-- "ID" in Tabelle "GERAETE" z.B. DPU-404263 -->
            <description>Bezeichnung der Netzelementes</description>
        </characteristic>
        <characteristic>
            <name>hersteller</name>
            <type>string</type>
            <!-- "HERSTELLER" in Tabelle "GERAETESTAMM" z.B. Huawei -->
            <description>Name des Herstellers</description>
        </characteristic>
        <characteristic>
            <name>seriennummer</name>
            <type>string</type>
            <!-- "SERIENNR" in Tabelle "GERAETE" z.B. ABC1234DEF) -->
            <description>Seriennummer des Netzelementes</description>
            <cardinality>
                <min>0</min>
            </cardinality>
        </characteristic>
        <characteristic>
            <name>modellnummer</name>
            <type>string</type>
            <!-- "TYPE" in Tabelle "GERAETESTAMM" -->
            <description>Modellbezeichnung des Netzelementes: MA5811S_AE04, MA5811S_AE08, MA5811S_DE16</description>
        </characteristic>
        <characteristic>
            <name>olt</name>
            <type>string</type>
            <!-- "ID" in Tabelle "SERVICE_DETAIL" z.B. OLT-400028-1-1-10-1-017 -->
            <description>Bezeichnung der OLT, an der das Netzelement angeschlossen ist</description>
        </characteristic>
        <characteristic>
            <name>oltRack</name>
            <type>long</type>
            <description>Rack der OLT</description>
        </characteristic>
        <characteristic>
            <name>oltSubrack</name>
            <type>long</type>
            <description>Subrack der OLT (bei Huawei leer)</description>
            <cardinality>
                <min>0</min>
            </cardinality>
        </characteristic>
        <characteristic>
            <name>oltSlot</name>
            <type>long</type>
            <description>Slot-Bezeichnung</description>
        </characteristic>
        <characteristic>
            <name>oltPort</name>
            <type>long</type>
            <description>Port-Bezeichnung</description>
        </characteristic>
        <characteristic>
            <name>oltGponId</name>
            <type>long</type>
            <description>G.PON-ID</description>
        </characteristic>
        <characteristic>
            <name>standort</name>
            <type>string</type>
            <!-- "BEZEICHNUNG" in Tabelle "STOCKWERK" z.B. MUC-AACHE-011 -->
            <description>Standortbezeichnung des Gebäudes</description>
        </characteristic>
        <characteristic>
            <name>raumbezeichung</name>
            <type>string</type>
            <!-- "NAME" in Tabelle "RAUM" z.B. Kellergang -->
            <description>Raum-Nummer bzw. –Name</description>
        </characteristic>
        <characteristic>
            <name>freigabe</name>
            <type>dateTime</type>
            <description>Zeitstempel Produktionsfreigabe</description>
        </characteristic>
        <characteristic>
            <!-- wird durch Hurrican ignoriert -->
            <name>installationsstatus</name>
            <type>string</type>
            <description>Planungsstatus der Resource</description>
            <restrictions>
                <value>GEPLANT</value>
                <value>IST</value>
                <value>GELOESCHT</value>
            </restrictions>
        </characteristic>
        <characteristic>
            <name>reversePower</name>
            <type>string</type>
            <description>Stromversorgung durch den Kunden</description>
            <restrictions>
                <value>TRUE</value>
                <value>FALSE</value>
            </restrictions>
        </characteristic>
    </resourceSpec>

