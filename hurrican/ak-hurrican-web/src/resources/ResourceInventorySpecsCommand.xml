    <resourceSpec>
        <id>ont-1</id>
        <inventory>command</inventory>
        <name>ONT</name>
        <type>PHYSICAL</type>
        <description>optical network terminal (Netzelement)</description>
        <!-- char. "Bezeichung" entfällt an dieser Stelle, die Information wird im Feld "NAME" der Resource selbst übertragen -->
        <characteristic>
            <name>hersteller</name>
            <type>string</type>
            <description>Name des Herstellers</description>
        </characteristic>
        <characteristic>
            <name>seriennummer</name>
            <type>string</type>
            <description>Seriennummer des Netzelementes</description>
            <cardinality>
                <min>0</min>
            </cardinality>
        </characteristic>
        <characteristic>
            <name>modellnummer</name>
            <type>string</type>
            <description>Modellbezeichnung des Netzelementes</description>
        </characteristic>
        <characteristic>
            <name>olt</name>
            <type>string</type>
            <description>Bezeichnung der OLT, an der das Netzelement angeschlossen ist</description>
        </characteristic>
        <characteristic>
            <name>oltRack</name>
            <type>long</type>
            <description>Rack der OLT</description>
        </characteristic>
        <characteristic>
            <name>oltSubrack</name>
            <type>long</type>
            <description>Subrack der OLT (bei Huawei leer)</description>
            <cardinality>
                <min>0</min>
            </cardinality>
        </characteristic>
        <characteristic>
            <name>oltSlot</name>
            <type>long</type>
            <description>Slot-Bezeichnung</description>
        </characteristic>
        <characteristic>
            <name>oltPort</name>
            <type>long</type>
            <description>Port-Bezeichnung</description>
        </characteristic>
        <characteristic>
            <name>oltGponId</name>
            <type>long</type>
            <description>G.PON-ID</description>
        </characteristic>
        <characteristic>
            <name>standort</name>
            <type>string</type>
            <description>Standortbezeichnung des Gebäudes</description>
        </characteristic>
        <characteristic>
            <name>raumbezeichung</name>
            <type>string</type>
            <description>Raum-Nummer bzw. –Name</description>
        </characteristic>
        <characteristic>
            <name>freigabe</name>
            <type>dateTime</type>
            <description>Zeitstempel Produktionsfreigabe</description>
        </characteristic>
        <characteristic>
            <!-- nur GELOESCHT wird von Hurrican ausgewertet -->
            <name>installationsstatus</name>
            <type>string</type>
            <description>Planungsstatus der Resource</description>
            <restrictions>
                <value>GEPLANT</value>
                <value>IST</value>
                <value>GELOESCHT</value>
            </restrictions>
        </characteristic>
    </resourceSpec>

    <resourceSpec>
        <id>ontport-1</id>
