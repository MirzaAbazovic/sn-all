<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.mnet.hurrican.scheduler.model">
    <class name="JobError" table="JOB_ERROR">

        <!-- PrimaryKey -->
        <id name="id" column="ID" unsaved-value="null" type="long">
            <generator class="sequence"><param name="sequence">S_JOB_ERROR_0</param></generator>
        </id>

        <!-- Sonstige Fields -->
        <property name="jobId" column="JOB_ID" type="long" not-null="true"/>
        <property name="errorLevel" column="ERROR_LEVEL" type="string"/>
        <property name="errorMessage" column="ERROR_MSG" type="string"/>

    </class>
</hibernate-mapping>