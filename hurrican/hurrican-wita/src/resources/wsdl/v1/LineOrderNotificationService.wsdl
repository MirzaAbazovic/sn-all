<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:tns="http://www.mnet.de/esb/cdm/SupplierPartner/LineOrderNotificationService/v1"
                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                  xmlns:jms="http://www.tibco.com/namespaces/ws/2004/soap/binding/JMS"
                  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
                  name="LineOrderNotificationService"
                  targetNamespace="http://www.mnet.de/esb/cdm/SupplierPartner/LineOrderNotificationService/v1">
   <wsdl:documentation>Version 1.2</wsdl:documentation>
   <wsdl:types>
      <xsd:schema xmlns="http://www.mnet.de/esb/cdm/SupplierPartner/LineOrderNotificationService/v1"
                  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                  targetNamespace="http://www.mnet.de/esb/cdm/SupplierPartner/LineOrderNotificationService/v1"
                  version="1.2"
                  elementFormDefault="qualified"
                  attributeFormDefault="unqualified">
         <xsd:simpleType name="AbfragestelleType">
            <xsd:annotation>
               <xsd:documentation>Nummer der Abfragestelle, maximal 6 Ziffern</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">Beispiel: 0</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="6"/>
               <xsd:pattern value="\d{1,6}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="AbgebenderProviderMitAbgabedatumType">
            <xsd:complexContent>
               <xsd:extension base="tns:AbgebenderProviderType">
                  <xsd:sequence>
                     <xsd:element name="abgabedatum" type="xsd:date" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Datum, an dem der Anschluss von Provider abgegeben wird</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AbgebenderProviderType">
            <xsd:sequence>
               <xsd:element name="providername" type="tns:ProvidernameType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Name des abgebenden Providers</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="zustimmungProviderwechsel" type="tns:BoolDecisionType">
                  <xsd:annotation>
                     <xsd:documentation>Kennzeichner, ob der abgebende Provider dem Wechsel zustimmt</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="antwortcode" type="tns:MeldungscodeType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Kurzbezeichnung für die Ablehnung/Zustimmung zum Providerwechsel</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Nur zulässige Antwortcode laut Spezifikation Anbieterwechsel zu verwenden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="antworttext" type="tns:MeldungstextType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Begründung, warum der Wechsel nicht durchgeführt werden kann</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Antworttext ist für zugehörigen Code in der Spezifikation Anbieterwechsel geführt.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="AenderungskennzeichenType">
            <xsd:annotation>
               <xsd:documentation>spezifiziert einen Auftrag als Standard, Storno oder Terminverschiebung</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="30"/>
               <xsd:enumeration value="Standard">
                  <xsd:annotation>
                     <xsd:documentation>Kennzeichnung eines Standardauftrages</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Storno">
                  <xsd:annotation>
                     <xsd:documentation>Kennzeichnung der Stornierung des Auftrages mit den korrespondierenden Auftragsdaten</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Terminverschiebung">
                  <xsd:annotation>
                     <xsd:documentation>Kennzeichnung eines Auftrages zur Terminverschiebung des Auftrages mit den korrespondierenden Auftragsdaten</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="AktionscodeType">
            <xsd:annotation>
               <xsd:documentation>(Zugang, Wegfall)</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:length value="1"/>
               <xsd:enumeration value="A">
                  <xsd:annotation>
                     <xsd:documentation>Änderung</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="W">
                  <xsd:annotation>
                     <xsd:documentation>Wegfall</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Z">
                  <xsd:annotation>
                     <xsd:documentation>Zugang</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="AngabenZurLeitungABMType">
            <xsd:complexContent>
               <xsd:extension base="tns:AngabenZurLeitungType">
                  <xsd:sequence>
                     <xsd:element name="leitungsabschnitt"
                                  type="tns:LeitungsabschnittType"
                                  minOccurs="0"
                                  maxOccurs="99"/>
                     <xsd:element name="maxBruttoBitrate" type="tns:BitrateType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Pflichtangabe, wenn beauftragtes Übertragungsverfahren: H16</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AngabenZurLeitungType">
            <xsd:complexContent>
               <xsd:extension base="tns:BasisAngabenZurLeitungType">
                  <xsd:sequence>
                     <xsd:element name="schleifenwiderstand"
                                  type="tns:LeitungswiderstandType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Gesamt-Widerstand (Einheit: Ohm) der Schleife</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AnlageMitTypType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:AnlageType">
                  <xsd:sequence>
                     <xsd:element name="anlagentyp" type="tns:AnlagentypType">
                        <xsd:annotation>
                           <xsd:documentation>Gibt die Art der Anlage an</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AnlageType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="dateiname">
                  <xsd:annotation>
                     <xsd:documentation>der Dateiname zum File einschließlich der Erweiterung (Datei-Typ)</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="tns:ErlaubterZeichenBereichType">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="255"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="dateityp" type="tns:DokumenttypType">
                  <xsd:annotation>
                     <xsd:documentation>der Datentyp des Dokuments (Text, PDF, ...) im Mime-Type-Format</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="beschreibung" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Freitext zur Beschreibung des Inhaltes der Anlage</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="tns:ErlaubterZeichenBereichMehrzeiligType">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="160"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="inhalt" type="xsd:base64Binary">
                  <xsd:annotation>
                     <xsd:documentation>der eigentliche Inhalt der Datei (binärer Dateninhalt)</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">zulässige Dateigröße: max. 1 MB pro Anlage, max. 4 MB gesamt</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="AnlagentypMeldungType">
            <xsd:annotation>
               <xsd:documentation>Lageplan, Sonstige</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:enumeration value="Kuendigungsschreiben">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Lageplan">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Kundenauftrag">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Sonstige">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="AnlagentypType">
            <xsd:annotation>
               <xsd:documentation>Lageplan, Kündigungsschreiben, Kundenauftrag, ..., Sonstige</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="30"/>
               <xsd:enumeration value="Kuendigungsschreiben">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Lageplan">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Kundenauftrag">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Portierungsanzeige">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Kuendigung abgebender Provider">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="letzte Telekom-Rechnung">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Sonstige">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="AnredeFirmaType">
            <xsd:annotation>
               <xsd:documentation>die Anrede zur Firma (deutsch: Firma)</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="1"/>
               <xsd:enumeration value="4">
                  <xsd:annotation>
                     <xsd:documentation>Firma</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="9">
                  <xsd:annotation>
                     <xsd:documentation>keine Anrede</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="AnredePersonType">
            <xsd:annotation>
               <xsd:documentation>die Anrede zur Person (deutsch: Frau/Herr)</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="1"/>
               <xsd:enumeration value="1">
                  <xsd:annotation>
                     <xsd:documentation>Herr</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="2">
                  <xsd:annotation>
                     <xsd:documentation>Frau</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="9">
                  <xsd:annotation>
                     <xsd:documentation>keine Anrede</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="AnschlussType">
            <xsd:annotation>
               <xsd:documentation>Der Typ Anschluss ermöglicht die Identifikation eines Anschlusses</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="ONKZ" type="tns:OrtsnetzkennzahlType">
                  <xsd:annotation>
                     <xsd:documentation>Ortsnetzkennzahl des Anschlusses</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="rufnummer" type="tns:RufnummerType">
                  <xsd:annotation>
                     <xsd:documentation>Rufnummer des Anschlusses</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="AnschlussartType">
            <xsd:annotation>
               <xsd:documentation>Übertragungseinrichtung am ISIS/OPAL-Netz</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="10"/>
               <xsd:enumeration value="Analog"/>
               <xsd:enumeration value="ISDN-Basis"/>
               <xsd:enumeration value="PMX"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="AnschlussbereichType">
            <xsd:annotation>
               <xsd:documentation>Der Anschlussbereich wird mit maximal 4 Ziffern spezifiziert.</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="4"/>
               <xsd:pattern value="\d{1,4}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="AnsprechpartnerBaseType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:PersonType">
                  <xsd:sequence>
                     <xsd:element name="telefonnummer" type="tns:TelefonnummerType">
                        <xsd:annotation>
                           <xsd:documentation>Telefonnummer des Ansprechpartners</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="emailadresse" type="tns:EmailadresseType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>E-mail-Adresse des Ansprechpartners</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AnsprechpartnerMitEmailType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:PersonType">
                  <xsd:sequence>
                     <xsd:element name="telefonnummer" type="tns:TelefonnummerType">
                        <xsd:annotation>
                           <xsd:documentation>Telefonnummer des Ansprechpartners</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="emailadresse" type="tns:EmailadresseType">
                        <xsd:annotation>
                           <xsd:documentation>E-mail-Adresse des Ansprechpartners</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AnsprechpartnerMitRolleType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:AnsprechpartnerBaseType">
                  <xsd:sequence>
                     <xsd:element name="rolle" type="tns:AnsprechpartnerRolleType">
                        <xsd:annotation>
                           <xsd:documentation>Ansprechpartner den Kunden für vertriebliche, technische Nachfragen
                           </xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:simpleType name="AnsprechpartnerRolleType">
            <xsd:annotation>
               <xsd:documentation>Auftragsmanagement, Technik, ...</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="20"/>
               <xsd:enumeration value="Auftragsmanagement">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Technik">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="AufnehmenderProviderABMType">
            <xsd:complexContent>
               <xsd:extension base="tns:AufnehmenderProviderType">
                  <xsd:sequence>
                     <xsd:element name="uebernahmeDatumVerbindlich" type="xsd:date">
                        <xsd:annotation>
                           <xsd:documentation>Datum, zu dem Providerwechsel verbindlich durchgeführt wird</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Das Datum ist identisch dem VLT der ABM. Es kann vom Abgabedatum aus vertraglichen Gründen (z.B. regionaler Feiertag) oder betrieblichen Gründen abweichen</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AufnehmenderProviderAKMType">
            <xsd:complexContent>
               <xsd:extension base="tns:AufnehmenderProviderType">
                  <xsd:sequence>
                     <xsd:element name="uebernahmeDatumGeplant" type="xsd:date">
                        <xsd:annotation>
                           <xsd:documentation>Datum, zu dem der Providerwechsel voraussichtlich durchgeführt wird</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Das Datum kann vom KWT des Auftrags des TNBauf abweichen wenn KWT vertraglich nicht zulässig ist</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="antwortfrist" type="xsd:date">
                        <xsd:annotation>
                           <xsd:documentation>Datum, zu dem die Antwort zur AKM-PV spätestens erwartet wird</xsd:documentation>
                           <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Die Antwortfrist endet zum angegebenen Datum um 24:00 Uhr. Anschließend wird automatisch der Folgeprozesschritt ausgeführt</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AufnehmenderProviderType" abstract="true">
            <xsd:sequence>
               <xsd:element name="providernameAufnehmend" type="tns:ProvidernameType">
                  <xsd:annotation>
                     <xsd:documentation>Name des aufnehmenden Providers</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="AuftragskennerType">
            <xsd:annotation>
               <xsd:documentation>setzt sich zusammen aus Auftragsklammer und Anzahl der Aufträge</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="auftragsklammer" type="tns:AuftragsklammerType">
                  <xsd:annotation>
                     <xsd:documentation>kennzeichnet zusammengehörige Aufträge</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Der Wert der Auftragsklammer darf nur einmalig verwendet werden. Auch bei einem zweiten Versand muss ein neuer Wert vergeben werden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="anzahlAuftraege">
                  <xsd:annotation>
                     <xsd:documentation>gibt die Anzahl der Aufträge an, die zu einer Auftragsklammer gehören</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:int">
                        <xsd:pattern value="\d{1,3}"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="AuftragsklammerType">
            <xsd:annotation>
               <xsd:documentation>kennzeichnet zusammengehörige Aufträge</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="10"/>
               <xsd:pattern value="\d{1,10}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="AuftragspositionsnummerType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:restriction base="xsd:int"/>
         </xsd:simpleType>
         <xsd:complexType name="AuftragstypType">
            <xsd:sequence>
               <xsd:element name="geschaeftsfall" type="tns:GeschaeftsfallEnumType"/>
               <xsd:element name="aenderungsKennzeichen" type="tns:AenderungskennzeichenType"/>
               <xsd:element name="geschaeftsfallart" type="tns:GeschaeftsfallArtType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="BasisAngabenZurLeitungType">
            <xsd:sequence>
               <xsd:element name="leitungsbezeichnung" type="tns:LeitungsbezeichnungType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="BitrateType">
            <xsd:annotation>
               <xsd:documentation>Max. Bruttobitrate in Kbit/s</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">Beispiel: 16000</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="6"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="BoolDecisionType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:length value="1"/>
               <xsd:enumeration value="J"/>
               <xsd:enumeration value="N"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="BuchungskontonummerType">
            <xsd:annotation>
               <xsd:documentation>die Buchungskontonummer identifiziert den Debitor wird von der Telekom zur Fakturierung verwendet.</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="10"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="DokumenttypType">
            <xsd:annotation>
               <xsd:documentation>zulässige Dateiformate</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:enumeration value="image/tiff">
                  <xsd:annotation>
                     <xsd:documentation>Der MIME-Typ für *.tif - Dateien</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="image/jpeg">
                  <xsd:annotation>
                     <xsd:documentation>Der MIME-Typ für *.jpg - Dateien</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="application/msword">
                  <xsd:annotation>
                     <xsd:documentation>Der MIME-Typ für *.doc - Dateien</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="application/excel">
                  <xsd:annotation>
                     <xsd:documentation>Der MIME-Typ für *.xls - Dateien</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="application/pdf">
                  <xsd:annotation>
                     <xsd:documentation>Der MIME-Typ für *.pdf - Dateien</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="DoppeladerBelegtType">
            <xsd:sequence>
               <xsd:element name="leitungsbezeichnung"
                            type="tns:LeitungsbezeichnungType"
                            maxOccurs="2"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="DoppeladerType">
            <xsd:annotation>
               <xsd:documentation>Nummer der Doppelader</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Einstellige Angaben werden immer mit führender 0 ergänzt</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="2"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="DurchwahlanlageBestandType">
            <xsd:annotation>
               <xsd:documentation>Anschlussdaten mit der Liste von allen Rufnummernblöcken des Bestandes eines Durchwahlanlage-Anschlusses</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="onkzDurchwahlAbfragestelle"
                            type="tns:OnkzDurchwahlAbfragestelleType"/>
               <xsd:element name="rufnummernbloecke"
                            type="tns:RufnummernblockType"
                            maxOccurs="5">
                  <xsd:annotation>
                     <xsd:documentation>Auflistung von allen Rufnummernblöcken des Bestandes eines Durchwahlanlagenanschlusses</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="DurchwahlnummerType">
            <xsd:annotation>
               <xsd:documentation>Durchwahlnummer des Anlageanschlüsses, maximal 8 Ziffern</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">Beispiel: 1234</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="8"/>
               <xsd:pattern value="\d{1,8}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="EmailadresseType">
            <xsd:annotation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
                  Format: 'local-part'@'domain'. Nur zulässige E-Mail-Adressformate möglich.
                  Beispiel: Heribert.Mustermann@telekom.de
               </xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:maxLength value="320"/>
               <xsd:whiteSpace value="collapse"/>
               <xsd:pattern value=".+@(.+\.)+\w+"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="EndverschlussType">
            <xsd:annotation>
               <xsd:documentation>Nummer des Endverschlusses</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Einstellige Angaben werden immer mit führender 0 ergänzt</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="2"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="ErlaubterZeichenBereichMehrzeiligType">
            <xsd:annotation>
               <xsd:documentation>
                  Einschränkung von zugelassenen UTF-8 Zeichen für die mehrzeiligen alphanumerischen Datenwerte.
                  Folgende Zeichen sind erlaubt: Tabulator, LineFeed, CarriageReturn, Leerzeichen, 0-9, a-z, A-Z, !"#$%&amp;'()*+,-./:;&lt;=&gt;?@[\]^_{}§°±²³ÀÁÂÃÄÅÆÇÈÉÊËÌÍÎÏÐÑÒÓÔÕÖØÙÚÛÜÝÞßàáâãäåæçèéêëìíîïðñòóôõöøùúûüýþÿ
               </xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:pattern value="[&#x9;&#xA;&#xD; -_a-{}&#x7f;§°-³À-ÖØ-öø-ÿ]*"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="ErlaubterZeichenBereichType">
            <xsd:annotation>
               <xsd:documentation>
                  Einschränkung von zugelassenen UTF-8 Zeichen für die alphanumerischen Datenwerte. Folgende Zeichen
                  sind erlaubt: Tabulator, Leerzeichen, 0-9, a-z, A-Z, !"#$%&amp;'()*+,-./:;&lt;=&gt;?@[\]^_{}§°±²³ÀÁÂÃÄÅÆÇÈÉÊËÌÍÎÏÐÑÒÓÔÕÖØÙÚÛÜÝÞßàáâãäåæçèéêëìíîïðñòóôõöøùúûüýþÿ
               </xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:pattern value="[&#x9; -_a-{}&#x7f;§°-³À-ÖØ-öø-ÿ]*"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="ErweiterteBestandssucheType">
            <xsd:annotation>
               <xsd:documentation>Bestandssuche-Identifikation für Einzel- und Anlagenanschlüsse</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Erweiterte Bestandssuche wird ausschließlich bei GF/PG: NEU/Preselection und REX-MK/TAL verwendet</xsd:documentation>
            </xsd:annotation>
            <xsd:choice>
               <xsd:element name="anlagenanschluss" type="tns:OnkzDurchwahlAbfragestelleType">
                  <xsd:annotation>
                     <xsd:documentation>Bestandssuche ID für Durchwahlanlagen</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="einzelanschluss" type="tns:OnkzRufNrType">
                  <xsd:annotation>
                     <xsd:documentation>Bestandssuche ID für Einzelanschlüsse</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:choice>
         </xsd:complexType>
         <xsd:simpleType name="ExterneAuftragsnummerType">
            <xsd:annotation>
               <xsd:documentation>Eindeutiger Identifier des Auftrages, durch den Kunden zu vergeben</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="20"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="FirmaType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="anrede" type="tns:AnredeFirmaType">
                  <xsd:annotation>
                     <xsd:documentation>Anrede zur Firma</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="firmenname" type="tns:VornameType">
                  <xsd:annotation>
                     <xsd:documentation>Name der Firma</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="firmennameZweiterTeil" type="tns:NachnameType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Firmenname 2. Teil</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="GebaeudeteilNameType">
            <xsd:annotation>
               <xsd:documentation>Gebäudeteil</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="25"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="GebaeudeteilType">
            <xsd:sequence>
               <xsd:element name="gebaeudeteilName" type="tns:GebaeudeteilNameType"/>
               <xsd:element name="gebaeudeteilZusatz"
                            type="tns:GebaeudeteilZusatzType"
                            minOccurs="0"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="GebaeudeteilZusatzType">
            <xsd:annotation>
               <xsd:documentation>Gebäudeteilzusatz</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="25"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="GeschaeftsfallArtType">
            <xsd:annotation>
               <xsd:documentation>der Schlüssel für die Art des Geschäftsfalles nach der Orderschnittstelle als Kategorisierung für die Art des Kundenwunsches</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="50"/>
               <xsd:enumeration value="Bereitstellung">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Kuendigung">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Aenderung">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Produktgruppenwechsel">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Anbieterwechsel">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="BereitstellungService">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="Auskunft">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="GeschaeftsfallEnumType">
            <xsd:annotation>
               <xsd:documentation>der Schlüssel zur Auswahl des Geschäftsfalles an der Orderschnittstelle als Unterkategorie der Geschäftsfall-Art; beschreibt, wie der Kundenauftrag prozessiert wird</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="10"/>
               <xsd:enumeration value="NEU">
                  <xsd:annotation>
                     <xsd:documentation>Bereitstellung (Bestellung neues Produkt)</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="KUE-KD">
                  <xsd:annotation>
                     <xsd:documentation>Kündigung durch den Kunden</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="KUE-DT">
                  <xsd:annotation>
                     <xsd:documentation>Kündigung durch die Telekom</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="AEN-LMAE">
                  <xsd:annotation>
                     <xsd:documentation>Änderung von Leistungsmerkmalen</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="LAE">
                  <xsd:annotation>
                     <xsd:documentation>Änderung von Leistungen</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="PGW">
                  <xsd:annotation>
                     <xsd:documentation>Produktgruppenwechsel</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="PV">
                  <xsd:annotation>
                     <xsd:documentation>Providerwechsel</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="VBL">
                  <xsd:annotation>
                     <xsd:documentation>Verbundleistung</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="REX-MK">
                  <xsd:annotation>
                     <xsd:documentation>Rufnummernexport mit Anschlusskündigung</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="SER-POW">
                  <xsd:annotation>
                     <xsd:documentation>Portwechsel</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
               <xsd:enumeration value="AUS-BUE">
                  <xsd:annotation>
                     <xsd:documentation>Bestandsübersicht</xsd:documentation>
                  </xsd:annotation>
               </xsd:enumeration>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="GeschaeftsfallProduktType" abstract="true">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basis für alle geschäftsfall-spezifische Produkt-Daten</xsd:documentation>
            </xsd:annotation>
         </xsd:complexType>
         <xsd:complexType name="GeschaeftsfallType" abstract="true"/>
         <xsd:simpleType name="HausnummerType">
            <xsd:annotation>
               <xsd:documentation>Die Hausnummer wird als Zahl (0-9999) angegeben.</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:pattern value="\d{1,4}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="HausnummernZusatzType">
            <xsd:annotation>
               <xsd:documentation>eine mögliche Ergänzung zur Hausnummer</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="6"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="IPTVBasisUserType">
            <xsd:annotation>
               <xsd:documentation>Kennzeichnet den Endkunden des Providers. Einer der fixen Anteile der IPTV-OnlineKennung.</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="40"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="IPTVOnlineKennungType">
            <xsd:annotation>
               <xsd:documentation>Einzelne Angaben zur Onlinekennung des Endkunden für Accessvarianten mit IPTV</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Pflichtfeld bei erstmaliger Bereitstellung von Accessvarianten mit IPTV.</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="providerKennung" type="tns:IPTVProviderKennungType"/>
               <xsd:element name="basisUser" type="tns:IPTVBasisUserType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="IPTVProviderKennungType">
            <xsd:annotation>
               <xsd:documentation>Kennzeichnet den Provider, der den Access betreibt. Einer der fixen Anteile der IPTV-OnlineKennung.</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="15"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="KVZNrType">
            <xsd:annotation>
               <xsd:documentation>Eindeutiger Identifier des Kabelverteilers</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="8"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="KVZSchaltnummerType">
            <xsd:annotation>
               <xsd:documentation>Angaben des Carriers zu seinen Schaltdaten</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">Beispiel: 3 K1 0060</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="8"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="KennerKuendigungRestlicheRnrType">
            <xsd:annotation>
               <xsd:documentation>Kennzeichnung, ob restliche Rufnummern zu kündigen sind</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:BoolDecisionType"/>
         </xsd:simpleType>
         <xsd:complexType name="KennerType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="ProjektID" type="tns:ProjektIDType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>eindeutige Identifikation eines Projektes</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">nicht vorgesehen für GF/PG: NEU/Preselection, AUS-BUE</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Auftragskenner" type="tns:AuftragskennerType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>setzt sich zusammen aus Auftragsklammer und Anzahl der Aufträge</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">nicht vorgesehen für GF: KUE-KD, REX-MK, AUS-BUE</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="KopplungskennerType">
            <xsd:annotation>
               <xsd:documentation>eindeutige Identifikation zur Kopplung mehrerer Aufträge innerhalb eines Projektes</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="30"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="KundeType">
            <xsd:sequence>
               <xsd:element name="kundennummer" type="tns:KundennummerType"/>
               <xsd:element name="leistungsnummer" type="tns:LeistungsnummerType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="KundennummerType">
            <xsd:annotation>
               <xsd:documentation>Eindeutige ID des Kunden, wird bei Neuanlage des Kunden vergeben</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:length value="10"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="KundenwunschterminType">
            <xsd:annotation>
               <xsd:documentation>Terminwunsch des Providers</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="datum" type="tns:TerminType">
                  <xsd:annotation>
                     <xsd:documentation>Terminwunsch des Providers, zu dem das Produkt betriebsfähig bereitgestellt werden soll</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="zeitfenster" type="tns:ZeitfensterType">
                  <xsd:annotation>
                     <xsd:documentation>Eingrenzung des Kundenwunschtermins auf definierte Stundenintervalle</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Nur zulässige Zeitfenster möglich. Zeitfenster und Kundenwunschtermin müssen konsistent sein.</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Die Zuordnungen von einzelnen Werten zu den Zeitfenstern müssen aus der Zeitfenstertabelle entnommen werden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="KupplungType">
            <xsd:annotation>
               <xsd:documentation>Nummer der Kupplung</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Einstellige Angaben werden immer mit führender 0 ergänzt</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="2"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="LandType">
            <xsd:annotation>
               <xsd:documentation>der zweistellige Ländercode (ISO)</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">der zweistellige ISO-Ländercode, Beispiel: de</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:length value="2"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="LeistungsnummerType">
            <xsd:annotation>
               <xsd:documentation>Eindeutiger Identifier der Leistung aus einem Rahmenvertrag des Kunden der für den Auftrag heranzuziehen ist</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="10"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="LeitungsabschnittType">
            <xsd:annotation>
               <xsd:documentation>Angaben zu Leitungsabschnitt der Leitung</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="LfdNrLeitungsabschnitt">
                  <xsd:annotation>
                     <xsd:documentation>fortlaufende Nummer zur Identifikation des Leitungsabschnittes</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:int">
                        <xsd:minExclusive value="0"/>
                        <xsd:maxExclusive value="99"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="Leitungslaenge" type="tns:LeitungslaengeType">
                  <xsd:annotation>
                     <xsd:documentation>Länge (Einheit: Meter) des Leitungsabschnitts</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Leitungsdurchmesser" type="tns:LeitungsdurchmesserType">
                  <xsd:annotation>
                     <xsd:documentation>Durchmesser des Leitungsabschnitts</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="LeitungsbezeichnungType">
            <xsd:annotation>
               <xsd:documentation>eindeutige Identifikation einer Leitung</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="leitungsschluesselzahl" type="tns:LeitungsschluesselzahlType">
                  <xsd:annotation>
                     <xsd:documentation>kennzeichnet die Art der Leitung</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="onkzA" type="tns:OrtsnetzkennzahlType">
                  <xsd:annotation>
                     <xsd:documentation>die Ortsnetzkennzahl des Standortes A</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="onkzB" type="tns:OrtsnetzkennzahlType">
                  <xsd:annotation>
                     <xsd:documentation>die Ortsnetzkennzahl des Standortes B</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ordnungsnummer" type="tns:OrdnungsnummerType">
                  <xsd:annotation>
                     <xsd:documentation>fortlaufende Nummer zur Kennzeichnung der Leitung</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="LeitungsdurchmesserType">
            <xsd:annotation>
               <xsd:documentation>Durchmesser des Leitungsabschnitts</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="5"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="LeitungslaengeType">
            <xsd:annotation>
               <xsd:documentation>Länge (Einheit: Meter) des Leitungsabschnitts</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="5"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="LeitungsschluesselzahlErgaenzungType">
            <xsd:annotation>
               <xsd:documentation>Zusatzangaben zur Spezifizierung der Leitungsart</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="5"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="LeitungsschluesselzahlType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="4"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="LeitungswiderstandType">
