         <xsd:complexType name="PortierungDurchwahlanlageType">
            <xsd:annotation>
               <xsd:documentation>Angaben zu Rufnummernportierung eines Durchwahlanlage-Anschlusses</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="onkzDurchwahlAbfragestelle"
                            type="tns:OnkzDurchwahlAbfragestelleType"/>
               <xsd:element name="zuPortierenderRufnummernblock"
                            type="tns:RufnummernblockType"
                            maxOccurs="5">
                  <xsd:annotation>
                     <xsd:documentation>Auflistung der zu portierenden Rufnummernblöcke</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="PortierungEinzelanschlussType">
            <xsd:annotation>
               <xsd:documentation>Angaben zu dem Einzelanschluss, von dem die Rufnummern portiert werden sollen</xsd:documentation>
            </xsd:annotation>
            <xsd:choice>
               <xsd:element name="alleRufnummern">
                  <xsd:annotation>
                     <xsd:documentation>Alle Rufnummern des Anschlusses müssen portiert werden. </xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes/important">Darf nur bei Universalanschlüssen angegeben werden.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="tns:BoolDecisionType">
                        <xsd:enumeration value="J"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="rufnummernliste" type="tns:RufnummernListeType">
                  <xsd:annotation>
                     <xsd:documentation>Bestimmte Rufnummern des Anschlusses müssen portiert werden</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:choice>
         </xsd:complexType>
         <xsd:complexType name="PortierungRufnummernMeldungType">
            <xsd:annotation>
               <xsd:documentation>Informationen über zu portierenden Rufnummern</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="zuPortierendeOnkzRnr" type="tns:OnkzRufNrType" maxOccurs="10">
                  <xsd:annotation>
                     <xsd:documentation>Auflistung der zu portierenden ONKZ/Runfnummern</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="PortierungRufnummernbloeckeMeldungType">
            <xsd:annotation>
               <xsd:documentation>Informationen über zu portierenden Rufnummernblöcken</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="onkzDurchwahlAbfragestelle"
                            type="tns:OnkzDurchwahlAbfragestelleType"/>
               <xsd:element name="zuPortierenderRufnummernblock"
                            type="tns:RufnummernblockType"
                            maxOccurs="5">
                  <xsd:annotation>
                     <xsd:documentation>Auflistung zu portierender Rufnummernblöcken am Anlagenanschluss</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="PortierungskennerType">
            <xsd:annotation>
               <xsd:documentation>Code für den Carrier oder ISP-Kennung der DT, zu dem die Rufnummern abgegeben werden</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
                  Format 1 (Code für den Carrier): 'D[NNN]-[CCC]', wo [NNN] - 3-stellige Zahl der GFU-Kennung, [CCC] -
                  Carrier-Code (Ziffern ohne führende Null)
                  Format 2 (ISP-Kennung der DT): '[NNN][X][NN]', wo [NNN] - 3-stellige Zahl, [X] - Trennzeichen A-X,
                  [NN] - 2-stellige Zahl
                  Beispiele: D001-333, 123A22
               </xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Nur gültige Portierungskenner gemäß Netzbetreiberliste oder ISP-Kennung für Portierung auf DT-Plattform zulässig</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="1"/>
               <xsd:whiteSpace value="collapse"/>
               <xsd:pattern value="D\d{3}-\d{3}"/>
               <xsd:pattern value="\d{3}[A-X]\d{2}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="PositionsnummerType">
            <xsd:annotation>
               <xsd:documentation>der Identifikator der Position innerhalb des Auftrages</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:int">
               <xsd:minInclusive value="1"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="PostleitzahlType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="10"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="ProduktType">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basis für alle Produkt-Strukturen</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="kategorie">
                  <xsd:annotation>
                     <xsd:documentation>Produktkategorie</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="TAL"/>
                        <xsd:enumeration value="XDSLb"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="bezeichner" type="tns:ProduktbezeichnerType">
                  <xsd:annotation>
                     <xsd:documentation>Produktname des Produktes, das in der Auftragsposition zugebucht, in Wegfall gebracht oder geändert werden soll</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Nur zulässige Produktnamen aus Produktbezeichner-Anlage möglich</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">s. Anlage Produktbezeichner je Produktgruppe</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="ProduktbezeichnerType">
            <xsd:annotation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
                  s. Anlage Produktbezeichner je Produktgruppe
                  Beispiele: "ADSL SA 16000", "TAL; CuDA 2 Draht hbr (HVt)"
               </xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="100"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="ProduktpositionType">
            <xsd:sequence>
               <xsd:element name="aktionscode" type="tns:AktionscodeType">
                  <xsd:annotation>
                     <xsd:documentation>Kenner für Zugang Wegfall oder Änderung des Produktes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="produkt" type="tns:MeldungProduktType">
                  <xsd:annotation>
                     <xsd:documentation>Produkt, das im Auftrag zugebucht, geändert bzw. in Wegfall gebracht wurde</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="ProjektIDType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="projektkenner" type="tns:ProjektkennerType">
                  <xsd:annotation>
                     <xsd:documentation>eindeutige ID des Projektes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="kopplungskenner" type="tns:KopplungskennerType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>eindeutige ID zur Kopplung mehrerer Aufträge innerhalb des Projektes
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="ProjektkennerType">
            <xsd:annotation>
               <xsd:documentation>eindeutige Identifikation eines Projektes</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="30"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="ProvidernameType">
            <xsd:annotation>
               <xsd:documentation>Name eines Providers</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="60"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="ReservierungsnummerType">
            <xsd:annotation>
               <xsd:documentation>Eindeutige Reservierungsnummer für den gebuchten Montagetermin</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="20"/>
               <xsd:pattern value="\d{1,20}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="RnrRechnungsnummerType">
            <xsd:annotation>
               <xsd:documentation>Die Rufnummer / Rechnungsnummer besteht aus maximal 14 Ziffern.</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:RufnummerType"/>
         </xsd:simpleType>
         <xsd:simpleType name="RufnummerType">
            <xsd:annotation>
               <xsd:documentation>Die Rufnummer besteht aus maximal 14 Ziffern.</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="14"/>
               <xsd:pattern value="\d{1,14}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="RufnummernListeType">
            <xsd:sequence>
               <xsd:element name="zuPortierendeOnkzRnr" type="tns:OnkzRufNrType" maxOccurs="10">
                  <xsd:annotation>
                     <xsd:documentation>Auflistung der zu portierenden ONKZ/Runfnummern</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="RufnummernblockType">
            <xsd:annotation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
                  - der Wert von 'Rufnummernblock von' muss kleiner sein als der Wert von 'Rufnummernblock bis'
                  - die die Nummernblocklänge von 'Rufnummernblock von' und von 'Rufnummernblock bis' müssen jeweils die
                  gleiche Stellenzahl aufweisen
                  - alle Ziffern außer der ersten Ziffer müssen bei 'Rufnummernblock von' gleich '0' sein,
                  beispielsweise '20', '300'
                  - alle Ziffern außer der ersten Ziffer müssen bei 'Rufnummernblock bis' gleich '9' sein,
                  beispielsweise '29', '499'
                  - die Schnittmenge der angegebenen Rufnummernblöcke muss die leere Menge sein, die Rufnummernblöcke
                  dürfen sich nicht überschneiden
               </xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="rnrBlockVon">
                  <xsd:annotation>
                     <xsd:documentation>Untergrenze innerhalb des zu portierenden Rufnummernblocks</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:pattern value="\d{1,6}"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="rnrBlockBis">
                  <xsd:annotation>
                     <xsd:documentation>Obergrenze innerhalb des zu portierenden Rufnummernblocks</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:pattern value="\d{1,6}"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="RufnummernportierungEinzelanschlussMeldungType">
            <xsd:annotation>
               <xsd:documentation>Hinweise zu der Portierung  von Rufnummern oder Rufnummernblöcken eines Einzel- oder Anlagenanschlusses</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:restriction base="tns:RufnummernportierungMeldungType">
                  <xsd:sequence>
                     <xsd:element name="portierungRufnummern" type="tns:PortierungRufnummernMeldungType">
                        <xsd:annotation>
                           <xsd:documentation>Hinweise zu der Portierung von Rufnummern eines Anschlusses</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:restriction>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="RufnummernportierungEinzelanschlussType">
            <xsd:annotation>
               <xsd:documentation>Angaben zur Rufnummern-Portierung</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Angabe nur möglich, wenn aufnehmender oder abgebender Provider Telekom ist. Bei keiner Angabe werden die Rufnummern nicht portiert.</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="einzelanschluss" type="tns:PortierungEinzelanschlussType">
                  <xsd:annotation>
                     <xsd:documentation>Angaben zum Einzelanschluss, von dem die Rufnummern portiert werden sollen				</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Portierungskenner" type="tns:PortierungskennerType">
                  <xsd:annotation>
                     <xsd:documentation>Code für den Carrier oder ISP-Kennung der DT, zu dem die Rufnummern abgegeben werden
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="RufnummernportierungMeldungType">
            <xsd:annotation>
               <xsd:documentation>Hinweise zu der Portierung  von Rufnummern oder Rufnummernblöcken eines Einzel- oder Anlagenanschlusses</xsd:documentation>
            </xsd:annotation>
            <xsd:choice>
               <xsd:element name="portierungRufnummern" type="tns:PortierungRufnummernMeldungType">
                  <xsd:annotation>
                     <xsd:documentation>Hinweise zu der Portierung von Rufnummern eines Anschlusses</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="portierungRufnummernbloecke"
                            type="tns:PortierungRufnummernbloeckeMeldungType">
                  <xsd:annotation>
                     <xsd:documentation>Hinweise zu der Portierung von Rufnummernblöcken eines Anlagenanschlusses</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:choice>
         </xsd:complexType>
         <xsd:complexType name="RufnummernportierungType">
            <xsd:annotation>
               <xsd:documentation>Angaben zur Rufnummern-Portierung</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Angabe nur möglich, wenn aufnehmender oder abgebender Provider Telekom ist. Bei keiner Angabe werden die Rufnummern nicht portiert.</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:choice>
                  <xsd:element name="anlagenanschluss" type="tns:PortierungDurchwahlanlageType">
                     <xsd:annotation>
                        <xsd:documentation>Angaben zu Rufnummernportierung eines Durchwahlanlage-Anschlusses</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="einzelanschluss" type="tns:PortierungEinzelanschlussType">
                     <xsd:annotation>
                        <xsd:documentation>Angaben zum Einzelanschluss, von dem die Rufnummern portiert werden sollen				</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:choice>
               <xsd:element name="Portierungskenner" type="tns:PortierungskennerType">
                  <xsd:annotation>
                     <xsd:documentation>Code für den Carrier oder ISP-Kennung der DT, zu dem die Rufnummern abgegeben werden
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="SchaltangabenType">
            <xsd:annotation>
               <xsd:documentation>beschreibt die technischen Vorgaben zur Schaltung</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="schaltung" type="tns:SchaltungAbstractType" maxOccurs="2">
                  <xsd:annotation>
                     <xsd:documentation>Angaben zur Schaltung bei 2-Draht / 1-Faser (einfach) oder 4-Draht / 2-Fase (doppelt)</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="SchaltungAbstractType" abstract="true">
            <xsd:annotation>
               <xsd:documentation>Angaben zur Schaltung</xsd:documentation>
            </xsd:annotation>
         </xsd:complexType>
         <xsd:complexType name="SchaltungGlasfaserType">
            <xsd:annotation>
               <xsd:documentation>Angaben zur Schaltung bei Glasfaserleitung</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:SchaltungAbstractType">
                  <xsd:sequence>
                     <xsd:element name="UEVT" type="tns:UebergabeverteilerType">
                        <xsd:annotation>
                           <xsd:documentation>Nummer des Übergabeverteilers</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="EVS" type="tns:EndverschlussType">
                        <xsd:annotation>
                           <xsd:documentation>Nummer des Endverschlusses</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="kupplung" type="tns:KupplungType">
                        <xsd:annotation>
                           <xsd:documentation>Nummer der Kupplung</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="SchaltungIsisOpalMeldungType">
            <xsd:annotation>
               <xsd:documentation>Technische Angaben zur Schaltung TAL ISIS/OPAL in Meldungen</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="schaltung"
                            type="tns:SchaltungKupferType"
                            minOccurs="0"
                            maxOccurs="2"/>
               <xsd:element name="v5-ID" type="tns:V51IdType" minOccurs="0"/>
               <xsd:element name="zeitschlitz"
                            type="tns:ZeitschlitzType"
                            minOccurs="0"
                            maxOccurs="2">
                  <xsd:annotation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Pflichtangabe, falls v5.1-ID angegeben</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Angabe Zeitschlitz 1, wenn Produkt Analoge TelAsl bei OPAL/ISIS-Outdoor</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Angabe Zeitschlitz 2, wenn Produkt BaAsl bei OPAL/ISIS-Outdoor</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="SchaltungIsisOpalType">
            <xsd:annotation>
               <xsd:documentation>Angaben zur Schaltung für ISIS/OPAL</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:SchaltungAbstractType">
                  <xsd:sequence>
                     <xsd:element name="UEVT" type="tns:UebergabeverteilerType">
                        <xsd:annotation>
                           <xsd:documentation>Nummer des Übergabeverteilers</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="anschlussart" type="tns:AnschlussartType">
                        <xsd:annotation>
                           <xsd:documentation>Übertragungseinrichtung am ISIS/OPAL-Netz</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="SchaltungKVZ-TAL-Type">
            <xsd:annotation>
               <xsd:documentation>Technische Angaben zur Schaltung der KVZ -TAL</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">zulässige TAL-Produktvarianten: TAL-CuDA 2 Draht (KVZ), TAL-CuDA 2 Draht hbr (KVZ), TAL-CuDA 4 Draht hbr (KVZ)</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:SchaltungAbstractType">
                  <xsd:sequence>
                     <xsd:element name="KVZ-Nr" type="tns:KVZNrType">
                        <xsd:annotation>
                           <xsd:documentation>Eindeutiger Identifier des Kabelverteilers</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="KVZ-Schaltnummer" type="tns:KVZSchaltnummerType">
                        <xsd:annotation>
                           <xsd:documentation>Angaben des Carriers zu seinen Schaltdaten</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="SchaltungKupferType">
            <xsd:annotation>
               <xsd:documentation>Angaben zur Schaltung bei Kupferleitung</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:SchaltungAbstractType">
                  <xsd:sequence>
                     <xsd:element name="UEVT" type="tns:UebergabeverteilerType">
                        <xsd:annotation>
                           <xsd:documentation>Nummer des Übergabeverteilers</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="EVS" type="tns:EndverschlussType">
                        <xsd:annotation>
                           <xsd:documentation>Nummer des Endverschlusses</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="doppelader" type="tns:DoppeladerType">
                        <xsd:annotation>
                           <xsd:documentation>Nummer der Doppelader</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="StandortAType">
            <xsd:annotation>
               <xsd:documentation>Standort-Adresse des Kunden, an dem die Leistung ausgeführt werden soll</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Sofern ein Standort A angegeben ist wird die ID für Bestandssuche bzw. die Vertragsnummer gegen den Straßennamen validiert.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:StandortType">
                  <xsd:sequence>
                     <xsd:element name="lageTAE_ONT" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Kommentar zur Beschreibung der TAE-Dose oder ONT (für FTTH) am Standort A</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="tns:ErlaubterZeichenBereichMehrzeiligType">
                              <xsd:minLength value="1"/>
                              <xsd:maxLength value="160"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="StandortBType">
            <xsd:annotation>
               <xsd:documentation>Standort-Adresse zum Kollokationsraum, bzw. zu Verbindungen zu einem zweiten Endpunkt</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="strasse" type="tns:StrasseType">
                  <xsd:annotation>
                     <xsd:documentation>Straße des Standortes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="land" default="de" type="tns:LandType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Land des Standortes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="postleitzahl" type="tns:PostleitzahlType">
                  <xsd:annotation>
                     <xsd:documentation>PLZ des Standortes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ort" type="tns:OrtType">
                  <xsd:annotation>
                     <xsd:documentation>Ort des Standortes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="lageKollokationsraum" type="tns:OnkzAnschlussType">
                  <xsd:annotation>
                     <xsd:documentation>Ergänzende Daten zum Kollokationsraum an Standort B
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="StandortKorrekturType">
            <xsd:sequence>
               <xsd:element name="strasse" type="tns:StrasseType">
                  <xsd:annotation>
                     <xsd:documentation>Straße des Standortes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="gebaeudeteil" type="tns:GebaeudeteilType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Gebäudeteil des Standortes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="land" type="tns:LandType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Land des Standortes (vorbelegt mit DE)</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="postleitzahl" type="tns:PostleitzahlType">
                  <xsd:annotation>
                     <xsd:documentation>PLZ des Standortes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ort" type="tns:OrtType">
                  <xsd:annotation>
                     <xsd:documentation>Ort des Standortes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="StandortOhneGebaeudeteilType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:choice>
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
                  <xsd:element name="firma" type="tns:FirmaType">
                     <xsd:annotation>
                        <xsd:documentation>die Firma bei Firmenadressen</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="person" type="tns:PersonType">
                     <xsd:annotation>
                        <xsd:documentation>die Person bei Adressen natürlicher Personen</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:choice>
               <xsd:element name="strasse" type="tns:StrasseType">
                  <xsd:annotation>
                     <xsd:documentation>Straße des Standortes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="land" default="de" type="tns:LandType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Land des Standortes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="postleitzahl" type="tns:PostleitzahlType">
                  <xsd:annotation>
                     <xsd:documentation>PLZ des Standortes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ort" type="tns:OrtType">
                  <xsd:annotation>
                     <xsd:documentation>Ort des Standortes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="StandortPersonType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:choice>
               <xsd:annotation>
                  <xsd:documentation/>
               </xsd:annotation>
               <xsd:element name="firma" type="tns:FirmaType">
                  <xsd:annotation>
                     <xsd:documentation>die Firma bei Firmenadressen</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="person" type="tns:PersonType">
                  <xsd:annotation>
                     <xsd:documentation>die Person bei Adressen natürlicher Personen</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:choice>
         </xsd:complexType>
         <xsd:complexType name="StandortType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:choice>
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
                  <xsd:element name="firma" type="tns:FirmaType">
                     <xsd:annotation>
                        <xsd:documentation>die Firma bei Firmenadressen</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="person" type="tns:PersonType">
                     <xsd:annotation>
                        <xsd:documentation>die Person bei Adressen natürlicher Personen</xsd:documentation>
                        <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Bei StandortA: Anschlussinhaber (Vertragspartner Endkunde); wird im Anbieterwechsel mit AKM-PV an den TNBabg übergeben</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:choice>
               <xsd:element name="strasse" type="tns:StrasseType">
                  <xsd:annotation>
                     <xsd:documentation>Straße des Standortes</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Bei Angabe von Standort A wird Vertragsnummer bzw. Bestandssuche ID gegen die ersten Zeichen des Straßennamens validiert</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="gebaeudeteil" type="tns:GebaeudeteilType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Gebäudeteil am Standort</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="land" default="de" type="tns:LandType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Land des Standortes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="postleitzahl" type="tns:PostleitzahlType">
                  <xsd:annotation>
                     <xsd:documentation>PLZ des Standortes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ort" type="tns:OrtType">
                  <xsd:annotation>
                     <xsd:documentation>Ort des Standortes</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="StandortVersandType">
            <xsd:annotation>
               <xsd:documentation>Lieferadresse für Hardware, wenn abweichend  von Standort A</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="tns:StandortOhneGebaeudeteilType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="StrasseType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="strassenname" type="tns:StrassennameType">
                  <xsd:annotation>
                     <xsd:documentation>Straßenname</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="hausnummer" type="tns:HausnummerType">
                  <xsd:annotation>
                     <xsd:documentation>Hausnummer</xsd:documentation>
                     <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Bei Grundstücken ohne Hausnummer,  ist die Hausnummer mit dem Wert 0 zu befüllen. Konkrete Hausnummer erforderlich; keine Angabe von z.B. 14-17 möglich</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="hausnummernZusatz"
                            type="tns:HausnummernZusatzType"
                            minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Hausnummernzusatz</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="StrassennameType">
            <xsd:annotation>
               <xsd:documentation>maximal 40 Zeichen für die Angabe der Strasse zur Adresse</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="40"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="TelefonnummerType">
            <xsd:annotation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
                  Es werden Formate mit und ohne Ländercode unterstützt. Beispiele: +49 351 111222 oder 0351 111222
               </xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="30"/>
               <xsd:whiteSpace value="collapse"/>
               <xsd:pattern value="((\+\d{2} |0)\d{2,5} )?\d{1,14}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="TerminType">
            <xsd:annotation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
                  Format: YYYY-MM-DD
                  Beispiel: 2009-02-18
               </xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:date">
               <xsd:pattern value="\d{4}-.*"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="UFAType">
            <xsd:annotation>
               <xsd:documentation>Nummer des unbelegten fiktiven Anschlusses</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="UFAnummer">
                  <xsd:annotation>
                     <xsd:documentation>Nummer des unbelegten fiktiven Anschlusses</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:annotation>
                        <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
                           Format 1: '[N1]C[N2]', wo [N1] - 2 bis 5-stellige Zahl, C - Trennzeichen, [N2] - 6 bis
                           7-stellige Zahl
                           Beispiel: 12345C1234567
                           Format 2: alphanumerisch 1 bis 15 Zeichen lang
                        </xsd:documentation>
                     </xsd:annotation>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="15"/>
                        <xsd:whiteSpace value="collapse"/>
                        <xsd:pattern value="\d{2,5}C\d{6,7}"/>
                        <xsd:pattern value="[0-9a-zA-Z]{1,15}"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="UebergabeverteilerType">
            <xsd:annotation>
               <xsd:documentation>Nummer des Übergabeverteilers</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="4"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="Uebertragungsverfahren-TAL-Type">
            <xsd:annotation>
               <xsd:documentation>Ausprägung des Übertragungsverfahrens</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:UebertragungsverfahrenType"/>
         </xsd:simpleType>
         <xsd:simpleType name="UebertragungsverfahrenType">
            <xsd:annotation>
               <xsd:documentation>Angaben zur produktspezifischen Qualität des Anschlusses</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:enumeration value="H01"/>
               <xsd:enumeration value="H02"/>
               <xsd:enumeration value="H03"/>
               <xsd:enumeration value="H04"/>
               <xsd:enumeration value="H05"/>
               <xsd:enumeration value="H06"/>
               <xsd:enumeration value="H07"/>
               <xsd:enumeration value="H08"/>
               <xsd:enumeration value="H10"/>
               <xsd:enumeration value="H11"/>
               <xsd:enumeration value="H13"/>
               <xsd:enumeration value="H14"/>
               <xsd:enumeration value="H15"/>
               <xsd:enumeration value="H16"/>
               <xsd:enumeration value="H17"/>
               <xsd:enumeration value="H18"/>
               <xsd:enumeration value="H19"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="V51IdType">
            <xsd:annotation>
               <xsd:documentation>ID der V5.1-Schnittstelle</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="8"/>
               <xsd:pattern value="\d{1,8}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="VLanIdType">
            <xsd:annotation>
               <xsd:documentation>V-LAN ID vom Wholesale-Kunden, das den Endkunden identifiziert</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">Eine Zahl zwischen 1 und 4000</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:int">
               <xsd:minInclusive value="1"/>
               <xsd:maxInclusive value="4000"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="VerschluesselterTextType">
            <xsd:annotation>
               <xsd:documentation>Verschlüsselte Textangaben</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="128"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="VersionType">
            <xsd:annotation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
                  Format: Zahl als Versionsnummer
                  Beispiel: '3', '02'
               </xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="20"/>
               <xsd:whiteSpace value="collapse"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="VertragsnummerType">
            <xsd:annotation>
               <xsd:documentation>Bezeichnet den Einzelvertrag, auf den sich der Auftrag bezieht.</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="10"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="VorabstimmungsIdType">
            <xsd:annotation>
               <xsd:documentation>ID der Vorabstimmungsanfrage aus dem Vorabstimmungsprozess</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">Beispiel: DEU.EKP1.V000000013</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:minLength value="16"/>
               <xsd:maxLength value="21"/>
               <xsd:pattern value="[0-9A-Z]{3}\.[0-9A-Z]{1,6}\.V[0-9A-Z]{9}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:complexType name="VormieterAnschlussType">
            <xsd:annotation>
               <xsd:documentation>Daten zur Identifikation eines vorhandenen Netzanschlusses (Telefon / Anlagenanschluss)</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/notes">Wenn Vormieter angegeben, muss mindestens: Person oder Anschluss oder UFA  aufgeführt werden</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="Person" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Identifikation des Netzanschlusses anhand der Person</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="vorname" type="tns:VornameType" minOccurs="0">
                           <xsd:annotation>
                              <xsd:documentation>Vorname des Vormieters</xsd:documentation>
                           </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="nachname" type="tns:NachnameType">
                           <xsd:annotation>
                              <xsd:documentation>Nachname des Vormieters</xsd:documentation>
                           </xsd:annotation>
                        </xsd:element>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
               <xsd:element name="anschluss" type="tns:AnschlussType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Identifikation des Netzanschlusses anhand der Anschlussrufnummer
                     </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="UFA" type="tns:UFAType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Identifikation des Netzanschlusses anhand der UFA-Nummer</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:simpleType name="VornameType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:restriction base="tns:ErlaubterZeichenBereichType">
               <xsd:minLength value="1"/>
               <xsd:maxLength value="30"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="ZeitfensterType">
            <xsd:annotation>
               <xsd:documentation>Das Zeitfenster konkretisiert den Termin (das Datum) um einen Bereich angegeben als Uhrzeit (von - bis).</xsd:documentation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">Ein Zeichen aus folgendem Bereich: 0-9, a-z. Beispiel: b </xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:length value="1"/>
               <xsd:pattern value="[0-9a-z]"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="ZeitschlitzType">
            <xsd:annotation>
               <xsd:documentation>Kanal mit 64 KBit/s auf der 2MBit/s-Leitung</xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:string">
               <xsd:maxLength value="2"/>
               <xsd:pattern value="\d{1,2}"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:simpleType name="ZeitstempelType">
            <xsd:annotation>
               <xsd:documentation source="http://wholesale.telekom.de/oss/doc/format">
                  Format: YYYY-MM-DDTHH:mm:ss.SSSZ, wo T - Trennungszeichen, SSS - Millisekunden, Z - Zeitzone
                  Beispiel: 2009-02-18T18:37:24.483+02:00
               </xsd:documentation>
            </xsd:annotation>
            <xsd:restriction base="xsd:dateTime">
               <xsd:pattern value="\d{4}-.*"/>
            </xsd:restriction>
         </xsd:simpleType>
         <xsd:element name="ABBM" type="tns:MeldungstypABBMType"/>
         <xsd:element name="ABBM-PV" type="tns:MeldungstypABBM-PVType"/>
         <xsd:element name="ABBMpos" type="tns:MeldungspositionsattributeABBMType"/>
         <xsd:element name="ABM" type="tns:MeldungstypABMType"/>
         <xsd:element name="ABM-PV" type="tns:MeldungstypABM-PVType"/>
         <xsd:element name="ABMpos" type="tns:MeldungspositionsattributeABMType"/>
         <xsd:element name="AKM-PV" type="tns:MeldungstypAKM-PVType"/>
         <xsd:element name="Anlage" type="tns:AnlageType"/>
         <xsd:element name="ENTM" type="tns:MeldungstypENTMType"/>
         <xsd:element name="ENTM-PV" type="tns:MeldungstypENTM-PVType"/>
         <xsd:element name="ERGM" type="tns:MeldungstypERGMType"/>
         <xsd:element name="ERLM" type="tns:MeldungstypERLMType"/>
         <xsd:element name="ERLM-K" type="tns:MeldungstypERLM-KType"/>
         <xsd:element name="ERLM-PV" type="tns:MeldungstypERLM-PVType"/>
         <xsd:element name="MTAM" type="tns:MeldungstypMTAMType"/>
         <xsd:element name="QEB" type="tns:MeldungstypQEBType"/>
         <xsd:element name="QEBpos" type="tns:MeldungspositionsattributeQEBType"/>
         <xsd:element name="RUEM-PV" type="tns:MeldungstypRUEM-PVType"/>
         <xsd:element name="TAM" type="tns:MeldungstypTAMType"/>
         <xsd:element name="VZM" type="tns:MeldungstypVZMType"/>
         <xsd:element name="VZM-PV" type="tns:MeldungstypVZM-PVType"/>
         <xsd:element name="meldung" type="tns:MeldungType"/>
         <xsd:element name="meldungsposition" type="tns:MeldungspositionType"/>
         <xsd:element name="notifyUpdateOrder">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="order" type="tns:AuftragstypType"/>
                  <xsd:element name="message">
                     <xsd:complexType>
                        <xsd:choice>
                           <xsd:element ref="tns:ABBM"/>
                           <xsd:element ref="tns:ABBM-PV"/>
                           <xsd:element ref="tns:ABM"/>
                           <xsd:element ref="tns:ABM-PV"/>
                           <xsd:element ref="tns:AKM-PV"/>
                           <xsd:element ref="tns:ENTM"/>
                           <xsd:element ref="tns:ENTM-PV"/>
                           <xsd:element ref="tns:ERGM"/>
                           <xsd:element ref="tns:ERLM"/>
                           <xsd:element ref="tns:ERLM-PV"/>
                           <xsd:element ref="tns:MTAM"/>
                           <xsd:element ref="tns:QEB"/>
                           <xsd:element ref="tns:TAM"/>
                           <xsd:element ref="tns:VZM"/>
                           <xsd:element ref="tns:VZM-PV"/>
                        </xsd:choice>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="captured" type="xsd:dateTime">
                     <xsd:annotation>
                        <xsd:documentation>Timestamp on which this notification was created.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:schema>
   </wsdl:types>
   <wsdl:service name="LineOrderNotificationService">
      <wsdl:port name="LineOrderNotificationServiceJMSQueue"
                 binding="tns:LineOrderNotificationServiceJMSBinding">
         <soap:address location="mnet.SupplierPartner.op.cdm.src.v1.LineOrderNotificationService"/>
         <jms:connectionFactory>QueueConnectionFactory</jms:connectionFactory>
         <jms:targetAddress destination="queue">mnet.SupplierPartner.op.cdm.src.v1.LineOrderNotificationService</jms:targetAddress>
      </wsdl:port>
      <wsdl:port name="LineOrderNotificationServiceJMSTopic"
                 binding="tns:LineOrderNotificationServiceJMSBinding">
         <soap:address location="mnet.SupplierPartner.op.cdm.ntf.v1.LineOrderNotificationService"/>
         <jms:connectionFactory>TopicConnectionFactory</jms:connectionFactory>
         <jms:targetAddress destination="topic">mnet.SupplierPartner.op.cdm.ntf.v1.LineOrderNotificationService</jms:targetAddress>
      </wsdl:port>
      <wsdl:port name="LineOrderNotificationServiceHTTP"
                 binding="tns:LineOrderNotificationServiceHTTPBinding">
         <soap:address location="http://mnetatlasdev01:12080/esb/SupplierPartner/LineOrderNotificationService/v1"/>
      </wsdl:port>
   </wsdl:service>
   <wsdl:portType name="LineOrderNotificationService">
      <wsdl:operation name="notifyUpdateOrder">
         <wsdl:input message="tns:notifyUpdateOrder"/>
      </wsdl:operation>
   </wsdl:portType>
   <wsdl:binding name="LineOrderNotificationServiceJMSBinding"
                 type="tns:LineOrderNotificationService">
      <soap:binding style="document"
                    transport="http://www.tibco.com/namespaces/ws/2004/soap/binding/JMS"/>
      <jms:binding messageFormat="Text"/>
      <wsdl:operation name="notifyUpdateOrder">
         <soap:operation style="document"
                         soapAction="/LineOrderNotificationService/notifyUpdateOrder"/>
         <wsdl:input>
            <soap:body use="literal" parts="in"/>
         </wsdl:input>
      </wsdl:operation>
   </wsdl:binding>
   <wsdl:binding name="LineOrderNotificationServiceHTTPBinding"
                 type="tns:LineOrderNotificationService">
      <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
      <wsdl:operation name="notifyUpdateOrder">
         <soap:operation style="document"
                         soapAction="/LineOrderNotificationService/notifyUpdateOrder"/>
         <wsdl:input>
            <soap:body use="literal" parts="in"/>
         </wsdl:input>
      </wsdl:operation>
   </wsdl:binding>
   <wsdl:message name="notifyUpdateOrder">
      <wsdl:part name="in" element="tns:notifyUpdateOrder"/>
   </wsdl:message>
</wsdl:definitions>
