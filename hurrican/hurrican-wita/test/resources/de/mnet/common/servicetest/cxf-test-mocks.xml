<?xml version="1.0" encoding="UTF-8"?>

<!-- Spring Konfigurationsfile - definiert eine ConnectionFactory auf den ATLAS ESB ueber JNDI. -->
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="esbTrackingOutInterceptor" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="org.apache.cxf.phase.AbstractPhaseInterceptor" />
    </bean>

</beans>
