<?xml version='1.0' encoding='UTF-8'?>
<getDocumentResponse
        xmlns="http://www.mnet.de/esb/cdm/Customer/DocumentArchiveService/v1">
    <document>
        <id>${documentId}</id>
        <class>AUFTRAEGE_UNTERLAGEN</class>
        <key>
            <name>Art des Dokuments</name>
            <value>${archiveDocumentType}</value>
            <type>String</type>
        </key>
        <key>
            <name>Vertrag</name>
            <value>citrus:randomNumber(6)</value>
            <type>String</type>
        </key>
        <key>
            <name>Kundennr.</name>
            <value>500331323</value>
            <type>Number</type>
        </key>
        <key>
            <name>Debitornr.</name>
            <value>M500596221</value>
            <type>String</type>
        </key>
        <key>
            <name>Taifun_Auftragsnr.</name>
            <value>citrus:randomNumber(8)</value>
            <type>Number</type>
        </key>
        <key>
            <name>Dokumentendatum</name>
            <value>citrus:currentDate('yyyy-MM-dd'T'HH:mm:00')</value>
            <type>DateTime</type>
        </key>
        <key>
            <name>UUID</name>
            <value>citrus:randomUUID()</value>
            <type>String</type>
        </key>
        <item>
            <id>citrus:randomNumber(10)</id>
            <name>${fileName}</name>
            <extension>${fileExtension}</extension>
            <creationDate>citrus:currentDate('yyyy-MM-dd'T'HH:mm:00')</creationDate>
            <size>${fileLength}</size>
            <data>${fileData}</data>
        </item>
    </document>
</getDocumentResponse>