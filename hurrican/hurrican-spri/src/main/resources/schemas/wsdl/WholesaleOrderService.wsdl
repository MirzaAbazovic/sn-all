                  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                  xmlns:ws="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2"
                  targetNamespace="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2"
                  version="1.0"
                  elementFormDefault="qualified"
                  attributeFormDefault="unqualified">
         <xsd:import namespace="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2/order"/>
         <xsd:import namespace="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2/message"/>
         <xsd:element name="cancelOrder">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="sendingCarrier" type="xsd:string">
                     <xsd:annotation>
                        <xsd:documentation>The sending carrier nach ITU-CC DEU</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="receivingCarrier" type="xsd:string">
                     <xsd:annotation>
                        <xsd:documentation>The receiving carrier nach ITU-CC DEU</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="version">
                     <xsd:annotation>
                        <xsd:documentation>Major interface version mit der die Nachricht publiziert wurde bzw. publiziert werden soll</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="xsd:positiveInteger">
                           <xsd:totalDigits value="3"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="order" type="order:AuftragType"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="createOrder">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="sendingCarrier" type="xsd:string">
                     <xsd:annotation>
                        <xsd:documentation>The sending carrier nach ITU-CC DEU</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="receivingCarrier" type="xsd:string">
                     <xsd:annotation>
                        <xsd:documentation>The receiving carrier nach ITU-CC DEU</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="version">
                     <xsd:annotation>
                        <xsd:documentation>Major interface version mit der die Nachricht publiziert wurde bzw. publiziert werden soll</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="xsd:positiveInteger">
                           <xsd:totalDigits value="3"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="order" type="order:AuftragType"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="rescheduleOrder">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="sendingCarrier" type="xsd:string">
                     <xsd:annotation>
                        <xsd:documentation>The sending carrier nach ITU-CC DEU</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="receivingCarrier" type="xsd:string">
                     <xsd:annotation>
                        <xsd:documentation>The receiving carrier nach ITU-CC DEU</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="version">
                     <xsd:annotation>
                        <xsd:documentation>Major interface version mit der die Nachricht publiziert wurde bzw. publiziert werden soll</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="xsd:positiveInteger">
                           <xsd:totalDigits value="3"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="order" type="order:TerminverschiebungType"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="updateOrder">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="sendingCarrier" type="xsd:string">
                     <xsd:annotation>
                        <xsd:documentation>The sending carrier nach ITU-CC DEU</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="receivingCarrier" type="xsd:string">
                     <xsd:annotation>
                        <xsd:documentation>The receiving carrier nach ITU-CC DEU</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="version">
                     <xsd:annotation>
                        <xsd:documentation>Major interface version mit der die Nachricht publiziert wurde bzw. publiziert werden soll</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="xsd:positiveInteger">
                           <xsd:totalDigits value="3"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="orderType" type="message:AuftragstypType">
                     <xsd:annotation>
                        <xsd:documentation>type and category of the  related order</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="message" type="message:MessageType">
                     <xsd:annotation>
                        <xsd:documentation>Order message details</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
