         <xsd:group name="WitaExtendedInfosGroup">
            <xsd:sequence>
               <xsd:group ref="complex:WitaInfosGroup"/>
               <xsd:element name="bestandssuche" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Identifikation zur Ermittlung des Bestandes der DTAG im Rahmen eines Anbieterwechsels über die WBCI.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="onkz">
                           <xsd:annotation>
                              <xsd:documentation>Ortsnetzkennzahl des Anschlusses.</xsd:documentation>
                           </xsd:annotation>
                           <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                 <xsd:pattern value="\d{2,5}"/>
                              </xsd:restriction>
                           </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="rufnummer">
                           <xsd:annotation>
                              <xsd:documentation>Rufnummer des Anschlusses, bei Anlagenanschlüssen bzw. digitalen Sammelanschlüssen die Rechnungsnummer des den Auftrag betreffendes Anschlussbestandes.</xsd:documentation>
                           </xsd:annotation>
                           <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                 <xsd:pattern value="\d{1,14}"/>
                              </xsd:restriction>
                           </xsd:simpleType>
                        </xsd:element>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
               <xsd:element name="rufnummernPortierung" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Angaben zur Rufnummern-Portierung im Rahmen eines Anbieterwechsels über die WBCI.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:choice>
                           <xsd:element name="anlagenanschluss">
                              <xsd:annotation>
                                 <xsd:documentation>Angaben zu Rufnummernportierung eines Durchwahlanlage-Anschlusses.</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="onkzDurchwahlAbfragestelle">
                                       <xsd:annotation>
                                          <xsd:documentation>Kombination von der ONKZ, Durchwahlnummer und Abfragestelle (für die Anlagenanschlüsse).</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="onkz">
                                                <xsd:simpleType>
                                                   <xsd:restriction base="xsd:string">
                                                      <xsd:pattern value="\d{2,5}"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="durchwahlnummer">
                                                <xsd:simpleType>
                                                   <xsd:restriction base="xsd:string">
                                                      <xsd:pattern value="\d{1,8}"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="abfragestelle">
                                                <xsd:simpleType>
                                                   <xsd:restriction base="xsd:string">
                                                      <xsd:pattern value="\d{1,6}"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="zuPortierenderRufnummernblock" maxOccurs="5">
                                       <xsd:annotation>
                                          <xsd:documentation>Auflistung der zu portierenden Rufnummernblöcke.</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="rnrBlockVon">
                                                <xsd:simpleType>
                                                   <xsd:restriction base="xsd:string">
                                                      <xsd:pattern value="\d{1,6}"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="rnrBlockBis">
                                                <xsd:simpleType>
                                                   <xsd:restriction base="xsd:string">
                                                      <xsd:pattern value="\d{1,6}"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="einzelanschluss">
                              <xsd:annotation>
                                 <xsd:documentation>Angaben zum Einzelanschluss, von dem die Rufnummern portiert werden sollen.</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:choice>
                                    <xsd:element name="alleRufnummern">
                                       <xsd:annotation>
                                          <xsd:documentation>Alle Rufnummern des Anschlusses müssen portiert werden.</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="xsd:string">
                                             <xsd:enumeration value="J"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="rufnummernliste">
                                       <xsd:annotation>
                                          <xsd:documentation>Bestimmte Rufnummern des Anschlusses müssen portiert werden.</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="zuPortierendeOnkzRnr" maxOccurs="10">
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="onkz">
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="xsd:string">
                                                               <xsd:pattern value="\d{2,5}"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="rufnummer">
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="xsd:string">
                                                               <xsd:pattern value="\d{1,14}"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:choice>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:choice>
                        <xsd:element name="portierungskenner" type="xsd:string">
                           <xsd:annotation>
                              <xsd:documentation>Code für den Carrier oder ISP-Kennung der DT, zu dem die Rufnummern abgegeben werden.</xsd:documentation>
                           </xsd:annotation>
                        </xsd:element>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
               <xsd:element name="abgebenderEKP" type="xsd:boolean" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Gibt an, ob EKPab Telekom (Retail) ist.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:group>
         <xsd:group name="WitaInfosGroup">
            <xsd:sequence>
               <xsd:element name="witaVertragsnummer" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Zur Identifizierung eines Vertrages im Bestand der DTAG im Rahmen eines Anbieterwechsels über die WBCI.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
