      <xsd:schema xmlns:message="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2/message"
                  xmlns="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2"
