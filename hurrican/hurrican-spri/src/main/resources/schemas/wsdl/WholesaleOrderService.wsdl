<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:ns2="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2/order"
                  xmlns:ns1="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2/message"
                  xmlns:ns4="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2/simple"
                  xmlns:ns3="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2/enm"
                  xmlns:tns="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2"
                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                  xmlns:ns0="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2/complex"
                  xmlns:jms="http://www.tibco.com/namespaces/ws/2004/soap/binding/JMS"
                  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
                  xmlns:jndi="http://www.tibco.com/namespaces/ws/2004/soap/apis/jndi"
                  name="WholesaleOrderService"
                  targetNamespace="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2">
   <wsdl:documentation>Version 2.0</wsdl:documentation>
   <wsdl:types>
      <xsd:schema xmlns:complex="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2/complex"
                  xmlns:simple="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2/simple"
                  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                  xmlns:enum="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2/enm"
                  targetNamespace="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2/complex"
                  version="2.0"
                  elementFormDefault="unqualified"
                  attributeFormDefault="unqualified">
         <xsd:import namespace="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2/simple"/>
         <xsd:import namespace="http://www.mnet.de/esb/cdm/SupplierPartner/WholesaleOrderService/v2/enm"/>
         <xsd:complexType name="AnlageMitTypType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="complex:AnlageType">
                  <xsd:sequence>
                     <xsd:element name="anlagentyp" type="enum:AnlagentypType">
                        <xsd:annotation>
                           <xsd:documentation>Gibt die Art der Anlage an</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AnlageType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="dateiname">
                  <xsd:annotation>
                     <xsd:documentation>der Dateiname zum File einschließlich der Erweiterung (Datei-Typ)</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="255"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="dateityp" type="enum:DokumenttypType">
                  <xsd:annotation>
                     <xsd:documentation>der Datentyp des Dokuments (Text, PDF, ...) im Mime-Type-Format</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="beschreibung" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Freitext zur Beschreibung des Inhaltes der Anlage</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="160"/>
                        <xsd:minLength value="1"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="inhalt" type="xsd:base64Binary">
                  <xsd:annotation>
                     <xsd:documentation>der eigentliche Inhalt der Datei (binärer Dateninhalt) - max. 1 MB</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="AnsprechpartnerESTType">
            <xsd:annotation>
               <xsd:documentation>Erweitert eine Person um die Informationen: Telefonnummer, Mobilfunknr., Faxnummer, EMail und KontaktaufnahmeErwuenscht (im Gegensatz zum Ansprechpartner ist hier die Telefonnummer ein optionales Feld)</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="anrede" type="enum:AnredePersonType">
                  <xsd:annotation>
                     <xsd:documentation>Anrede der Person</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="vorname" type="simple:VornameType">
                  <xsd:annotation>
                     <xsd:documentation>Vorname der Person</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="nachname" type="simple:NachnameType">
                  <xsd:annotation>
                     <xsd:documentation>Nachname der Person</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="telefonnummer" type="simple:TelefonnummerType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Telefonnummer des Ansprechpartners</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="mobilfunknummer"
                            type="simple:TelefonnummerType"
                            minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Mobilfunknummer des Ansprechpartners</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="faxnummer" type="simple:TelefonnummerType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Faxnummer des Ansprechpartners</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="emailadresse" type="simple:EmailadresseType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>E-mail-Adresse des Ansprechpartners
          </xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="kontaktaufnahmeErwuenscht" type="xsd:boolean">
                  <xsd:annotation>
                     <xsd:documentation>Gibt an, ob eine Kontaktaufnahme durch den Leistungserbringer erfolgen darf.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="AnsprechpartnerMitRolleType">
            <xsd:annotation>
               <xsd:documentation>Erweitert einen Ansprechpartner um die Informatione zur Rolle (Funktion) der Person</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="complex:AnsprechpartnerType">
                  <xsd:sequence>
                     <xsd:element name="rolle" type="enum:AnsprechpartnerRolleType">
                        <xsd:annotation>
                           <xsd:documentation>Rolle des Ansprechpartners.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AnsprechpartnerType">
            <xsd:annotation>
               <xsd:documentation>Erweitert eine Person um die Informationen: Telefonnummer, Mobilfunknr., Faxnummer und EMail</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="complex:PersonType">
                  <xsd:sequence>
                     <xsd:element name="telefonnummer" type="simple:TelefonnummerType">
                        <xsd:annotation>
                           <xsd:documentation>Telefonnummer des Ansprechpartners</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="mobilfunknummer"
                                  type="simple:TelefonnummerType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Mobilfunknummer des Ansprechpartners</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="faxnummer" type="simple:TelefonnummerType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Faxnummer des Ansprechpartners</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="emailadresse" type="simple:EmailadresseType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Pflicht bei Rolle 'Auftragsmanagement'; E-mail-Adresse des Ansprechpartners
							</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="AuftraggeberType">
            <xsd:sequence>
               <xsd:element name="auftraggebernummer" type="simple:AuftraggebernummerType"/>
               <xsd:element name="leistungsnummer" type="simple:LeistungsnummerType"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="FTTxAnschlussEinfachType">
            <xsd:annotation>
               <xsd:documentation>Einfache Beschreibung eines FTTx-Anschlusses.</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="lineId" type="simple:LineIdType">
                  <xsd:annotation>
                     <xsd:documentation>Die eindeutige Line-Id, die den Kundenanschluss kennzeichnet (bestehend aus Country-, Carrier- und Line-Code).</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="FTTxAnschlussType">
            <xsd:annotation>
               <xsd:documentation>Beschreibt den Übergabepunkt und die kundenspezifischen Daten für einen beauftragten FTTx-Anschluss.</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="a10nsp">
                  <xsd:annotation>
                     <xsd:documentation>Der Übergabepunkt zwischen den Kooperationspartnern.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="20"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="port">
                  <xsd:annotation>
                     <xsd:documentation>Der Port zur näheren Spezifikation der A10NSP-SST.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="10"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="s-vlan" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Die S-VLAN-Id des Kundenanschlusses.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="4"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="c-vlan" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Die C-VLAN-Id des Kundenanschlusses.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="4"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="lineId" type="simple:LineIdType">
                  <xsd:annotation>
                     <xsd:documentation>Die eindeutige Line-Id, die den Kundenanschluss kennzeichnet (bestehend aus Country-, Carrier- und Line-Code).</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="uebergabepunktKunde" type="simple:UebergabepunktKundeType">
                  <xsd:annotation>
                     <xsd:documentation>Übergabepunkt beim Kunden.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="witaVertragsnummer" minOccurs="0">
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="10"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="FTTxVersandinfosONTType">
            <xsd:sequence>
               <xsd:element name="seriennummerONT" type="simple:SeriennummerONTType">
                  <xsd:annotation>
                     <xsd:documentation>SN des ONT welchen der LE an den AG oder Endkunden im Rahmen des Auftrages verschickt.</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="versand">
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="unternehmen">
                           <xsd:annotation>
                              <xsd:documentation>Unternehmen mit dem der ONT Unternehmen, mit welchem der ONT versandt wurde.</xsd:documentation>
                           </xsd:annotation>
                           <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                 <xsd:minLength value="1"/>
                                 <xsd:maxLength value="10"/>
                              </xsd:restriction>
                           </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="sendungsnummer">
                           <xsd:annotation>
                              <xsd:documentation>Sendungsverfolgungnummer des Versandunternehmens.</xsd:documentation>
                           </xsd:annotation>
                           <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                 <xsd:minLength value="1"/>
                                 <xsd:maxLength value="40"/>
                              </xsd:restriction>
                           </xsd:simpleType>
                        </xsd:element>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="FTTxVormieterType">
            <xsd:sequence>
               <xsd:element name="person" minOccurs="0">
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="vorname" type="simple:VornameType" minOccurs="0"/>
                        <xsd:element name="nachname" type="simple:NachnameType"/>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
               <xsd:element name="homeId" type="complex:HomeIdType" minOccurs="0"/>
               <xsd:element name="seriennummerONT"
                            type="simple:SeriennummerONTType"
                            minOccurs="0"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="FirmaMitTelefonType">
            <xsd:annotation>
               <xsd:documentation>Firma, die über optionale Kontaktmöglichkeiten (Telefon und Mobil) verfügt.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="complex:FirmaType">
                  <xsd:sequence>
                     <xsd:element name="telefonnummer" type="simple:TelefonnummerType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Telefonnummer der Firma.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="mobilfunknummer"
                                  type="simple:TelefonnummerType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Mobilfunknummer der Firma.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="FirmaType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="anrede" type="enum:AnredeFirmaType">
                  <xsd:annotation>
                     <xsd:documentation>Anrede zur Firma</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="firmenname" type="simple:VornameType">
                  <xsd:annotation>
                     <xsd:documentation>Name der Firma</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="firmennameZweiterTeil"
                            type="simple:NachnameType"
                            minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="GebaeudeteilType">
            <xsd:sequence>
               <xsd:element name="gebaeudeteilName" type="simple:GebaeudeteilNameType"/>
               <xsd:element name="gebaeudeteilZusatz"
                            type="simple:GebaeudeteilZusatzType"
                            minOccurs="0"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="GeschaeftsfallProduktType" abstract="true">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basis für alle geschäftsfall-spezifische Produkt-Daten</xsd:documentation>
            </xsd:annotation>
         </xsd:complexType>
         <xsd:complexType name="GeschaeftsfallType" abstract="true"/>
         <xsd:complexType name="HomeIdType">
            <xsd:sequence>
               <xsd:element name="homeIdNummer">
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1"/>
                        <xsd:maxLength value="15"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="KennerType">
            <xsd:sequence>
               <xsd:element name="projektId" minOccurs="0">
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="projektkenner">
                           <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                 <xsd:minLength value="1"/>
                                 <xsd:maxLength value="30"/>
                              </xsd:restriction>
                           </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="kopplungskenner" minOccurs="0">
                           <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                 <xsd:minLength value="1"/>
                                 <xsd:maxLength value="30"/>
                              </xsd:restriction>
                           </xsd:simpleType>
                        </xsd:element>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
               <xsd:element name="auftragskenner" minOccurs="0">
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="auftragsklammer">
                           <xsd:simpleType>
                              <xsd:restriction base="xsd:long">
                                 <xsd:minInclusive value="0"/>
                                 <xsd:maxInclusive value="9999999999"/>
                              </xsd:restriction>
                           </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="anzahlAuftraege" type="xsd:int"/>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="MeldungspositionOhneAttributeType">
            <xsd:annotation>
               <xsd:documentation>eine Einzelposition zu einer Meldung</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="meldungscode" type="simple:MeldungscodeType">
                  <xsd:annotation>
                     <xsd:documentation>Kurzbezeichnung zum Meldungstext</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="meldungstext" type="simple:MeldungstextType">
                  <xsd:annotation>
                     <xsd:documentation>ausführliche Beschreibung für den Meldungscode</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="MessageTEQType">
            <xsd:sequence>
               <xsd:element name="meldungsTyp" type="enum:MeldungstypType"/>
               <xsd:element name="externeAuftragsnummer"
                            type="simple:ExterneAuftragsnummerType"
                            minOccurs="0"/>
               <xsd:element name="auftraggebernummer"
                            type="simple:AuftraggebernummerType"
                            minOccurs="0"/>
               <xsd:element name="meldungspositionen">
                  <xsd:annotation>
                     <xsd:documentation>Meldungspositionen zum Meldungstyp - TEQ</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                     <xsd:sequence>
                        <xsd:element name="position"
                                     type="complex:MeldungspositionOhneAttributeType"
                                     maxOccurs="unbounded"/>
                     </xsd:sequence>
                  </xsd:complexType>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="MontageleistungType">
            <xsd:annotation>
               <xsd:documentation>ergänzende Daten zur Durchführung der Montageleistung</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="ansprechpartner" type="complex:AnsprechpartnerType">
                  <xsd:annotation>
                     <xsd:documentation>der Ansprechpartner, mit dem die Montageleistung abzustimmen ist</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="montagehinweis" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Nähere Angaben zur Durchführung der Montageleistung</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="100"/>
                        <xsd:minLength value="1"/>
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="OrtType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="ortsname" type="simple:OrtsnameType">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="ortsteil" type="simple:OrtsteilType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation/>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="PersonMitTelefonType">
            <xsd:annotation>
               <xsd:documentation>Person, die über optionale Kontaktmöglichkeiten (Telefon und Mobil) verfügt.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexContent>
               <xsd:extension base="complex:PersonType">
                  <xsd:sequence>
                     <xsd:element name="telefonnummer" type="simple:TelefonnummerType" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Telefonnummer der Person.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="mobilfunknummer"
                                  type="simple:TelefonnummerType"
                                  minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation>Mobilfunknummer der Person.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:sequence>
               </xsd:extension>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="PersonType">
            <xsd:annotation>
               <xsd:documentation/>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="anrede" type="enum:AnredePersonType">
                  <xsd:annotation>
                     <xsd:documentation>Anrede der Person</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="vorname" type="simple:VornameType" minOccurs="0">
                  <xsd:annotation>
                     <xsd:documentation>Vorname der Person</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="nachname" type="simple:NachnameType">
                  <xsd:annotation>
                     <xsd:documentation>Nachname der Person</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="ProduktFTTXType">
            <xsd:complexContent>
               <xsd:extension base="complex:ProduktType"/>
            </xsd:complexContent>
         </xsd:complexType>
         <xsd:complexType name="ProduktType" abstract="true">
            <xsd:annotation>
               <xsd:documentation>die abstrakte Basis für alle Produkt-Strukturen</xsd:documentation>
            </xsd:annotation>
            <xsd:sequence>
               <xsd:element name="bezeichner" type="simple:ProduktbezeichnerType">
                  <xsd:annotation>
                     <xsd:documentation>Produktname des Produktes, das in der Auftragsposition zugebucht, in Wegfall gebracht oder geändert werden soll</xsd:documentation>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="ProviderwechselAnkuendigungType">
            <xsd:sequence>
               <xsd:element name="providername" type="simple:String60ZeichenType"/>
               <xsd:element name="uebernahmedatumGeplant" type="xsd:date"/>
               <xsd:element name="antwortfrist" type="xsd:date"/>
            </xsd:sequence>
         </xsd:complexType>
         <xsd:complexType name="ProviderwechselBestaetigungType">
            <xsd:sequence>
               <xsd:element name="providername" type="simple:String60ZeichenType"/>
               <xsd:element name="uebernahmedatumVerbindlich" type="xsd:date"/>
            </xsd:sequence>
