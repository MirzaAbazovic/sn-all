<?xml version="1.0" encoding="UTF-8"?>
<createOrder xmlns="http://www.mnet.de/esb/cdm/SupplierPartner/LineOrderService/v1">
    <order>
        <externeAuftragsnummer>@ignore@</externeAuftragsnummer>
        <kunde>
            <kundennummer>@ignore@</kundennummer>
            <leistungsnummer>@ignore@</leistungsnummer>
        </kunde>
        <besteller>
            <kundennummer>@ignore@</kundennummer>
            <leistungsnummer>@ignore@</leistungsnummer>
        </besteller>
        <geschaeftsfall>
            <LAE>
                <BKTOFaktura>5883000391</BKTOFaktura>
                <vertragsnummer>@ignore@</vertragsnummer>
                <ansprechpartner>
                    <auftragsmanagement>
                        <anrede>9</anrede>
                        <vorname>@ignore@</vorname>
                        <nachname>@ignore@</nachname>
                        <telefonnummer>@ignore@</telefonnummer>
                        <emailadresse>@ignore@</emailadresse>
                    </auftragsmanagement>
                </ansprechpartner>
                <termine>
                    <kundenwunschtermin>
                        <datum>@ignore@</datum>
                        <zeitfenster>9</zeitfenster>
                    </kundenwunschtermin>
                </termine>
                <auftragsposition>
                    <aktionscode>A</aktionscode>
                    <produkt>
                        <kategorie>TAL</kategorie>
                        <bezeichner>TAL; CuDA 2 Draht hbr (HVt)</bezeichner>
                    </produkt>
                    <geschaeftsfallProdukt>
                        <TAL>
                            <standortA>
                                <person>
                                    <anrede>@ignore@</anrede>
                                    <vorname>${endkunde.vorname}</vorname>
                                    <nachname>${endkunde.nachname}</nachname>
                                </person>
                                <strasse>
                                    <strassenname>${standort.strasse}</strassenname>
                                    <hausnummer>${standort.hnr}</hausnummer>
                                    <hausnummernZusatz>${standort.hnr.zusatz}</hausnummernZusatz>
                                </strasse>
                                <gebaeudeteil>
                                    <gebaeudeteilName>@ignore@</gebaeudeteilName>
                                    <gebaeudeteilZusatz>@ignore@</gebaeudeteilZusatz>
                                </gebaeudeteil>
                                <land>de</land>
                                <postleitzahl>${standort.plz}</postleitzahl>
                                <ort>
                                    <ortsname>${standort.ort}</ortsname>
                                    <ortsteil>${standort.ortsteil}</ortsteil>
                                </ort>
                                <lageTAE_ONT>@ignore@</lageTAE_ONT>
                            </standortA>
                            <standortB>
                                <strasse>
                                    <strassenname>@ignore@</strassenname>
                                    <hausnummer>@ignore@</hausnummer>
                                </strasse>
                                <postleitzahl>${standort.plz}</postleitzahl>
                                <ort>
                                    <ortsname>${standort.ort}</ortsname>
                                </ort>
                                <lageKollokationsraum>
                                    <ONKZ>@ignore@</ONKZ>
                                    <anschlussbereich>@ignore@</anschlussbereich>
                                </lageKollokationsraum>
                            </standortB>
                            <montageleistung>
                                <ansprechpartner>
                                    <anrede>@ignore@</anrede>
                                    <vorname>@ignore@</vorname>
                                    <nachname>@ignore@</nachname>
                                    <telefonnummer>@ignore@</telefonnummer>
                                    <emailadresse>@ignore@</emailadresse>
                                </ansprechpartner>
                            </montageleistung>
                            <bestandsvalidierung-2>
                                <leitungsschluesselzahl>${lbz.leitungsschluesselzahl}</leitungsschluesselzahl>
                                <onkzA>${lbz.onkz.a}</onkzA>
                                <onkzB>${lbz.onkz.b}</onkzB>
                                <ordnungsnummer>${lbz.ordnungsnummer}</ordnungsnummer>
                            </bestandsvalidierung-2>
                            <uebertragungsverfahren>H13</uebertragungsverfahren>
                            <schaltangaben>
                                <schaltung>
                                    <kupfer>
                                        <UEVT>1PH1</UEVT>
                                        <EVS>01</EVS>
                                        <doppelader>01</doppelader>
                                    </kupfer>
                                </schaltung>
                            </schaltangaben>
                        </TAL>
                    </geschaeftsfallProdukt>
                </auftragsposition>
            </LAE>
        </geschaeftsfall>
    </order>
</createOrder>
