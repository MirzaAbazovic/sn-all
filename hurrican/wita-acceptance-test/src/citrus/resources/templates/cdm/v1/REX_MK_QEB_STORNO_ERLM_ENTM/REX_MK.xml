<?xml version="1.0" encoding="UTF-8"?>
<createOrder xmlns="http://www.mnet.de/esb/cdm/SupplierPartner/LineOrderService/v1">
    <order>
        <externeAuftragsnummer>@ignore@</externeAuftragsnummer>
        <kunde>
            <kundennummer>@ignore@</kundennummer>
            <leistungsnummer>@ignore@</leistungsnummer>
        </kunde>
        <besteller>
            <kundennummer>@ignore@</kundennummer>
            <leistungsnummer>@ignore@</leistungsnummer>
        </besteller>
        <geschaeftsfall>
            <REX-MK>
                <BKTOFaktura>5883000391</BKTOFaktura>
                <ansprechpartner>
                    <auftragsmanagement>
                        <anrede>9</anrede>
                        <vorname>TestFirstName</vorname>
                        <nachname>REX_MK_QEB_STORNO_ERLM_ENTM</nachname>
                        <telefonnummer>089 0000000</telefonnummer>
                        <emailadresse>hurrican_developer@m-net.de</emailadresse>
                    </auftragsmanagement>
                </ansprechpartner>
                <termine>
                    <kundenwunschtermin>
                        <datum>@ignore@</datum>
                        <zeitfenster>1</zeitfenster>
                    </kundenwunschtermin>
                </termine>
                <auftragsposition>
                    <produkt>
                        <kategorie>TAL</kategorie>
                        <bezeichner>TAL; Rufnummernmitnahme Einzelrufnummer</bezeichner>
                    </produkt>
                    <geschaeftsfallProdukt>
                        <TAL>
                            <standortA>
                                <person>
                                    <anrede>1</anrede>
                                    <vorname>${endkunde.vorname}</vorname>
                                    <nachname>${endkunde.nachname}</nachname>
                                </person>
                                <strasse>
                                    <strassenname>${standort.strasse}</strassenname>
                                    <hausnummer>${standort.hnr}</hausnummer>
                                    <hausnummernZusatz>${standort.hnr.zusatz}</hausnummernZusatz>
                                </strasse>
                                <land>de</land>
                                <postleitzahl>${standort.plz}</postleitzahl>
                                <ort>
                                    <ortsname>${standort.ort}</ortsname>
                                    <ortsteil>${standort.ortsteil}</ortsteil>
                                </ort>
                            </standortA>
                            <bestandssuche>
                                <einzelanschluss>
                                    <ONKZ>${rufnummer.onkz.0}</ONKZ>
                                    <rufnummer>${rufnummer.dnbase.0}</rufnummer>
                                </einzelanschluss>
                            </bestandssuche>
                            <rufnummernPortierung>
                                <einzelanschluss>
                                    <rufnummernliste>
                                    <zuPortierendeOnkzRnr>
                                    <ONKZ>${rufnummer.onkz.0}</ONKZ>
                                    <rufnummer>${rufnummer.dnbase.0}</rufnummer>
                                    </zuPortierendeOnkzRnr>
                                    </rufnummernliste>
                                </einzelanschluss>
                                <Portierungskenner>@contains('D043-')@</Portierungskenner>
                            </rufnummernPortierung>
                        </TAL>
                    </geschaeftsfallProdukt>
                </auftragsposition>
            </REX-MK>
        </geschaeftsfall>
    </order>
</createOrder>
